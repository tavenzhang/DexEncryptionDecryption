scriptPath=$(cd `dirname $0`; pwd)
echo 开始执行脚本 请等待...
cd ${scriptPath}
npm update
npm install
rm ./node_modules/react-native/local-cli/core/__fixtures__/files/package.json



#code push 替换
cp ./Init/CodePush.js ${scriptPath}/node_modules/react-native-code-push/CodePush.js
cp -rf ./Init/CodePush.java ${scriptPath}/node_modules/react-native-code-push/android/app/src/main/java/com/microsoft/codepush/react/CodePush.java

#webivew 替换覆盖
cp -rf ./Init/RNCWKWebView.m ${scriptPath}/node_modules/react-native-webview/ios/RNCWKWebView.m
cp -rf ./Init/RNCWebViewManager.java ${scriptPath}/node_modules/react-native-webview/android/src/main/java/com/reactnativecommunity/webview/RNCWebViewManager.java
#极光
cp -rf ./Init/JpushAndroidManifest.xml ${scriptPath}/node_modules/jpush-react-native/android/src/main/AndroidManifest.xml

#codePush ios 指定版本号
cp -rf ./Init/CodePushConfig.m  ${scriptPath}/node_modules/react-native-code-push/ios/CodePush/CodePushConfig.m

cp -rf ./Init/RN_Fetch_fs.js  ${scriptPath}/node_modules/react-native-fetch-blob/fs.js

#cp -rf ./Init/MKTouchableManager.m  ${scriptPath}/node_modules/react-native-material-kit/iOS/RCTMaterialKit/MKTouchableManager.m

#react-native-orientation 替换覆盖
cp -rf ./Init/rn_orientation/index.js  ${scriptPath}/node_modules/react-native-orientation/index.js
cp -rf ./Init/rn_orientation/Orientation.h  ${scriptPath}/node_modules/react-native-orientation/iOS/RCTOrientation/Orientation.h
cp -rf ./Init/rn_orientation/Orientation.m  ${scriptPath}/node_modules/react-native-orientation/iOS/RCTOrientation/Orientation.m
cp -rf ./Init/rn_orientation/OrientationModule.java  ${scriptPath}/node_modules/react-native-orientation/android/src/main/java/com/github/yamill/orientation/OrientationModule.java
#替换pod配置
cp -rf ./Init/RNSound.podspec  ${scriptPath}/node_modules/react-native-sound/RNSound.podspec
cp -rf ./Init/ReactNativeKeyboardManager.podspec ${scriptPath}/node_modules/react-native-keyboard-manager/ReactNativeKeyboardManager.podspec
cp -rf ./Init/react-native-fetch-blob.podspec  ${scriptPath}/node_modules/react-native-fetch-blob/react-native-fetch-blob.podspec
cp -rf ./Init/RNZipArchive.podspec ${scriptPath}/node_modules/react-native-zip-archive/RNZipArchive.podspec

npx jetify

react-native link react-native-extra-dimensions-android
echo '初始化完毕 可以开始 Xcode -> Run'

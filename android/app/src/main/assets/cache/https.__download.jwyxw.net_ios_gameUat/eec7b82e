b.text = String(chongzhi);\r\n\t\tthis.youhuilab.text = String(youhui);\r\n\t\tthis.tixianlab.text = String(tixian);\r\n\t\tthis.fanshuilab.text = String(fanshui);\r\n\t}\r\n\r\n\tpublic destroy(): void {\r\n\t\tEventManager.removeAllEvents(this);\r\n\t\tsuper.destroy(true);\r\n\t}\r\n}\r\n","import { ResConfig } from '../../data/ResConfig';\r\nimport { EventManager } from '../../events/EventManager';\r\n/**Created by the LayaAirIDE*/\r\nimport { ui } from \"../../ui/layaMaxUI\";\r\nimport { GameUtils } from '../../utils/GameUtils';\r\nimport { SoundPlayer } from '../../utils/SoundPlayer';\r\nimport { MenuTabComp } from \"../comp/MenuTabComp\";\r\nimport { PersonalModel } from './../../data/PersonalModel';\r\nimport { PersonalInfoView } from './personaCenter/PersonalInfoView';\r\nimport { VipDetailsView } from './personaCenter/VipDetailsView';\r\nimport { AccountDetailsView } from './personaCenter/AccountDetailsView';\r\nimport { SelfReportView } from './personaCenter/SelfReportView';\r\nimport { BetRecording } from './personaCenter/BetRecording';\r\n\r\n/**\r\n * 全屏个人中心\r\n */\r\nexport class PersonaCenterDlg extends ui.sceneUI.dlg.PersonaCenterDlgUI {\r\n\tprivate curTabId: number = 0;//当前tab-id\r\n\tprivate tabArr: MenuTabComp[] = [];\r\n\tprivate prevTab: MenuTabComp;//上一次被选中的tab\r\n\t//\r\n\tprivate curView: Laya.View;\r\n\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t\tthis.initView();\r\n\t}\r\n\t/**\r\n\t * 入口\r\n\t */\r\n\tpublic static show() {\r\n\t\tlet dlg = new PersonaCenterDlg();\r\n\t\tdlg.popup(false, true);\r\n\t}\r\n\tprivate initView() {\r\n\t\tthis.width = Laya.stage.width;\r\n\t\t//每次打开弹窗刷新vip数据\r\n\t\tPersonalModel.getVipData();\r\n\r\n\t\tthis.titleGroup.left = GameUtils.getScreencOffset(-48, 0);\r\n\t\tthis.contentList.left = GameUtils.getScreencOffset(-48, 0);\r\n\t\tthis.backBtn.right = GameUtils.getScreencOffset(-55, 0);\r\n\t\tthis.verTxt.x = GameUtils.getScreencOffset(28, 78);\r\n\r\n\t\tlet btn: Laya.Image;\r\n\t\tlet posy:number = 42;\r\n
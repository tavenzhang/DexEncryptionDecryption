mon(): any {\r\n        if (!this.commonObj) {\r\n            this.commonObj = Laya.Loader.getRes(\"./assets/conf/config.json\");\r\n        }\r\n        return this.commonObj;\r\n    }\r\n\r\n    public static urlObj: any = null;\r\n    public static getConfUrl(): any {\r\n        if (!ConfObjRead.urlObj) {\r\n            ConfObjRead.urlObj = Laya.Loader.getRes(\"./assets/conf/urls.json\");\r\n        }\r\n        return ConfObjRead.urlObj;\r\n    }\r\n    //获取http-cmd\r\n    public static get httpCmd(): any {\r\n        return this.getConfUrl().cmd;\r\n    }\r\n    //获取当前域名\r\n    public static get apihome(): string {\r\n        return this.getConfUrl().url.apihome;\r\n    }\r\n\r\n    /**\r\n     * 游戏图标配置\r\n     */\r\n    private static gameIconConfig: any = null;\r\n    public static graySuffix: string = \"\";//灰度图片后缀\r\n    public static getGameIconConfig(): any {\r\n        if (!this.gameIconConfig) {\r\n            this.gameIconConfig = Laya.Loader.getRes(\"./assets/conf/gameIcons.json\");\r\n            this.graySuffix = this.gameIconConfig.graySuffix || \"_gray\";\r\n        }\r\n        return this.gameIconConfig;\r\n    }\r\n    private static verConfig: any = null;\r\n    public static getVerConfig(): any {\r\n        if (!this.verConfig) {\r\n            this.verConfig = Laya.Loader.getRes(\"./assets/conf/version.json\");\r\n        }\r\n        return this.verConfig;\r\n    }\r\n    //获取图标预加载列表\r\n    public static getIconList(): string[] {\r\n        let list: any[] = this.getGameIconConfig().list;\r\n        let arr: string[] = [];\r\n        list.forEach(value => {\r\n            arr.push(value.anim_sk);\r\n            let skArr = value.anim_sk.split(\".sk\");\r\n            arr.push(skArr[0] + \".png\");\r\n        });\r\n        return arr;\r\n    }\r\n}","\r\nexport class MyBase64 {\r\n    // private property  \r\n    public static _keyStr = \"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=\";\r\n\r\n    // public method for encoding  \r\n    public encode(input: string): string {\r\n        var output = \"\";\r\n        var chr1, chr2, chr3, enc1, enc2, enc3, enc4;\r\n        var i = 0;\r\n        input = this._utf8_encode(input);\r\n        while (i < input.length) {\r\n            chr1 = input.charCodeAt(i++);\r\n            chr2 = input.charCodeAt(i++);\r\n            chr3 = input.charCodeAt(i++);\r\n            enc1 = chr1 >> 2;\r\n            enc2 = ((chr1 & 3) << 4) | (chr2 >> 4);\r\n            enc3 = ((chr2 & 15) << 2) | (chr3 >> 6);\r\n            enc4 = chr3 & 63;\r\n            if (isNaN(chr2)) {\r\n                enc3 = enc4 = 64;\r\n            } else if (isNaN(chr3)) {\r\n                enc4 = 64;\r\n            }\r\n            output = output +\r\n                MyBase64._keyStr.charAt(enc1) + MyBase64._keyStr.charAt(enc2) +\r\n                MyBase64._keyStr.charAt(enc3) + MyBase64._keyStr.charAt(enc4);\r\n        }\r\n        return output;\r\n    }\r\n\r\n    // public method for decoding  \r\n    public decode(input: string): string {\r\n        var output = \"\";\r\n        var chr1, chr2, chr3;\r\n        var enc1, enc2, enc3, enc4;\r\n        var i = 0;\r\n        input = input.replace(/[^A-Za-z0-9\\+\\/\\=]/g, \"\");\r\n        while (i < input.length) {\r\n            enc1 = MyBase64._keyStr.indexOf(input.charAt(i++));\r\n            enc2 = MyBase64._keyStr.indexOf(input.charAt(i++));\r\n            enc3 = MyBase64._keyStr.indexOf(input.charAt(i++));\r\n            enc4 = MyBase64._keyStr.indexOf(input.charAt(i++));\r\n            chr1 = (enc1 << 2) | (enc2 >> 4);\r\n            chr2 = ((enc2 & 15) << 4) | (enc3 >> 2);\r\n            chr3 = ((enc3 & 3) << 6) | enc4;\r\n            output = output + String.fromCharCode(chr1);\r\n            if (enc3 != 64) {\r\n                output = output + String.fromCharCode(chr2);\r\n            }\r\n            if (enc4 != 64) {\r\n                output = output + String.fromCharCode(chr3);\r\n            }\r\n        }\r\n        output = this._utf8_decode(output);\r\n        return output;\r\n    }\r\n\r\n    // private method for UTF-8 encoding  \r\n    public _utf8_encode(string: string): string {\r\n        string = string.replace(/\\r\\n/g, \"\\n\");\r\n        var utftext = \"\";\r\n        for (var n = 0; n < string.length; n++) {\r\n            var c = string.charCodeAt(n);\r\n            if (c < 128) {\r\n                utftext += String.fromCharCode(c);\r\n            } else if ((c > 127) && (c < 2048)) {\r\n                utftext += String.fromCharCode((c >> 6) | 192);\r\n                utftext += String.fromCharCode((c & 63) | 128);\r\n            } else {\r\n                utftext += String.fromCharCode((c >> 12) | 224);\r\n                utftext += String.fromCharCode(((c >> 6) & 63) | 128);\r\n                utftext += String.fromCharCode((c & 63) | 128);\r\n            }\r\n\r\n        }\r\n        return utftext;\r\n    }\r\n\r\n    // private method for UTF-8 decoding  \r\n    public _utf8_decode(utftext: string): string {\r\n        var string = \"\";\r\n        var i = 0;\r\n        var c, c1, c2, c3 = 0;\r\n        while (i < utftext.length) {\r\n            c = utftext.charCodeAt(i);\r\n            if (c < 128) {\r\n                string += String.fromCharCode(c);\r\n                i++;\r\n            } else if ((c > 191) && (c < 224)) {\r\n                c2 = utftext.charCodeAt(i + 1);\r\n                string += String.fromCharCode(((c & 31) << 6) | (c2 & 63));\r\n                i += 2;\r\n            } else {\r\n                c2 = utftext.charCodeAt(i + 1);\r\n                c3 = utftext.charCodeAt(i + 2);\r\n                string += String.fromCharCode(((c & 15) << 12) | ((c2 & 63) << 6) | (c3 & 63));\r\n                i += 3;\r\n            }\r\n        }\r\n        return string;\r\n    }\r\n} \r\n","import { Debug } from \"./Debug\";\r\n\r\nexport class SaveManager {\r\n\r\n    /***************************************** 公用本地缓存(游戏与大厅通用)键值命名 PS:存在被游戏使用者不小心清空的不安全性) *********************************************/\r\n    /** 公用Json键值命名 */\r\n    public static SAVE_KEY_NN: string = \"MT-Card\";\r\n    /** 音乐音量值 0...1 */\r\n    public static KEY_MUSIC_VL: string = \"music\";\r\n    /** 音乐开关 0、1 */\r\n    public static KEY_MUSIC_SWITCH: string = \"music_switch\";\r\n    /** 音效音量值 0...1 */\r\n    public static KEY_SFX_VL: string = \"sound\";\r\n    /** 音效开关 0、1 */\r\n    public static KEY_SFX_SWITCH: string = \"sound_switch\";\r\n    /** 头像ID \"01\"...\"08\"头像 */\r\n    public static KEY_AVATOR_ID: string = \"avator_id\";\r\n    /** 玩家token */\r\n    public static KEY_TOKEN: string = \"token\";\r\n    /** \"xxxxxx\" */\r\n    public static KEY_API_URL: string = \"httpUrl\";\r\n\r\n    /** 公用键值索引 */\r\n    private commonKeyMap = {\r\n        [SaveManager.KEY_MUSIC_VL]: true,\r\n        [SaveManager.KEY_MUSIC_SWITCH]: true,\r\n        [SaveManager.KEY_SFX_VL]: true,\r\n        [SaveManager.KEY_SFX_SWITCH]: true,\r\n        [SaveManager.KEY_AVATOR_ID]: true,\r\n        [SaveManager.KEY_TOKEN]: true,\r\n        [SaveManager.KEY_API_URL]: true,\r\n    }\r\n\r\n    /***************************************** 大厅本地缓存键值命名（单独只用于大厅） *********************************************/\r\n    /** 大厅Json键值命名 */\r\n    private static SAVE_KEY_LOBBY: string = \"MT-Card-Lobby\";\r\n    /**快捷登录用户名 */\r\n    public static KEY_QK_USERNAME: string = \"qk_username\";\r\n    /**快捷登录密码 */\r\n    public static KEY_QK_PASSWORD: string = \"qk_password\";\r\n    public static KEY_QK_PWD_CHANGED: string = \"qk_pwd_changed\";\r\n    /**登录类型 */\r\n    public static KEY_LOGIN_TYPE: string = \"loginType\";\r\n    public static KEY_LOGIN_INFO: string = \"loginInfo\";\r\n    /** */\r\n    public static KEY_UID: string = \"uuid\";\r\n\r\n    public static KEY_GATEWAYINFO: string = \"gatewayInfo\";\r\n    public static KEY_PHONEPWD: string = \"phone_pwd\";//手机登录密码\r\n    public static KEY_WEICHATPWD: string = \"weichat_pwd\";//微信登录密码\r\n\r\n    public static KEY_ACCOUNT: string = \"keyAccount\";//本地缓存账号\r\n    public static KEY_PHONE: string = \"keyPhone\";//本地缓存手机登录账号\r\n\r\n    public static KEY_FLUSHTOKEN: string = \"keyFlushToken\";//用于刷新token的标记\r\n    public static KEY_LASTLOGININFO: string = \"keyLastLoginInfo\";//缓存最后一次登录的信息(包括登录方式，用户名，密码)\r\n    public static KEY_VISTORINFO: string = \"keyVisitorInfo\";//用于缓存游客账号(如果游客账号变成了正式账号时要清空此账号)\r\n\r\n    /***************************************** 管理器属性与实例 *********************************************/\r\n    /** 获取管理器实例 */\r\n    public static getObj(): SaveManager {\r\n        if (!SaveManager.obj) {\r\n            SaveManager.obj = new SaveManager();\r\n        }\r\n        return SaveManager.obj;\r\n    }\r\n    public static obj: SaveManager;\r\n\r\n    /** 大厅本地缓存对象 */\r\n    public lobbyJosnObj: any = null;\r\n    /** 通用本地缓存对象 */\r\n    public commonJosnObj: any = null;\r\n\r\n    /** 构造函数初始化管理器数据 */\r\n    constructor() {\r\n        //兼容\r\n        this.compatibleSaveObj();\r\n        this.refreshSaveObj();\r\n    }\r\n\r\n    /** 清空所有本地缓存 慎用 */\r\n    public clearAll(): void {\r\n        Laya.LocalStorage.clear();\r\n        this.refreshSaveObj();\r\n        Debug.log(\"SaveManager.clearAll-----------\");\r\n    }\r\n\r\n    /** 兼容存储地址 */\r\n    private compatibleSaveObj() {\r\n        var lobbyJosnObj = Laya.LocalStorage.getJSON(SaveManager.SAVE_KEY_LOBBY) || {};\r\n        if (lobbyJosnObj.isCompatible) return;\r\n        var commonJosnObj = Laya.LocalStorage.getJSON(SaveManager.SAVE_KEY_NN) || {};\r\n        //兼容大厅本地缓存记录转移\r\n        for (const key in commonJosnObj) {\r\n            var cmValue = commonJosnObj[key];\r\n            //如果大厅本地缓存没有改值,而通用本地缓存里有将其\r\n            if (!this.commonKeyMap[key] && cmValue != null && cmValue != undefined) {\r\n                lobbyJosnObj[key] = cmValue;\r\n                delete commonJosnObj[key];\r\n            }\r\n        }\r\n        //设置已兼容标志\r\n        lobbyJosnObj.isCompatible = true;\r\n        //保存兼容后的大厅数据与通用数据\r\n        Laya.LocalStorage.setJSON(SaveManager.SAVE_KEY_LOBBY, lobbyJosnObj);\r\n        Laya.LocalStorage.setJSON(SaveManager.SAVE_KEY_NN, commonJosnObj);\r\n    }\r\n\r\n    /** 重新读取本地本地缓存中的文件 */\r\n    public refreshSaveObj(): void {\r\n        this.commonJosnObj = this.getObjTotal(SaveManager.SAVE_KEY_NN, {});\r\n        this.lobbyJosnObj = this.getObjTotal(SaveManager.SAVE_KEY_LOBBY, {});\r\n    }\r\n\r\n    /**\r\n     * 保存对象Json到本地缓存\r\n     * @param key 键值\r\n     * @param obj 对象\r\n     */\r\n    public saveObjTotal(key: string, obj: any): void {\r\n        Laya.LocalStorage.setJSON(key, obj);\r\n    }\r\n\r\n    /**\r\n     * 从本地缓存获取对象Json\r\n     * @param key 键值\r\n     * @param def 对象默认值\r\n     */\r\n    public getObjTotal(key: string, def: any): any {\r\n        var a = Laya.LocalStorage.getJSON(key);\r\n        if (a) {\r\n            return a;\r\n        }\r\n        return def;\r\n    }\r\n\r\n    /**\r\n     * 只设置数据,但是暂时不保存到本地缓存,需要调用saveData方法保存\r\n     * 目的：批次设置的时候只保存一次\r\n     * @param key 键值\r\n     * @param v 数据\r\n     */\r\n    public set(key: string, v: any): any {\r\n        if (this.commonKeyMap[key]) {\r\n            this.commonJosnObj[key] = v;\r\n        } else {\r\n            this.lobbyJosnObj[key] = v;\r\n        }\r\n    }\r\n\r\n    /**\r\n     * 保存数据到本地缓存\r\n     * @param key 键值\r\n     * @param v 数据\r\n     */\r\n    public save(key: string, v: any): any {\r\n        if (this.commonKeyMap[key]) {\r\n            this.commonJosnObj[key] = v;\r\n            this.saveObjTotal(SaveManager.SAVE_KEY_NN, this.commonJosnObj);\r\n        } else {\r\n            this.lobbyJosnObj[key] = v;\r\n            this.saveObjTotal(SaveManager.SAVE_KEY_LOBBY, this.lobbyJosnObj);\r\n        }\r\n    }\r\n\r\n    /**\r\n     * 获取本地缓存的数据\r\n     * @param key 数据key值\r\n     * @param def 默认值\r\n     */\r\n    public get(key: string, def: any): any {\r\n        if (this.commonKeyMap[key]) {\r\n            if (this.commonJosnObj[key] != null && this.commonJosnObj[key] != undefined) {\r\n                return this.commonJosnObj[key];\r\n            }\r\n        } else {\r\n            if (this.lobbyJosnObj[key] != null && this.lobbyJosnObj[key] != undefined) {\r\n                return this.lobbyJosnObj[key];\r\n            }\r\n        }\r\n        this.save(key, def);\r\n        return def;\r\n    }\r\n\r\n    /**\r\n     * 设置公用数据\r\n     * @param token \r\n     * @param rooturl \r\n     */\r\n    public initCommon(token: any, rooturl: any): void {\r\n        var token = this.get(SaveManager.KEY_TOKEN, token);\r\n        var httpUrl = this.get(SaveManager.KEY_API_URL, rooturl);\r\n    }\r\n    /**\r\n     * 调用set之后统一保存\r\n     */\r\n    public saveData() {\r\n        this.saveObjTotal(SaveManager.SAVE_KEY_NN, this.commonJosnObj);\r\n        this.saveObjTotal(SaveManager.SAVE_KEY_LOBBY, this.lobbyJosnObj);\r\n    }\r\n}","import { Debug } from \"./Debug\";\r\nimport { MyBase64 } from \"./MyBase64\";\r\nimport { SaveManager } from \"./SaveManager\";\r\n\r\nexport class MyUid {\r\n    public static uid: string = \"\";\r\n\r\n    public static KEY_P_MOBILE: string = \"MOBILE\";\r\n    public static KEY_P_PC: string = \"PC\";\r\n    public static KEY_P_MAC: string = \"MAC\";\r\n\r\n    public static KEY_P_IOS: string = \"IOS\";\r\n    public static KEY_P_ANDROID: string = \"ANDROID\";\r\n    public static KEY_P_IPAD: string = \"IPAD\";\r\n    public static KEY_P_IPHONE: string = \"IPHONE\";\r\n    public static KEY_P_WP: string = \"WP\";\r\n\r\n    public static KEY_P_QQBROWSER: string = \"QQBROWSER\";\r\n    public static KEY_P_MQQBROWSER: string = \"MQQBROWSER\";\r\n    public static KEY_P_SAFARI: string = \"SAFARI\";\r\n    public static KEY_P_IE: string = \"IE\";\r\n    public static KEY_P_FIREFOX: string = \"FIREFOX\";\r\n    public static KEY_P_EDGE: string = \"EDGE\";\r\n\r\n    public static KEY_P_QQ: string = \"QQ\";\r\n    public static KEY_P_WX: string = \"WX\";\r\n    public static KEY_P_LIMIXIU: string = \"LIMIXIU\";\r\n    public static KEY_P_MINIGAME: string = \"MINIGAME\";\r\n\r\n    public static KEY_P_WAP: string = \"WAP\";\r\n\r\n    public static KEY_P_UNKNOW: string = \"UNKNOW\";\r\n\r\n    public static platforms: Array<string> = null;\r\n\r\n    public static getPlatforms(): Array<string> {\r\n        var rs = [];\r\n\r\n        if (Laya.Browser.onWP) {\r\n            rs.push(MyUid.KEY_P_WP);\r\n        }\r\n        if (Laya.Browser.onWeiXin) {\r\n            rs.push(MyUid.KEY_P_WX);\r\n        }\r\n        if (Laya.Browser.onSafari) {\r\n            rs.push(MyUid.KEY_P_SAFARI);\r\n        }\r\n        if (Laya.Browser.onQQBrowser) {\r\n            rs.push(MyUid.KEY_P_QQBROWSER);\r\n        }\r\n\r\n        if (Laya.Browser.onMQQBrowser) {\r\n            rs.push(MyUid.KEY_P_MQQBROWSER);\r\n        }\r\n        if (Laya.Browser.onMobile) {\r\n            rs.push(MyUid.KEY_P_MOBILE);\r\n        }\r\n        if (Laya.Browser.onMiniGame) {\r\n            rs.push(MyUid.KEY_P_MINIGAME);\r\n        }\r\n        if (Laya.Browser.onMac) {\r\n            rs.push(MyUid.KEY_P_MAC);\r\n        }\r\n\r\n        if (Laya.Browser.onIPhone) {\r\n            rs.push(MyUid.KEY_P_IPHONE);\r\n        }\r\n        if (Laya.Browser.onIPad) {\r\n            rs.push(MyUid.KEY_P_IPAD);\r\n        }\r\n\r\n\r\n\r\n        if (Laya.Browser.onMobile) {\r\n            rs.push(MyUid.KEY_P_MOBILE);\r\n        }\r\n\r\n        if (Laya.Browser.onEdge) {\r\n            rs.push(MyUid.KEY_P_EDGE);\r\n        }\r\n\r\n        if (Laya.Browser.onIE) {\r\n            rs.push(MyUid.KEY_P_IE);\r\n        }\r\n\r\n        if (Laya.Browser.onFirefox) {\r\n            rs.push(MyUid.KEY_P_FIREFOX);\r\n        }\r\n\r\n        if (Laya.Browser.onPC) {\r\n            rs.push(MyUid.KEY_P_PC);\r\n        }\r\n        if (Laya.Browser.onIOS) {\r\n            rs.push(MyUid.KEY_P_IOS);\r\n        }\r\n        if (Laya.Browser.onAndroid) {\r\n            rs.push(MyUid.KEY_P_ANDROID);\r\n        }\r\n\r\n        if (rs.length <= 0) {\r\n            rs.push(MyUid.KEY_P_UNKNOW);\r\n        }\r\n\r\n        return rs;\r\n    }\r\n\r\n    public static getPlatform(): string {\r\n        if (Laya.Browser.onPC) {\r\n            return MyUid.KEY_P_PC;\r\n        } else if (Laya.Browser.onMac) {\r\n            return MyUid.KEY_P_MAC;\r\n        } else if (Laya.Browser.onMobile) {\r\n            return MyUid.KEY_P_MOBILE;\r\n        }\r\n\r\n        // return MyUid.KEY_P_UNKNOW;\r\n        return null;\r\n    }\r\n\r\n    public static setUid(s: string): void {\r\n        MyUid.uid = s;\r\n        SaveManager.getObj().save(SaveManager.KEY_UID, MyUid.uid);\r\n    }\r\n    /**\r\n     * 获取32位随机码\r\n     */\r\n    public static getUid(): string {\r\n        MyUid.uid = SaveManager.getObj().get(SaveManager.KEY_UID, \"\");\r\n        if (MyUid.uid.length <= 0) {\r\n            MyUid.createUid();\r\n            SaveManager.getObj().save(SaveManager.KEY_UID, MyUid.uid);\r\n        }\r\n\r\n        // Debug.log(\"getUid:\"+MyUid.uid);\r\n        return MyUid.uid;\r\n    }\r\n\r\n    public static createUid(): void {\r\n        var b64 = new MyBase64();\r\n\r\n        var t = Laya.Browser.now();\r\n        var ua = Laya.Browser.userAgent;\r\n        if (isNative) ua += AppData.deviceToken;\r\n        var str = t + ua;\r\n        var str2 = str.substr(0, 32);\r\n        MyUid.uid = b64.encode(str2);\r\n        Debug.log('MyUid.createUid uid:' + MyUid.uid);\r\n    }\r\n    //创建新的uid\r\n    public static newUid(): string {\r\n        var b64 = new MyBase64();\r\n        let time = Laya.Browser.now();\r\n        let ua = Laya.Browser.userAgent;//native:LayaBox Android\r\n        if (isNative) ua += AppData.deviceToken;\r\n        let str = time + ua;\r\n        str = str.substr(0, 32);\r\n        return b64.encode(str);\r\n    }\r\n}","\r\nexport class GameData {\r\n    public static bClickFullscreen: boolean = false;\r\n    //转移至version.json\r\n    public static readonly channel: string = \"channel20181016\";\r\n    public static joinLobbyType: JoinLobbyType;\r\n    public static bindVo: bindPhoneVo;//绑定手机相关数据\r\n}\r\n\r\nexport class NoticeData {\r\n    public static shareId: number = 0;\r\n    public static shareLimit: number = 0;\r\n    public static currentTab: number = 0;//0-公告，1-活动\r\n    public static noticeid: number = -1;\r\n    public static noticeType: string = null;\r\n}\r\n\r\nexport interface bindPhoneVo {\r\n    bind: boolean;//业主开关(是否开启绑定送金功能)\r\n    receive: boolean;//是否已领取过绑定送金奖励\r\n    reward: number;//绑定送金额度\r\n}\r\n/**\r\n * 游戏状态\r\n */\r\nexport enum GameState {\r\n    NORMAL,//正常状态(可点击进入游戏)\r\n    PAUSE,//维护中\r\n    EXPECTATION,//尽请期待\r\n    UPDATE,//待更新\r\n    CLOSE,//关闭状态\r\n    TESTING,//测试状态(同维护中状态处理)\r\n}\r\n/**\r\n * 游戏分类\r\n */\r\nexport enum GameType {\r\n    classify = 1,//分类游戏\r\n    selfGame,//自有游戏\r\n    other,//三方游戏\r\n}\r\n/**\r\n * 游戏菜单分类\r\n */\r\nexport enum GameMenuType {\r\n    chessGame = 192,//棋牌游戏\r\n    hotGame = 197,//热门\r\n    fishGame = 200,//捕鱼\r\n    FGCard = 198,//FG棋牌\r\n    kaiyuan = 199,//开元\r\n    electron = 195,//电子\r\n    lottery = 196,//彩票\r\n    realPerson = 193,//真人视讯\r\n    sport = 194,//体育竞技\r\n}\r\n/**\r\n * 游戏网络数据模型\r\n */\r\nexport interface GameIconVo {\r\n    alias: string,\r\n    gameType: string,\r\n    id: number,\r\n    name: string,\r\n    jumpUrl: boolean,\r\n    state: string,\r\n    url: string,//网络地址目录\r\n    classifyOnlineFlag?: boolean;//true-上线，false-下线\r\n    classify: GameType;//1-分类， 2-自有游戏， 3-第三方游戏\r\n    classifySort: string;//1是一级分类游戏   2是二级分类游戏\r\n    classifyAlias?: string;//分类别名\r\n    classifyGameIds?: string;//分类游戏id集合拼接字符串(请求分类列表时需要)\r\n    gamePlatform?: string;//分类或第三方游戏所属平台\r\n    classifyThirdGameId?: string;//三方游戏点击后调用接口需要\r\n    bhot?: boolean,\r\n    icon: string;//三方游戏icon\r\n    vertical?: boolean;//是否竖屏显示\r\n}\r\n/**\r\n * 游戏本地配置数据模型\r\n */\r\nexport interface GameIconConfig {\r\n    desc: string,//描述\r\n    alias: string,//别名\r\n    norm:
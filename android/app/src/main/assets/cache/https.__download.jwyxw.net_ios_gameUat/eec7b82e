idth - 1334 >> 1;\r\n\t\tif (offset > 0) this.confirmBtn.right += offset;\r\n\t\t//点击透明背景\r\n\t\tthis.bgmc.on(Laya.Event.MOUSE_DOWN, this, () => {\r\n\t\t\tif (this.isclick) KeyboardView.hide();\r\n\t\t});\r\n\t\t//完成\r\n\t\tEventManager.addTouchScaleListener(this.confirmBtn, this, () => {\r\n\t\t\tSoundPlayer.closeSound();\r\n\t\t\tif (!this.isclick) return;\r\n\t\t\tif (this.vo.caller && this.vo.callback) this.vo.callback.call(this.vo.caller, this.input);\r\n\t\t\tKeyboardView.hide();\r\n\t\t}, null, 0.7, true);\r\n\r\n\t\tlet btn: Laya.Button;\r\n\t\tfor (let i = 0; i < 12; i++) {\r\n\t\t\tbtn = this[\"btn\" + i];\r\n\t\t\tif (btn) {\r\n\t\t\t\tEventManager.addTouchScaleListener(btn, this, this.btnHandler, i, 0.95, false, false);\r\n\t\t\t\tbtn.on(Laya.Event.MOUSE_DOWN, this, this.downBtn, [btn, i == 11]);\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n\r\n\tprivate downBtn(btn: Laya.Button, isDel: boolean) {\r\n\t\tbtn.alpha = 0.5;\r\n\t\tbtn.on(Laya.Event.MOUSE_OUT, this, this.outBtn, [btn]);\r\n\t\tbtn.on(Laya.Event.MOUSE_UP, this, this.outBtn, [btn]);\r\n\t\tif (isDel) Laya.timer.once(500, this, this.startDel);\r\n\t}\r\n\tprivate startDel() {\r\n\t\tthis.doDel();\r\n\t\tLaya.timer.loop(200, this, this.doDel);\r\n\t}\r\n\tprivate doDel() {\r\n\t\tif (this.input.length) {\r\n\t\t\tthis.input = this.input.substr(0, this.input.length - 1);\r\n\t\t\tthis.updateInput();\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t} else {\r\n\t\t\tLaya.timer.clear(this, this.doDel);\r\n\t\t}\r\n\t}\r\n\tprivate outBtn(btn: Laya.Button) {\r\n\t\tbtn.off(Laya.Event.MOUSE_OUT, this, this.outBtn);\r\n\t\tbtn.off(Laya.Event.MOUSE_UP, this, this.outBtn);\r\n\t\tLaya.timer.clearAll(this);\r\n\t\tbtn.alpha = 1;\r\n\t}\r\n\tprivate btnHandler(e: Laya.Event, num: number) {\r\n\t\tSoundPlayer.clickSound();\r\n\t\t//长度限制\r\n\t\tif (this.vo.maxLen && this.input.length >= this.vo.maxLen && num != 11) {\r\n\t\t\treturn;\r\n\t\t}\r\n\r\n\t\tif (num < 10) {\r\n\t\t\tthis.input += num;\r\n\t\t} else if (num == 10) {//小数点\r\n\t\t\tthis.input += \".\";\r\n\t\t} else if (num == 11) {//删除键\r\n\t\t\tthis.input = this.input.substr(0, this.input.length - 1);\r\n\t\t}\r\n\t\tthis.updateInput();\r\n\t}\r\n\t//同步输入\r\n\tprivate updateInput() {\r\n\t\t//自定义键盘输入赋值\r\n\t\tthis.inputTxt.text = this.input;\r\n\t\t//同步到目标文本\r\n\t\tif (this.vo.inputExtend) this.vo.inputExtend.text = this.input;\r\n\t\telse {\r\n\t\t\tif (this.target) this.target.text = this.input;\r\n\t\t}\r\n\t}\r\n\t/**\r\n\t * 绑定自定义键盘\r\n\t * @param vo \r\n\t */\r\n\tpublic static bindKeyboard(input: Laya.TextInput, vo?: KeyBoardVo) {\r\n\t\tif (!vo) vo = {};\r\n\t\tvo.input = input;\r\n\t\tif (!vo.hint) vo.hint = input.prompt;\r\n\t\tif (!vo.maxLen && input.maxChars) vo.maxLen = input.maxChars;\r\n\t\tinput.editable = false;\r\n\t\tif (isNative) input.editable = true;//todo:debug\r\n\t\tinput.on(Laya.Event.MOUSE_DOWN, this, () => {\r\n\t\t\tinput.focus = false;\r\n\t\t\tthis.show(vo.inputExtend ? vo.inputExtend.text : input.text, vo);\r\n\t\t});\r\n\t}\r\n\t/**\r\n\t * 刷新界面\r\n\t * @param hint \r\n\t * @param caller \r\n\t * @param callback \r\n\t */\r\n\tpublic update(input: string, vo: KeyBoardVo) {\r\n\t\tthis.isclick = false;\r\n\t\tthis.target = vo.input;\r\n\t\tthis.vo = vo;\r\n\t\tthis.input = input || \"\";\r\n\t\tthis.setType = false;\r\n\t\tif (vo.hint) this.inputTxt.prompt = vo.hint;\r\n\t\telse this.inputTxt.prompt = \"请输入金额\";\r\n\t\t//设置类型\r\n\t\tthis.inputTxt.text = this.input;\r\n\t\tif (this.inputTxt.type != this.target.type) {\r\n\t\t\tthis.inputTxt.type = this.target.type;\r\n\t\t\tthis.setType = true;\r\n\t\t}\r\n\t}\r\n\t//激活\r\n\tpublic doActive() {\r\n\t\t//当类型发生改变时需要获得焦点才会生效\r\n\t\tif (this.setType) this.inputTxt.focus = true;\r\n\t\tthis.isclick = true;\r\n\t}\r\n\t/**\r\n\t * 显示键盘\r\n\t * @param hint 输入提示\r\n\t * @param caller \r\n\t * @param callback \r\n\t */\r\n\tpublic static show(input: string = \"\", vo: KeyBoardVo) {\r\n\t\tif (!this.view) {\r\n\t\t\tthis.view = new KeyboardView();\r\n\t\t}\
试用\r\n            if (Common.isWeb) {//发布web版本需要\r\n                ConfObjRead.getConfUrl().url = ConfObjRead.getConfUrl().urlWeb;\r\n            }\r\n        }\r\n    }\r\n    /**\r\n     * 获取渠道开关\r\n     * 开关开启后会屏蔽一些内容\r\n     */\r\n    public static get channelOpen(): boolean {\r\n        return AppData.isAndroidHack;\r\n    }\r\n    /**\r\n     * 获取游戏黑名单列表(用于渠道)\r\n     */\r\n    public static get channelBlackIconList(): string[] {\r\n        if (!AppData.hackData) AppData.hackData = { filterGameList: [\"zjh\"] };\r\n        let arr: string[] = AppData.hackData.filterGameList || [];\r\n        return this.channelOpen ? arr : [];\r\n    }\r\n    /**\r\n     * 热更版本编号\r\n     */\r\n    public static get hotVerNum(): number {\r\n        return AppData.specialVersionHot || 0;\r\n    }\r\n}\r\n","import { ui } from \"../../ui/layaMaxUI\";\r\nimport { Toast } from \"../toast/Toast\";\r\n\r\n\r\n/**\r\n * toast提示ui\r\n */\r\nexport class ToastView extends ui.sceneUI.comp.ToastViewUI {\r\n\tprivate readonly delayTime = 2600;//延迟删除时间\r\n\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t}\r\n\t/**\r\n\t * 读取信息\r\n\t * @param value \r\n\t */\r\n\tpublic readInfo(value: string) {\r\n\t\tthis.msgTxt.text = value;\r\n\t\tthis.x = Laya.stage.width - this.width >> 1;\r\n\t\tthis.y = Laya.stage.height / 2 + this.height;\r\n\t\tthis.alpha = 0;\r\n\t\tLaya.Tween.to(this, { alpha: 1, y: Laya.stage.height - this.height >> 1 }, 300, Laya.Ease.cubicOut, Laya.Handler.create(this, this.delayClear));\r\n\t}\r\n\tprivate delayClear() {\r\n\t\tLaya.timer.once(this.delayTime, this, () => {\r\n\t\t\tlet id = Toast.tipArr.indexOf(this);\r\n\t\t\tif (id != -1) {\r\n\t\t\t\tToast.tipArr.splice(id, 1);\r\n\t\t\t}\r\n\t\t\tLaya.Tween.to(this, { y: this.y - this.height, alpha: 0 }, 300, Laya.Ease.linearNone, Laya.Handler.create(this, this.removeUI));\r\n\t\t});\r\n\t}\r\n\tprivate removeUI() {\r\n\t\tLaya.Tween.clearAll(this);\r\n\t\tthis.removeSelf();\r\n\t\tLaya.Pool.recover(Toast.poolMark, this);\r\n\
 }\r\n    public hideLastView() {\r\n        if (this.lastView) this.lastView.visible = false;\r\n        this.clearOther();\r\n    }\r\n    /**\r\n     * 清理辅助界面\r\n     */\r\n    public clearOther() {\r\n        if (this.viewbox) {\r\n            this.viewbox.destroyChildren();\r\n        }\r\n    }\r\n    /**\r\n     * 支付入口\r\n     * @param vo \r\n     * @param money 支付金额\r\n     * @param bankPay 是否为银行转账\r\n     */\r\n    public startPay(vo: any, money: number, bankPay?: boolean) {\r\n        let showInfo: boolean;//是否需要填写存款人信息\r\n        vo.amount = money;//记录金额\r\n\r\n        if (bankPay) {//银行转账类型\r\n            let data = vo as BankSubListVo;\r\n            showInfo = data.realNameReq;\r\n            if (!data.isCombine) showInfo = true;//todo:(临时处理,后续了解情况后删除)\r\n        } else {//其他类型\r\n            let data = vo as PaySubListVo;\r\n            showInfo = data.realNameReq || data.mobileNoReq || data.cardNoReq;\r\n            if (data.platform && data.webBankList) {//网银支付\r\n                showInfo = true;\r\n            }\r\n        }\r\n\r\n        if (showInfo) {//需要填写存款人信息\r\n            this.hideLastView();\r\n\r\n            let ui = new AccountInfoView();\r\n            ui.show(vo, bankPay);\r\n            this.viewbox.addChild(ui);\r\n        } else {\r\n            bankPay ? this.showBankPayInfoView(vo) : this.showOtherPayInfoView(vo);\r\n        }\r\n    }\r\n    /**\r\n     * 公众号支付(与其他支付不同，不需要调用支付接口)\r\n     * @param vo \r\n     */\r\n    public doWechatPublicPay(vo: BankSubListVo) {\r\n        this.hideLastView();\r\n        let qrvo: QRShowVo = {\r\n            qrUrl: vo.bankCardNo,\r\n            isBank: true\r\n        }\r\n        let ui = new SaveQrInfoView();\r\n        ui.show(vo, qrvo);\r\n        this.viewbox.addChild(ui);\r\n    }\r\n    /**\r\n     * 调用银行转账\r\n     * @param vo \r\n     * @param money \r\n     * @param depositorName 存款人\r\n     */\r\n    public doBankPay(vo: BankSubListVo, depositorName?: string) {\r\n        PayModel.payByBankList(vo, vo.amount, depositorName, this, (suc: boolean, jobj: any) => {\r\n            Debug.log(\"银行转账结果:\", suc, jobj);\r\n            if (suc) {\r\n                if (vo.isCombine) {\r\n                    this.hideLastView();\r\n                    let ui = new SaveQrInfoView();\r\n                    let qrvo: QRShowVo = {\r\n                        amount: jobj.amount,\r\n                        orderNo: jobj.transactionNo,\r\n                        qrUrl: vo.bankCardNo,\r\n                        isBank: true\r\n                    }\r\n                    ui.show(vo, qrvo);\r\n                    this.viewbox.addChild(ui);\r\n                } else {\r\n                    this.showPayResultView(jobj.amount);\r\n                }\r\n            }\r\n        });\r\n    }\r\n    /**\r\n     * 银行转账信息展示\r\n     */\r\n    public showBankPayInfoView(vo: BankSubListVo, depositorName?: string) {\r\n        if (vo.isCombine) {//是合并数据(银行列表中的非银行类型)\r\n            this.doBankPay(vo, depositorName);\r\n        } else {\r\n            this.hideLastView();\r\n\r\n            let ui = new BankTransferInfoView();\r\n            ui.show(vo, depositorName);\r\n            this.viewbox.addChild(ui);\r\n        }\r\n    }\r\n    /**\r\n     * 转账结果展示(所有类型都会展示)\r\n     * @param money \r\n     */\r\n    public showPayResultView(money: number) {\r\n        this.hideLastView();\r\n\r\n        let ui = new PayResultView();\r\n        ui.show(money);\r\n        this.viewbox.addChild(ui);\r\n    }\r\n    /**\r\n     * 显示其他转账信息界面\r\n     */\r\n    public showOtherPayInfoView(vo: PaySubListVo, info?: PayInfoVo) {\r\n        PayModel.payByListV2(vo, vo.amount, info, this, (suc: boolean, jobj: PayResultVo) => {\r\n            Debug.log(\"支付结果:\", suc, jobj);\r\n            if (suc) {\r\n                if (jobj.webview) {//跳转浏览器\r\n                    this.showPayResultView(jobj.amount);\r\n                    GameUtils.openWeb(jobj.data);\r\n                } else if (!jobj.webview) {//显示二维码\r\n                    if (!jobj.paymentJumpTypeEnum || PayMentJumpType[jobj.paymentJumpTypeEnum] == PayMentJumpType.IMG) {\r\n                        this.hideLastView();\r\n\r\n                        let ui = new SaveQrInfoView();\r\n                        let qrvo: QRShowVo = {\r\n                            amount: jobj.amount,\r\n                            orderNo: jobj.transactionId,\r\n                            qrUrl: jobj.data,\r\n                            creatQR: !jobj.paymentJumpTypeEnum\r\n                        }\r\n                        ui.show(vo, qrvo);\r\n                        this.viewbox.addChild(ui);\r\n                    }\r\n                }\r\n            }\r\n        });\r\n    }\r\n    public reset() {\r\n        this.clearLastView();\r\n        this.clearOther();\r\n        if (this.viewbox) this.viewbox = null;\r\n    }\r\n}","/**Created by the LayaAirIDE*/\r\n\r\nimport { ui } from \"../../../ui/layaMaxUI\";\r\n\r\n/**\r\n * 金额按钮\r\n */\r\nexport class PayMoneyBtn extends ui.sceneUI.dlg.pay.PayMoneyBtnUI {\r\n\tprivate _value: number = 0;\r\n\tpublic selected: boolean;\r\n\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t\tthis.selectBg.alpha = 0;\r\n\t}\r\n\tpublic set value(vl: number) {\r\n\t\tthis._value = vl;\r\n\t\tthis.txt.text = vl + \"元\";\r\n\t\tthis.selectBg.alpha = 0;\r\n\t\tthis.selected = false;\r\n\t}\r\n\tpublic get value(): number {\r\n\t\treturn this._value;\r\n\t}\r\n\tpublic setSelect(bl: boolean) {\r\n\t\tLaya.Tween.to(this.selectBg, { alpha: bl ? 1 : 0 }, 300);\r\n\t\tthis.selected = bl;\r\n\t}\r\n}\r\n","import { Debug } from '../../../common/Debug';\r\nimport { PayModel, PaySubListVo } from '../../../data/PayModel';\r\nimport { EventManager } from '../../../events/EventManager';\r\nimport { ui } from \"../../../ui/layaMaxUI\";\r\nimport { ComponentTool } from '../../../utils/GameUtils';\r\nimport { SoundPlayer } from '../../../utils/SoundPlayer';\r\nimport { Toast } from '../../toast/Toast';\r\ni
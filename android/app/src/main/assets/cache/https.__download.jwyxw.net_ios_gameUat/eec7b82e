   this.bits.length = 0;\r\n    }\r\n    private creatFont() {\r\n        let strArr = this._txt.split(\"\");\r\n        let index: number = 0;\r\n        let tox: number = 0;\r\n        let maxH: number = 0;\r\n        strArr.forEach(str => {\r\n            let url: string = this.config[str];\r\n            if (url) {\r\n                let bit = new Laya.Image(url);\r\n                bit.x = tox;\r\n                this.addChild(bit);\r\n                this.bits.push(bit);\r\n                index++;\r\n                tox += bit.width + this._spacing;\r\n                if (bit.height > maxH) maxH = bit.height;\r\n            }\r\n        });\r\n        this.width = tox - this._spacing;\r\n        this.height = maxH;\r\n    }\r\n    public destroy() {\r\n        this.reset();\r\n        this.bits = null;\r\n        super.destroy(true);\r\n    }\r\n}","import { PostMHelp } from \"../../../common/PostMHelp\";\r\nimport { AgentModel } from \"../../../data/AgentModel\";\r\nimport { DlgCmd } from \"../../../data/GameVos\";\r\nimport { EventManager } from \"../../../events/EventManager\";\r\nimport { ui } from \"../../../ui/layaMaxUI\";\r\nimport { GameMgr } from \"../../../utils/GameMgr\";\r\nimport { SoundPlayer } from \"../../../utils/SoundPlayer\";\r\nimport { PageManager } from \"../../PageManager\";\r\nimport { Toast } from \"../../toast/Toast\";\r\n\r\n\r\n/**\r\n * 代理中心二维码放大弹窗\r\n */\r\nexport class AgentQrDlg extends ui.sceneUI.dlg.agent.AgentQrDlgUI {\r\n\tpublic static saveBase64: string = null;\r\n\t/**\r\n\t * 打开代理二维码海报弹窗\r\n\t * @param url 二维码链接\r\n\t * @param msg 海报信息\r\n\t */\r\n\tpublic static show(url: string, msg: string, nameStr: string) {\r\n\t\tlet dlg = new AgentQrDlg();\r\n\t\t//全屏适配\r\n\t\tdlg.width = Laya.stage.width;\r\n\t\t//初始化\r\n\t\tdlg.SetData(url, msg, nameStr);\r\n\t\t//显示\r\n\t\tdlg.popup(false, true);\r\n\t}\r\n\t/**\r\n\t * 用于分享截屏-----------------------\r\n\t */\r\n\tprivate static shareObj: any;\r\n\tpublic static shareDarw(share
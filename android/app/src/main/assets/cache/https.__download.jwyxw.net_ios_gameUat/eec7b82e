ogin/FindPasswordDlg.ts","../../src/view/dlg/login/AccountLoginDlg.ts","../../src/common/QueueTask.ts","../../src/view/LobbyNewView.ts","../../src/view/LobbyScene.ts","../../src/view/LoginScene.ts","../../src/view/dlg/notice/NoticeTabView.ts","../../src/view/EmbedLoadingView.ts","../../src/view/notice/message/Notice_Message.ts","../../src/view/notice/NoticeDlg.ts","../../src/view/notice/RouletteDlg.ts","../../src/view/notice/RouletteRewardDlg.ts","../../src/view/notice/SharedailyDlg.ts","../../src/utils/UpdateMsgHandle.ts","../../src/common/DragingBox.ts","../../src/view/comp/GameIconListPanel.ts","../../src/view/comp/UploadTimeBar.ts","../../src/view/comp/GameIconView.ts","../../src/view/dlg/ChessGameDlg.ts","../../src/view/PageManager.ts","../../src/common/Tools.ts","../../src/data/LoginModel.ts","../../src/utils/GameMgr.ts","../../src/data/AgentModel.ts","../../src/common/Common.ts","../../src/view/dlg/GameUpdateNotice.ts","../../src/data/LobbyModel.ts","../../src/common/PostMHelp.ts","../../src/utils/GameUtils.ts","../../src/view/debug/ScreenPrintView.ts","../../src/view/debug/Printer.ts","../../src/common/Debug.ts","../../src/common/ColorTool.ts","../../src/view/comp/GameMenuItem.ts","../../src/view/comp/ClassifyMenu.ts","../../src/view/comp/ChessMenu.ts","../../src/view/notice/message/spinner/Roullette.ts","../../src/view/dlg/personaCenter/VipGiftsPanel.ts","../../src/view/dlg/personaCenter/VipLevelIntroduce.ts","../../src/view/dlg/withdraw/WithDrawTab.ts","../../src/view/anim/LightAnim.ts","../../src/common/CyclePageBox.ts","../../src/view/comp/CycleComp.ts","../../src/view/comp/NoticeComp.ts","../../src/view/comp/UserInfoView.ts","../../src/GameConfig.ts","../../src/Main.ts"],"sourcesContent":["/**This class is automatically generated by LayaAirIDE, please do not make any modifications. */\nimport View=Laya.View;\r\nimport Dialog=Laya.Dialog;\r\nimport Scene=Laya.Scene;\nvar REG: Function = Laya.ClassUtils.regClass;\nexport module ui.sceneUI.anim {\r\n    ex
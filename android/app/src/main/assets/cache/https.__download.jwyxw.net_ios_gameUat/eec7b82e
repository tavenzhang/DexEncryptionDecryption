rt { PageManager } from \"../view/PageManager\";\r\nimport { Debug } from \"./Debug\";\r\n\r\nexport interface CycelVo {\r\n    carouselUrl: string,//图片地址\r\n    carouselHref: string,//链接地址\r\n    jumpInner?: boolean,//是否内部跳转(否则跳转网页)\r\n    jumpGame?: boolean,//是否跳转到游戏\r\n}\r\n/*\r\n* 轮播图控件\r\n*/\r\nexport class CyclePageBox extends Laya.Sprite {\r\n    private itemBox: Laya.Sprite = new Laya.Sprite();\r\n    private imgList: Laya.Image[] = [];\r\n    private itemList: CycelVo[];//数据\r\n\r\n    private viewWidth: number;\r\n    private viewHeight: number;\r\n    private spaceTime: number;//轮播间隔时间\r\n    public tweenTime: number = 400;//缓动时间\r\n    private total: number;\r\n    private moveIndex: number;\r\n\r\n    private downPos: number;\r\n    private downStageX: number;\r\n    private moveNum: number;\r\n    private downTime: number;\r\n\r\n    //当前实际索引\r\n    public get curIndex(): number {\r\n        return Math.abs(this.moveIndex % this.total);\r\n    }\r\n    /**\r\n     * \r\n     * @param viewWidth 轮播视图的宽\r\n     * @param viewHeight \r\n     */\r\n    constructor(viewWidth: number, viewHeight: number) {\r\n        super();\r\n        this.width = viewWidth;\r\n        this.height = viewHeight;\r\n        this.viewWidth = viewWidth;\r\n        this.viewHeight = viewHeight;\r\n        this.scrollRect = new Laya.Rectangle(0, 0, viewWidth, viewHeight);\r\n\r\n        this.addChild(this.itemBox);\r\n    }\r\n    /**\r\n     * 初始化数据\r\n     * @param itemList \r\n     * @param spaceTime \r\n     */\r\n    public init(itemList: CycelVo[], spaceTime: number) {\r\n        this.itemList = itemList;\r\n        this.spaceTime = spaceTime;\r\n        this.total = itemList.length;\r\n        this.moveIndex = 0;\r\n\r\n        itemList.forEach(vo => {\r\n            let img = new Laya.Image();\r\n            img.width = this.viewWidth;\r\n            img.height = this.viewHeight;\r\n            img.skin = vo.carouselUrl;\r\n            this.imgList.push(img);\r\n        });\r\n\r\n        if (this.total > 0) this.itemBox.addChild(this.imgList[0]);\r\n\r\n        if (this.total > 1) {\r\n            this.addListener();\r\n        } else if (this.total == 1) {\r\n            this.on(Laya.Event.CLICK, this, this.singleHandler);\r\n        }\r\n    }\r\n    private singleHandler() {\r\n        let vo: CycelVo = this.itemList[0];\r\n        this.openLink(vo);\r\n    }\r\n    private openLink(vo: CycelVo) {\r\n        if (vo && vo.carouselHref) {\r\n            if (vo.jumpInner) {\r\n                if (vo.jumpGame) {\r\n                    EventManager.dispath(EventType.JUMP_GAME, vo.carouselHref);\r\n                }\r\n                else {\r\n                    SoundPlayer.enterPanelSound();\r\n                    if (vo.carouselHref.indexOf(\"activityDetails\") != -1) {//跳转到活动指定页面\r\n                        let arr = vo.carouselHref.split(\"-\");\r\n                        let id = arr[1];\r\n                        let type = arr[2];\r\n                        Debug.log(\"跳转指定活动页:\", vo.carouselHref);\r\n                        if (type && id) {\r\n                            PageManager.showDlg(DlgCmd.activityCenter, parseInt(type) + 1, parseInt(id));\r\n                        } else if (id) {\r\n                            PageManager.showDlg(DlgCmd.activityCenter, DlgCmd.activityCenter, parseInt(id));\r\n                        } else {\r\n                            Debug.error(\"配置异常：\", vo.carouselHref);\r\n                        }\r\n                    } else {\r\n                        InnerJumpUtil.doJump(DlgCmd[vo.carouselHref]);\r\n                    }\r\n                }\r\n            } else {//跳转网页\r\n                SoundPlayer.clickSound();\r\n                GameUtils.openWeb(vo.carouselHref);\r\n            }\r\n        }\r\n    }\r\n    /**\r\n     * 刷新数据\r\n     * @param itemList \r\n     */\r\n    public flushData(itemList: CycelVo[]) {\r\n        if (!itemList || itemList.length == 0) return;\r\n        this.off(Laya.Event.MOUSE_DOWN, this, this.touchBeginHandler);\r\n        this.off(Laya.Event.CLICK, this, this.singleHandler);\r\n        this.stopTimer();\r\n\r\n        this.imgList.forEach(img => {\r\n            img.destroy(true);\r\n        })\r\n        this.imgList.length = 0;\r\n        this.itemBox.x = 0;\r\n        this.itemBox.removeChildren();\r\n        //\r\n        this.init(itemList, this.spaceTime);\r\n    }\r\n    /**\r\n     * 销毁\r\n     */\r\n    public destroy(bl: boolean = true) {\r\n        this.off(Laya.Event.MOUSE_DOWN, this, this.touchBeginHandler);\r\n        this.off(Laya.Event.CLICK, this, this.singleHandler);\r\n        this.stopTimer();\r\n\r\n        super.destroy(true);\r\n    }\r\n    private addListener() {\r\n        this.on(Laya.Event.MOUSE_DOWN, this, this.touchBeginHandler);\r\n        this.addTimer();\r\n    }\r\n    public addTimer() {\r\n        if (this.total > 1) Laya.timer.loop(this.spaceTime, this, this.doTimer);\r\n    }\r\n    public stopTimer() {\r\n        Laya.timer.clear(this, this.doTimer);\r\n    }\r\n    private touchBeginHandler(evt: Laya.Event) {\r\n        this.downPos = this.itemBox.x;\r\n        this.downStageX = evt.stageX;\r\n        this.downTime = Laya.Browser.now();\r\n        this.stopTimer();\r\n\r\n        this.on(Laya.Event.MOUSE_MOVE, this, this.touchMoveHandler);\r\n        this.on(Laya.Event.MOUSE_OUT, this, this.touchEndHandler);\r\n        this.on(Laya.Event.MOUSE_UP, this, this.touchEndHandler);\r\n    }\r\n    private touchMoveHandler(evt: Laya.Event) {\r\n        this.moveNum = evt.stageX - this.downStageX;\r\n        let abs: number = this.moveNum < 0 ? -this.moveNum : this.moveNum;\r\n        if (abs > 10) {\r\n            Laya.Tween.clearTween(this.itemBox);\r\n\r\n            this.itemBox.x = this.downPos + this.moveNum;\r\n            let index: number = this.moveIndex % this.total;\r\n            let dex: number = this.moveNum > 0 ? index - 1 : index + 1;\r\n            if (dex < 0) dex = this.total + dex;\r\n            if (dex >= this.total) dex = 0;\r\n\r\n            let pic: Laya.Image = this.imgList[dex];\r\n\r\n            if (!this.itemBox.contains(pic)) {\r\n                this.itemBox.addChild(pic);\r\n            }\r\n            pic.x = (this.itemBox.getChildAt(0) as Laya.Image).x;\r\n            this.moveNum > 0 ? pic.x -= this.viewWidth : pic.x += this.viewWidth;\r\n        }\r\n    }\r\n    private touchEndHandler() {\r\n        this.off(Laya.Event.MOUSE_MOVE, this, this.touchMoveHandler);\r\n        this.off(Laya.Event.MOUSE_OUT, this, this.touchEndHandler);\r\n        this.off(Laya.Event.MOUSE_UP, this, this.touchEndHandler);\r\n\r\n        this.addTimer();\r\n        if (Math.abs(this.moveNum) > 10) {\r\n            let next: boolean = Boolean(this.moveNum < 0);\r\n            let dex: number = next ? this.moveIndex + 1 : this.moveIndex - 1;\r\n            let toPos: number = -dex * this.viewWidth;\r\n            if (this.itemBox.numChildren > 2) {\r\n                let temp = this.itemBox.getChildAt(2) as Laya.Sprite;\r\n                let temp2 = this.itemBox.getChildAt(1) as Laya.Sprite;\r\n                if (next) {\r\n                    if (temp.x < temp2.x) this.itemBox.removeChild(temp);\r\n                    else this.itemBox.removeChild(temp2);\r\n                } else {\r\n                    if (temp.x > temp2.x) this.itemBox.removeChild(temp);\r\n                    else this.itemBox.removeChild(temp2);\r\n                }\r\n            }\r\n            this.doScroll(toPos, 250, next);\r\n        } else {\r\n            let time: number = Laya.Browser.now() - this.downTime;\r\n            if (time < 350) {//点击处理\r\n                let vo: CycelVo = this.itemList[this.curIndex];\r\n                this.openLink(vo);\r\n            }\r\n        }\r\n        this.moveNum = 0;\r\n    }\r\n    //定时滚动\r\n    private doTimer(): void {\r\n        let index: number = this.moveIndex + 1;\r\n        let toPos: number = -index * this.viewWidth;\r\n        let dex: number = this.moveIndex % this.total + 1;\r\n        if (dex < 0) dex = this.total + dex;\r\n        if (dex >= this.total) dex = 0;\r\n\r\n        let pic: Laya.Image = this.imgList[dex];\r\n        this.itemBox.addChild(pic);\r\n        pic.x = (this.itemBox.getChildAt(0) as Laya.Sprite).x + this.viewWidth;\r\n\r\n        this.doScroll(toPos, 0, true);\r\n    }\r\n\r\n    private doScroll(tPos: number, time?: number, next?: boolean): void {\r\n        let t: number = time ? time : this.tweenTime;\r\n        Laya.Tween.clearTween(this.itemBox);\r\n\r\n        Laya.Tween.to(this.itemBox,\r\n            { x: tPos },\r\n            t,\r\n            Laya.Ease.linearOut,\r\n            Laya.Handler.create(this, () => {\r\n                if (this.itemBox.numChildren > 1) this.itemBox.removeChildAt(0);\r\n                next ? this.moveIndex++ : this.moveIndex--;\r\n                this.changeIndex();\r\n            })\r\n        );\r\n    }\r\n    private changeIndex(): void {\r\n        //改变点点状态\r\n    }\r\n\r\n}","import { CycelVo, CyclePageBox } from \"../../common/CyclePageBox\";\r\nimport { LobbyModel } from \"../../data/LobbyModel\";\r\nimport { EventManager, EventType } from \"../../events/EventManager\";\r\nimport { ui } from \"../../ui/layaMaxUI\";\r\n\r\n/**\r\n * 轮播图模块组\r\n */\r\nexport default class CycleComp extends ui.sceneUI.comp.CycleCompUI {\r\n\tprivate cycleView: CyclePageBox;\r\n\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t\tthis.initView();\r\n\t}\r\n\tprivate initView() {\r\n\t\tEventManager.register(EventType.CAROUSE_DATA, this, this.initCycelView);\r\n\t\tLobbyModel.requestCycelData();\r\n\t}\r\n\t/**\r\n\t * 暂停轮播图滚动\r\n\t */\r\n\tpublic pause() {\r\n\t\tif (this.cycleView) {\r\n\t\t\tthis.cycleView.stopTimer();\r\n\t\t}\r\n\t}\r\n\t//轮播图\r\n\tprivate initCycelView(flush: boolean) {\r\n\t\tif (!flush) return;\r\n\t\tlet arr: CycelVo[] = LobbyModel.cycelData;\r\n\t\tif (!arr || arr.length == 0) retu
\n\t */\r\n\tpublic static isQueue(type: QueueType): boolean {\r\n\t\tlet list = this.queueList[type];\r\n\t\treturn Boolean(list);\r\n\t}\r\n\t/**\r\n\t * 检测队列任务\r\n\t */\r\n\tpublic static checkQueue(types?: QueueType[]): boolean {\r\n\t\tlet isopen: boolean = false;\r\n\t\tif (types) {\r\n\t\t\tfor (let i = 0; i < types.length; i++) {\r\n\t\t\t\tlet type = types[i];\r\n\t\t\t\tlet list = this.queueList[type];\r\n\t\t\t\tif (list) {\r\n\t\t\t\t\tlet obstruct = false;\r\n\t\t\t\t\tlet data = list.shift();\r\n\t\t\t\t\tisopen = this.execute(data);\r\n\t\t\t\t\tif (data.obstruct) {\r\n\t\t\t\t\t\tobstruct = true;\r\n\t\t\t\t\t}\r\n\t\t\t\t\tif (list.length == 0)\r\n\t\t\t\t\t\tdelete this.queueList[type];\r\n\t\t\t\t\tif (obstruct)\r\n\t\t\t\t\t\tbreak;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t}\r\n\t\treturn isopen;\r\n\t}\r\n\r\n\tprivate static execute(vo: QueueVo): boolean {\r\n\t\tlet isopen: boolean = false;\r\n\t\tswitch (vo.type) {\r\n\t\t\tcase QueueType.bindPhoneActiv:\r\n\t\t\t\tPageManager.showDlg(DlgCmd.bindPhoneAct);\r\n\t\t\t\tisopen = true;\r\n\t\t\t\tbreak;\r\n\t\t\tcase QueueType.shartDlg:\r\n\t\t\t\tPageManager.showDlg(DlgCmd.shareDaily);\r\n\t\t\t\tisopen = true;\r\n\t\t\t\tbreak;\r\n\t\t\tcase QueueType.rouletteDlg:\r\n\t\t\t\tPageManager.showDlg(DlgCmd.RouletteDlg);\r\n\t\t\t\tisopen = true;\r\n\t\t\t\tbreak;\r\n\t\t\tcase QueueType.activityDlg:\r\n\t\t\t\tPageManager.showDlg(DlgCmd.activityCenter, vo.params.noticeCate, vo.params.noticeid);\r\n\t\t\t\tisopen = true;\r\n\t\t\t\tbreak;\r\n\t\t}\r\n\t\treturn isopen;\r\n\t}\r\n}\r\n","import { Common } from '../common/Common';\r\nimport { Debug } from '../common/Debug';\r\nimport { QueueTask, QueueType } from '../common/QueueTask';\r\nimport { ToolsApp } from \"../common/ToolsApp\";\r\nimport { DlgCmd, GameData } from \"../data/GameVos\";\r\nimport { LobbyCmd, LobbyModel } from \"../data/LobbyModel\";\r\nimport { LoginModel } from '../data/LoginModel';\r\nimport { WithDrawModel } from '../data/WithDrawModel';\r\nimport { EventManager, EventType } from \"../events/EventManager\";\r\n/**Created by the LayaAirIDE*/\r\nimport { ui } from \"../ui/layaMaxUI\";\r\nimport { SoundPlayer } from \"../utils/SoundPlayer\";\r\nimport { GameUtils, InnerJumpUtil } from './../utils/GameUtils';\r\nimport { PageManager } from \"./PageManager\";\r\nimport { Toast } from \"./toast/Toast\";\r\n\r\n/**\r\n * 新版大厅\r\n */\r\nexport class LobbyNewView extends ui.sceneUI.LobbyNewViewUI {\r\n\tprivate animArr: Laya.Skeleton[] = [];//存放动画，用于统一管理\r\n\tprivate isFirst: boolean;\r\n\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t\tthis.initView();\r\n\t}\r\n\tprivate initView() {\r\n\t\tthis.initEvents();\r\n\t\tthis.readData();\r\n\r\n\t\tthis.layoutBottomLeftBtn();\r\n\t\tthis.layoutTopRightBtn();\r\n\t\tthis.dpiLayout();\r\n\r\n\t\tthis.animArr.push(this.userInfo.goldAnim, this.bindAnim, this.agentAnim, this.tixianAnim, this.payAnim);\r\n\r\n\t\tthis.playEntryAnim();\r\n\t}\r\n\t//读取相关数据\r\n\tprivate readData() {\r\n\t\tthis.gameList.bgImg = this.bgImg;\r\n\t\t//用于渠道包\r\n\t\tthis.tixianAnim.visible = !ToolsApp.channelOpen;\r\n\t\tthis.payAnim.visible = !ToolsApp.channelOpen;\r\n\t\tthis.tixianPic.visible = ToolsApp.channelOpen;\r\n\t\tthis.payPic.visible = ToolsApp.channelOpen;\r\n\t\tthis.btn_kefu.visible = !ToolsApp.channelOpen;\r\n\t\tthis.withdrawBtn.visible = !ToolsApp.channelOpen;\r\n\t\tthis.payBtn.visible = !ToolsApp.channelOpen;\r\n\t\tif (ToolsApp.channelOpen) {\r\n\t\t\tif (AppData.clientId == \"1147\") {\r\n\t\t\t\tthis.payBtn.visible = true;\r\n\t\t\t\tthis.btn_mail.visible = false;\r\n\t\t\t\tthis.btn_notice.visible = false;\r\n\t\t\t\tthis.cirbox.visible = false;\r\n\t\t\t\tthis.actBtn.visible = false;\r\n\t\t\t}\r\n\t\t}\r\n\r\n\t\tthis.bindBtn.visible = false;\r\n\t\tthis.mailHot.visible = false;\r\n\t\tthis.actHot.visible = false;\r\n\t\tthis.noticeHot.visible = false;\r\n\t\tthis.cirBtn.visible = false;\r\n\t\tthis.songjinbiBtn.visible = false;\r\n\r\n\t\tthis.checkUnreadNotice();\r\
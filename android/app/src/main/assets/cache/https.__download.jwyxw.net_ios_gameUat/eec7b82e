 += \"<span style='color:#FFFFFF;fontSize:30;'>-本次更新维护内容:</span><br />\";\r\n\t\t\t//Debug.log(data.content);\r\n\t\t\tvar content = data.content.replace(/\\n/g, \"<br />\");\r\n\t\t\t//Debug.log(content);\r\n\t\t\tstr += content;\r\n\t\t\tthis.htmlText.innerHTML = str;\r\n\t\t\tthis.htmlText.height = this.htmlText.contextHeight + 20;\r\n\t\t} catch (error) {\r\n\t\t\tthis.htmlText.innerHTML = error.toString();\r\n\t\t\tDebug.error(\"全局维护内容有不支持的格式\");\r\n\t\t}\r\n\t}\r\n\r\n\tpublic onClosed(type?: string): void {\r\n\t\tEventManager.removeAllEvents(this);\r\n\t\tsuper.onClosed(type);\r\n\t\tthis.destroy(true);\r\n\t}\r\n\r\n\t/**\r\n\t * 显示游戏更新公告\r\n\t * @param data 更新公告数据\r\n\t */\r\n\tpublic static show(data: any) {\r\n\t\tlet self = new GameUpdateNotice();\r\n\t\tself.SetData(data);\r\n\t\tself.popup(false, true);\r\n\t\tLobbyModel.maintenance = true;\r\n\t}\r\n}\r\n","import { Common } from '../common/Common';\r\nimport { ConfObjRead } from \"../common/ConfObjRead\";\r\nimport { CycelVo } from \"../common/CyclePageBox\";\r\nimport { Debug } from '../common/Debug';\r\nimport { PostMHelp } from '../common/PostMHelp';\r\nimport { QueueTask, QueueType } from '../common/QueueTask';\r\nimport { SaveManager } from '../common/SaveManager';\r\nimport { Tools } from '../common/Tools';\r\nimport { EventManager, EventType } from '../events/EventManager';\r\nimport { GameMgr } from \"../utils/GameMgr\";\r\nimport { GameUtils, InnerJumpUtil } from '../utils/GameUtils';\r\nimport { GameUpdateNotice } from '../view/dlg/GameUpdateNotice';\r\nimport { SystemDlg } from '../view/dlg/SystemDlg';\r\nimport { TipsDlg } from '../view/dlg/TipsDlg';\r\nimport { PageManager } from \"../view/PageManager\";\r\nimport { Toast } from '../view/toast/Toast';\r\nimport { DownLoadView } from './../view/DownLoadView';\r\nimport { ActiveType, DlgCmd, GameData, GameIconVo, GameMenuVo, IBackCardVo } from \"./GameVos\";\r\nimport { HttpRequester } from \"./HttpRequester\";\r\nimport { LoginMethod, Lo
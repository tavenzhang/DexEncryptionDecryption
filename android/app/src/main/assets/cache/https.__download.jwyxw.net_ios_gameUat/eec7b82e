st.filter(value => value.id == id);\r\n        return arr[0];\r\n    }\r\n\r\n    //默认的字体\r\n    public static normalFont: string = null;\r\n\r\n    //读取当前默认字体\r\n    public static getNormalFontByDevice(): void {\r\n        var dv = \"\";\r\n        //先读取当前的设备\r\n        if (Laya.Browser.onPC) {\r\n            dv = MyUid.KEY_P_PC;\r\n        } else if (Laya.Browser.onIOS) {\r\n            dv = MyUid.KEY_P_IOS;\r\n        } else if (Laya.Browser.onAndroid) {\r\n            dv = MyUid.KEY_P_ANDROID;\r\n        } else if (Laya.Browser.onMac) {\r\n            dv = MyUid.KEY_P_MAC;\r\n        } else {\r\n            Common.normalFont = \"xxx\";\r\n        }\r\n\r\n        var plen = ResConfig.platform_font.length;\r\n        for (var k = 0; k < plen; k++) {\r\n            var pf = ResConfig.platform_font[k];\r\n            var ps = pf.platform;\r\n\r\n            var pslen = ps.length;\r\n            for (var m = 0; m < pslen; m++) {\r\n                var pname = ps[m];\r\n                if (pname == dv) {\r\n                    Common.normalFont = pf.font;\r\n                    return;\r\n                }\r\n            }\r\n        }\r\n\r\n        Common.normalFont = \"xxx\";\r\n    }\r\n\r\n    //当前登录设备\r\n    public static LOGIN_PLATFORM: string = \"\";\r\n\r\n    public static getLoginPlatform(): string {\r\n        return Common.LOGIN_PLATFORM;\r\n    }\r\n\r\n    public static setLoginPlatform(s: string) {\r\n        Common.LOGIN_PLATFORM = s;\r\n    }\r\n    /**\r\n     * 重置相关数据\r\n     */\r\n    public static resetData() {\r\n        this.userInfo = null;\r\n        this.userInfo_current = null;\r\n        this.loginInfo = null;\r\n        this.avatorInfo = null;\r\n        this.cardInfo = null;\r\n        this.bankInfo = null;\r\n        this.alipayInfo = null;\r\n        this.balanceInfo = null;\r\n        this.userBalance = 0;\r\n        this.weChatData = null;\r\n        this.access_token = null;\r\n\r\n        GameData.bindVo = null;\r\n\r\n        Lob
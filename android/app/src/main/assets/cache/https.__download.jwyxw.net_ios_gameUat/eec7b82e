eight * scl;\r\n                this.frame.height = this.image.height + 44;\r\n            } else {\r\n                this.showImageLoading();\r\n                //加载图片\r\n                Laya.loader.load(url, Laya.Handler.create(this, () => {\r\n                    if (this.destroyed) return;  //如果已经被销毁了，则不执行后面逻辑\r\n\r\n                    //关闭图片加载遮罩\r\n                    this.hideImageLoading();\r\n\r\n                    this.image.skin = url;\r\n                    let tt = this.image.source;\r\n                    if (tt) {\r\n                        let scl = this.image.width / tt.width;\r\n                        this.image.skin = url;\r\n                        this.image.height = tt.height * scl;\r\n                        this.frame.height = this.image.height + 44;\r\n                    } else {\r\n                        Debug.error(\"图片异常:\" + url, tt);\r\n                    }\r\n                }), null, Laya.Loader.IMAGE);\r\n            }\r\n            //注册图片点击事件\r\n            if ($data.jumpHref != \"\") {\r\n                this.frame.mouseEnabled = true;\r\n                this.imgPanel.on(Laya.Event.MOUSE_UP, this, this.requestJump);\r\n            }\r\n        } else {\r\n            this.imgPanel.visible = false;\r\n            this.message.visible = true;\r\n\r\n            this.titleTxt.text = $data.title;\r\n            this.contentTxt.text = $data.content;\r\n            this.dateTxt.text = $data.author;\r\n        }\r\n    }\r\n\r\n    /**\r\n     * 打开图片加载遮罩\r\n     */\r\n    protected showImageLoading(): void {\r\n        if (!this.embedLoading) {\r\n            this.embedLoading = new EmbedLoadingView();\r\n        }\r\n        this.embedLoading.EmbedBox(this);\r\n    }\r\n\r\n    /**\r\n     * 关闭图片加载遮罩\r\n     */\r\n    protected hideImageLoading(): void {\r\n        if (this.embedLoading) {\r\n            this.embedLoading.destroy();\r\n            this.embedLoading = null;\r\n        }\r\n    }\r\n\r\n    /**\r\n     * 图片公告/活动跳转逻辑\r\n     */\r\n    protected requestJump(evt: Laya.Event): void {\r\n        if (!(evt.target instanceof Laya.Image)) {\r\n            return;\r\n        }\r\n\r\n        if (!this.data.jumpGame && !this.data.jumpInner) {\r\n            if (GameUtils.isNativeApp) PostMHelp.game_common({ name: \"openWeb\", param: this.data.jumpHref });\r\n            else window.open(this.data.jumpHref);\r\n        }\r\n        else if (this.data.jumpGame) {\r\n            EventManager.dispath(EventType.JUMP_GAME, this.data.jumpHref);\r\n            this.event(\"closeNoticeDlg\");\r\n        }\r\n        else {\r\n            let s: string = this.data.jumpHref.toString();\r\n            InnerJumpUtil.doJump(DlgCmd[s]);\r\n            this.event(\"closeNoticeDlg\");\r\n        }\r\n        this.event(\"jump\");\r\n    }\r\n\r\n    /**\r\n     * 销毁\r\n     */\r\n    public destroy() {\r\n        this.hideImageLoading();\r\n        //\r\n        super.destroy(true);\r\n    }\r\n}","import { Common } from \"../../common/Common\";\r\nimport { ConfObjRead } from \"../../common/ConfObjRead\";\r\nimport { Debug } from \"../../common/Debug\";\r\nimport { ActiveType, DlgCmd, NoticeData } from \"../../data/GameVos\";\r\nimport { HttpRequester } from \"../../data/HttpRequester\";\r\nimport { LobbyModel } from \"../../data/LobbyModel\";\r\nimport { EventManager } from \"../../events/EventManager\";\r\nimport { ui } from \"../../ui/layaMaxUI\";\r\nimport { GameMgr } from \"../../utils/GameMgr\";\r\nimport { ComponentTool, GameUtils } from \"../../utils/GameUtils\";\r\nimport { SoundPlayer } from \"../../utils/SoundPlayer\";\r\nimport { NoticeTabView } from \"../dlg/notice/NoticeTabView\";\r\nimport { TipsDlg } from \"../dlg/TipsDlg\";\r\nimport { Notice_Message } from \"./message/Notice_Message\";\r\n\r\n// const NoticeData = {\r\n// \tshareId: 0,\r\n// \tshareLimit: 0,\r\n// \tcurrentTab: 0,//0-公告，1-活动\r\n// \tnoticeid: -1,\r\n// \tnoticeType: null\r\n// }\r\n\r\n\r\n/**\r\n * 活动/公告\r\n */\r\nexp
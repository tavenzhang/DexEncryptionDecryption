e = MailState[vo.state];\r\n\r\n\t\tthis.selectPic.alpha = 0;\r\n\t\tthis.giftIcon.visible = false;\r\n\r\n\t\tswitch (this.state) {\r\n\t\t\tcase MailState.READ: {//已读(表示没有奖励的邮件)\r\n\t\t\t\tthis.stateIcon.skin = \"ui/mailbox/icon_yx_xtyj-yj-k.png\";\r\n\t\t\t\tthis.normGroup.alpha = 0.6;\r\n\t\t\t\tbreak;\r\n\t\t\t} case MailState.UNREAD: {//未读(表示没有奖励的邮件)\r\n\r\n\t\t\t\tbreak;\r\n\t\t\t} case MailState.READUNCLAIMED: {//已读未领取(有奖励的邮件)\r\n\t\t\t\tthis.giftIcon.visible = true;\r\n\t\t\t\tthis.stateIcon.skin = \"ui/mailbox/icon_yx_xtyj-yj-k.png\";\r\n\t\t\t\tthis.normGroup.alpha = 0.6;\r\n\t\t\t\tthis._isAward = true;\r\n\t\t\t\tbreak;\r\n\t\t\t} case MailState.RECEIVE: {//已领取(有奖励的邮件)\r\n\t\t\t\tthis.stateIcon.skin = \"ui/mailbox/icon_yx_xtyj-yj-k.png\";\r\n\t\t\t\tthis.normGroup.alpha = 0.6;\r\n\t\t\t\tbreak;\r\n\t\t\t} case MailState.UNCLAIMED: {//未读未领取(有奖励的邮件)\r\n\t\t\t\tthis.giftIcon.visible = true;\r\n\t\t\t\tthis._isAward = true;\r\n\t\t\t\tbreak;\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n}\r\n","import { BitmapFont } from \"../../common/BitmapFont\";\r\nimport { ConfObjRead } from '../../common/ConfObjRead';\r\nimport { MailItemVo, MailState } from \"../../data/GameVos\";\r\nimport { HttpRequester } from '../../data/HttpRequester';\r\nimport { LobbyModel } from '../../data/LobbyModel';\r\nimport { ResConfig } from '../../data/ResConfig';\r\nimport { EventManager } from '../../events/EventManager';\r\n/**Created by the LayaAirIDE*/\r\nimport { ui } from \"../../ui/layaMaxUI\";\r\nimport { GameMgr } from '../../utils/GameMgr';\r\nimport { GameUtils } from \"../../utils/GameUtils\";\r\nimport { SoundPlayer } from '../../utils/SoundPlayer';\r\nimport { ComponentTool } from './../../utils/GameUtils';\r\nimport { MailAwardDlg } from './mail/MailAwardDlg';\r\nimport { MailItemView } from \"./mail/MailItemView\";\r\n\r\n/**\r\n * 邮箱\r\n */\r\nexport class MailboxDlg extends ui.sceneUI.dlg.MailboxDlgUI {\r\n\tprivate mlistData: MailItemVo[];\r\n\tprivate bitFont: BitmapFont;\r\n\tpr
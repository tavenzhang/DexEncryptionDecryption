s.showCacheLogs();\r\n            });\r\n        }\r\n    }\r\n    /**\r\n     * 手动打开\r\n     */\r\n    public static handOpenDebug() {\r\n        if (isNative) return;\r\n        if (!this.inited) {\r\n            Debug.handOpen = true;\r\n            loadLib(\"libthird/vconsole.min.js\");\r\n            Laya.timer.once(800, this, () => {//如果不延迟执行，在h5版本更新后会导致黑屏\r\n                if (window[\"initVconsole\"]) window[\"initVconsole\"]();\r\n                this.inited = true;\r\n                this.showCacheLogs();\r\n            });\r\n        }\r\n    }\r\n    private static showCacheLogs() {\r\n        if (this.logCache) {\r\n            this.logCache.forEach(value => {\r\n                let mark = value.shift();\r\n                if (mark == \"log\") this.log(value);\r\n                else this.error(value);\r\n            });\r\n            this.logCache = null;\r\n        }\r\n    }\r\n    //日志开关(通过http获取的)\r\n    public static get httpDebug(): boolean {\r\n        return this._httDebug;\r\n    }\r\n    /**\r\n     * 普通输出\r\n     * @param mess \r\n     * @param parms \r\n     */\r\n    public static log(mess?: any, ...parms: any[]) {\r\n        if (isNative) Printer.output(mess, ...parms);//todo:sit,uat,线上判断\r\n        else {\r\n            if (!this.inited && this.isapp) {\r\n                if (this.logCache) this.logCache.push([\"log\", mess, ...parms]);\r\n                return;\r\n            }\r\n            console.log(mess, ...parms);\r\n        }\r\n    }\r\n    /**\r\n     * 异常打印\r\n     * @param mess \r\n     * @param parms \r\n     */\r\n    public static error(mess?: any, ...parms: any[]) {\r\n        if (isNative) Printer.output(mess, ...parms);//todo:sit,uat,线上判断\r\n        else {\r\n            if (!this.inited && this.isapp) {\r\n                if (this.logCache) this.logCache.push([\"error\", mess, ...parms]);\r\n                return;\r\n            }\r\n            console.error(mess, ...parms);\r\n        }\r\n    }\r\n    /**\r\n  
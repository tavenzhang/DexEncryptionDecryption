ack: Function) {\r\n        let url = ConfObjRead.getConfUrl().url.apihome;\r\n        if (isAuthor) {\r\n            url += ConfObjRead.getConfUrl().cmd.getAuthorVerCode;\r\n            url += \"?access_token=\" + Common.access_token;\r\n        }\r\n        else url += ConfObjRead.getConfUrl().cmd.getunAuthorVerCode;\r\n\r\n        let data = {\r\n            phoneNumber: phone,\r\n            smsMsgType: VerCodeType[type]\r\n        }\r\n        let jd = JSON.stringify(data);\r\n        let header = this.getEncryHeader();\r\n        this.doRequest(url, header, jd, caller, callback);\r\n    }\r\n\r\n    /**\r\n     * 修改密码(包括登录密码和取款密码)\r\n     * @param pwd \r\n     * @param newpwd \r\n     * @param isSetLoginPwd 是否修改登录密码(否则修改取款密码)\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static changePassword(pwd: string, newpwd: string, isSetLoginPwd: boolean, caller: any, callback: Function) {\r\n        let url = ConfObjRead.getConfUrl().url.apihome;\r\n        url += ConfObjRead.getConfUrl().cmd.changePassword;\r\n        url += \"?access_token=\" + Common.access_token;\r\n\r\n        let header = this.getEncryHeader();\r\n\r\n        let ePwd = window['SecretUtils'].rsaEncodePWD(pwd);\r\n        let eNpwd = window['SecretUtils'].rsaEncodePWD(newpwd);\r\n\r\n        let data = {\r\n            isWep: !GameUtils.isNativeApp,\r\n            mode: isSetLoginPwd ? \"PASSWORD\" : \"SECURITY_PASSWORD\",\r\n            oldPassword: ePwd,\r\n            newPassword: eNpwd\r\n        };\r\n\r\n        let jd = JSON.stringify(data);\r\n\r\n        this.doRequest(url, header, jd, caller, callback);\r\n    }\r\n    /**\r\n     * 通过手机短信验证修改密码(包括登录密码和取款密码)\r\n     * @param newpwd \r\n     * @param phone \r\n     * @param code \r\n     * @param setLoginPwd \r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static changePasswordWithPhone(newpwd: string, phone: string, code: string, setLoginPwd: boolean, caller: any, callback: Funct
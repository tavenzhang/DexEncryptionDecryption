\tthis.nianpanel.vScrollBar.stopScroll();\r\n\t\tthis.yuepanel.vScrollBar.stopScroll();\r\n\t\tthis.ripanel.vScrollBar.stopScroll();\r\n\t\tlet nianduo: number = this.nianpanel.vScrollBar.value % 64;\r\n\t\tif (nianduo != 0)\r\n\t\t\tthis.nianpanel.vScrollBar.value = nianduo > 32 ? this.nianpanel.vScrollBar.value + 64 - nianduo : this.nianpanel.vScrollBar.value - nianduo;\r\n\t\tlet yueduo: number = this.yuepanel.vScrollBar.value % 64;\r\n\t\tif (yueduo != 0)\r\n\t\t\tthis.yuepanel.vScrollBar.value = yueduo > 32 ? this.yuepanel.vScrollBar.value + 64 - yueduo : this.yuepanel.vScrollBar.value - yueduo;\r\n\t\tlet riduo: number = this.ripanel.vScrollBar.value % 64;\r\n\t\tif (riduo != 0)\r\n\t\t\tthis.ripanel.vScrollBar.value = riduo > 32 ? this.ripanel.vScrollBar.value + 64 - riduo : this.ripanel.vScrollBar.value - riduo;\r\n\r\n\t\tif (this.maxNianNum == 1)\r\n\t\t\tthis.nianpanel.vScrollBar.value = 64;\r\n\t\telse {\r\n\t\t\tif (this.nianpanel.vScrollBar.value < 0)\r\n\t\t\t\tthis.nianpanel.vScrollBar.value = 0;\r\n\t\t\telse if (this.nianpanel.vScrollBar.value > 64)\r\n\t\t\t\tthis.nianpanel.vScrollBar.value = 64;\r\n\t\t}\r\n\r\n\t\tlet yuevalue = this.yuepanel.vScrollBar.value;\r\n\t\tif (this.yuepanel.vScrollBar.value < 0)\r\n\t\t\tthis.yuepanel.vScrollBar.value = 0;\r\n\t\telse if (yuevalue >= 703) {\r\n\t\t\tthis.yuepanel.vScrollBar.value = 704;\r\n\t\t\tyuevalue = 704;\r\n\t\t}\r\n\t\tlet rivalue = this.ripanel.vScrollBar.value;\r\n\t\tif (this.ripanel.vScrollBar.value < 0)\r\n\t\t\tthis.ripanel.vScrollBar.value = 0;\r\n\t\telse if (rivalue >= (this.maxDayNum - 1) * 64 - 1) {\r\n\t\t\tthis.ripanel.vScrollBar.value = (this.maxDayNum - 1) * 64;\r\n\t\t\trivalue = (this.maxDayNum - 1) * 64;\r\n\t\t}\r\n\t\tlet shownian: number = this.nianpanel.vScrollBar.value == 0 ? this.miniNian : this.miniNian + 1;\r\n\t\tif (this.maxNianNum == 1)\r\n\t\t\tshownian = this.miniNian;\r\n\t\tlet showyue: number = Math.abs(yuevalue / 64) + 1;\r\n\t\tlet showri: number = Math.abs(rivalue / 64) + 1;\r\n\t\t//是否小于最小日期\r\n\t\tif (shownian == this.miniNian) {\r\n\t\t\tif (showyue < this.miniYue) {\r\n\t\t\t\tthis.showminiTime();\r\n\t\t\t\treturn;\r\n\t\t\t}\r\n\t\t\telse if (showyue == this.miniYue && showri < this.miniRi) {\r\n\t\t\t\tthis.showminiTime();\r\n\t\t\t\treturn;\r\n\t\t\t}\r\n\t\t}\r\n\t\t//是否大于今天\r\n\t\tlet nian: number = this.nowDate.getUTCFullYear();\r\n\t\tlet yue: number = this.nowDate.getUTCMonth() + 1;\r\n\t\tlet ri: number = this.nowDate.getUTCDate();\r\n\t\tif (shownian == nian) {\r\n\t\t\tif (showyue > yue) {\r\n\t\t\t\tthis.showMaxTime();\r\n\t\t\t\treturn;\r\n\t\t\t}\r\n\t\t\telse if (showyue == yue && showri > ri) {\r\n\t\t\t\tthis.showMaxTime();\r\n\t\t\t\treturn;\r\n\t\t\t}\r\n\t\t}\r\n\t\tif (this.curyue != showyue) {\r\n\t\t\tthis.showtimeri(this.curyue, showyue);\r\n\t\t\tthis.curyue = showyue;\r\n\t\t} else if (this.curri != showri) {\r\n\t\t\tthis.curri = showri;\r\n\t\t}\r\n\t\tthis.showtextColor();\r\n\t}\r\n\r\n\tprivate initDateBox(): void {\r\n\t\tthis.nianpanel.vScrollBarSkin = \"\";\r\n\t\tthis.yuepanel.vScrollBarSkin = \"\";\r\n\t\tthis.ripanel.vScrollBarSkin = \"\";\r\n\t\tlet nian: number = this.nowDate.getUTCFullYear();\r\n\t\tlet yue: number = this.nowDate.getUTCMonth() + 1;\r\n\t\tlet ri: number = this.nowDate.getUTCDate();\r\n\t\tthis.curnian = nian;\r\n\t\tthis.curyue = yue;\r\n\t\tthis.curri = ri;\r\n\t\tlet minidate: Date = new Date(this.nowDate.getTime() - 90 * 3600 * 24000);\r\n\t\tlet mininian: number = minidate.getUTCFullYear();\r\n\t\tthis.miniNian = mininian;\r\n\t\tthis.miniYue = minidate.getUTCMonth() + 1;\r\n\t\tthis.miniRi = minidate.getUTCDate();\r\n\t\tif (nian == mininian)\r\n\t\t\tthis.maxNianNum = 1;\r\n\t\telse\r\n\t\t\tthis.maxNianNum = 2;\r\n\t\tlet lab: Laya.Label;\r\n\t\tfor (let i = 0; i < 7; i++) {\r\n\t\t\tlab = this.getlabel();\r\n\t\t\tif (nian == mininian) {\r\n\t\t\t\tif (i != 3)\r\n\t\t\t\t\tlab.text = \"\";\r\n\t\t\t\telse\r\n\t\t\t\t\tlab.text = String(nian) + \" 年\
r\n        item.mouseEnabled = false;\r\n        this.index = this.group.indexOf(item);\r\n        let bl = this.prev ? this.prev.mouseEnabled : true;\r\n        if (this.caller && this.callback && bl) {\r\n            this.callback.apply(this.caller);\r\n        }\r\n        this.prev = item;\r\n    }\r\n    public set selectIndex(value: number) {\r\n        let cb = this.group[value];\r\n        cb.selected = true;\r\n    }\r\n    public get selectIndex(): number {\r\n        return this.index;\r\n    }\r\n    public setGrayIndex(value: number, gray: boolean) {\r\n        let cb = this.group[value];\r\n        cb.mouseEnabled = !gray;\r\n        cb.gray = gray;\r\n        if (gray) cb.selected = false;\r\n    }\r\n    public reset() {\r\n        this.group.forEach(value => {\r\n            value.selected = false;\r\n            value.gray = false;\r\n            value.mouseEnabled = true;\r\n        });\r\n        if (this.prev) {\r\n            this.prev = null;\r\n        }\r\n    }\r\n    public destory() {\r\n        this.group.forEach(value => {\r\n            value.off(Laya.Event.CHANGE, this, this.changeState);\r\n        });\r\n        this.group = null;\r\n    }\r\n}","\r\nimport { ui } from \"../../../ui/layaMaxUI\";\r\nimport { AchievementVo } from \"../../../data/AgentModel\";\r\n\r\n\r\n/**\r\n * 我的业绩item\r\n */\r\nexport class EnterpriseItem extends ui.sceneUI.dlg.agent.EnterpriseItemUI {\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t}\r\n\tpublic initData(data: AchievementVo, index: number) {\r\n\t\tif (index % 2 != 0) this.bgIcon.skin = \"ui/agent/img_daili_tiaodibian02.png\";\r\n\r\n\t\tthis.etxt1.text = data.username;\r\n\t\tthis.etxt2.text = data.subBet.toString();\r\n\t\tthis.etxt3.text = data.teamBet.toString();\r\n\t\tthis.etxt4.text = data.directCount.toString();\r\n\t\tthis.etxt5.text = data.teamCount.toString();\r\n\t\tthis.etxt6.text = data.brokerage.toString();\r\n\t}\r\n}\r\n","import { RadioBox } from \"../../../common/RadioBox\";\r\nimpor
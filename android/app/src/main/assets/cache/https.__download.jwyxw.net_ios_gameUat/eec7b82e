}\r\n\r\n/*\r\n* 龙骨动画播放器\r\n* 采用时时加载龙骨动画素材，不需要预加载\r\n*/\r\nexport class DragonBoneAnim extends Laya.Sprite {\r\n    private mFactory: Laya.Templet;\r\n    private mArmature: Laya.Skeleton;\r\n\r\n    private _vo: IAnimVo;\r\n    private caller: any;\r\n    private callback: Function;\r\n\r\n    constructor() {\r\n        super();\r\n\r\n        this.mFactory = new Laya.Templet();\r\n        this.mFactory.on(Laya.Event.COMPLETE, this, this.parseComplete);\r\n        this.mFactory.on(Laya.Event.ERROR, this, this.onError);\r\n    }\r\n    /**\r\n     * 动态加载模式初始化(如果动画的显示不需要那么及时，建议使用这种方式)\r\n     * @param vo \r\n     */\r\n    public loadInit(vo: IAnimVo, caller?: any, callback?: Function) {\r\n        this.caller = caller;\r\n        this.callback = callback;\r\n        if (vo.playRate == undefined) vo.playRate = 1;\r\n        if (vo.autoPlay == undefined) vo.autoPlay = true;\r\n        if (vo.loop == undefined) vo.loop = true;\r\n        if (vo.loopDelay == undefined) vo.loopDelay = 0;\r\n        this._vo = vo;\r\n\r\n        this.mFactory.loadAni(vo.skUrl);\r\n    }\r\n    /**\r\n     * 解析模式初始化(需要预加载sk和png文件)\r\n     * @param vo \r\n     */\r\n    public parseInit(vo: IAnimVo, caller?: any, callback?: Function) {\r\n        this.caller = caller;\r\n        this.callback = callback;\r\n        if (vo.playRate == undefined) vo.playRate = 1;\r\n        if (vo.autoPlay == undefined) vo.autoPlay = true;\r\n        if (vo.loop == undefined) vo.loop = true;\r\n        if (vo.loopDelay == undefined) vo.loopDelay = 0;\r\n        if (vo.pngUrl == undefined || vo.pngUrl == \"\") {\r\n            Debug.error(\"请设置动画png路径\");\r\n            return;\r\n        }\r\n        this._vo = vo;\r\n\r\n        let text: Laya.Texture = Laya.Loader.getRes(vo.pngUrl);\r\n        let arrbuffer: ArrayBuffer = Laya.Loader.getRes(vo.skUrl);\r\n        if (!text || !arrbuffer) {\r\n            Debug.error(\"动画素材需要预加载：\", vo.pngUrl, vo.skUrl);\r\n            return;\r\n        }\r\n        this.mFactory.parseData(text, arrbuffer, 30);\r\n    }\r\n    /**\r\n     * 开始播放\r\n     */\r\n    public play() {\r\n        if (this.mArmature) {\r\n            this.mArmature.play(0, false);\r\n        }\r\n    }\r\n    /**\r\n     * 停止播放(停止后不能再恢复)\r\n     */\r\n    public stop() {\r\n        if (this.mArmature) {\r\n            this.mArmature.stop();\r\n        }\r\n    }\r\n    /**\r\n     * 暂停播放\r\n     */\r\n    public pause() {\r\n        if (this.mArmature) {\r\n            this.mArmature.paused();\r\n        }\r\n    }\r\n    /**\r\n     * 恢复播放\r\n     */\r\n    public resume() {\r\n        if (this.mArmature) {\r\n            this.mArmature.resume();\r\n        }\r\n    }\r\n    private parseComplete() {\r\n        this.mArmature = this.mFactory.buildArmature(0);\r\n        this.mArmature.playbackRate(this._vo.playRate);\r\n        this.addChild(this.mArmature);\r\n        this.mArmature.on(Laya.Event.STOPPED, this, this.playEnd);\r\n        this.play();\r\n        if (!this._vo.autoPlay) this.pause();\r\n        if (this.caller && this.callback) this.callback.call(this.caller);\r\n    }\r\n    private playEnd() {\r\n        if (this._vo.loop) {\r\n            if (this._vo.loopDelay > 0) Laya.timer.once(this._vo.loopDelay, this, this.play);\r\n            else this.play();\r\n        }\r\n    }\r\n    private onError(e: Laya.Event) {\r\n        Debug.error(\"parse-anim-err:\", e);\r\n    }\r\n    /**\r\n     * 销毁\r\n     * @param dc \r\n     */\r\n    public destroy(dc?: boolean) {\r\n        this.stop();\r\n        if (this.mFactory) {\r\n            this.mFactory.off(Laya.Event.COMPLETE, this, this.parseComplete);\r\n            this.mFactory.off(Laya.Event.ERROR, this, this.onError);\r\n            if (dc) this.mFactory.destroy();\r\n            this.mFactory = null;\r\n        }\r\n\r\n        if (this.mArmature) {\r\n            this.mArmature.off(Laya.Event.STOPPED, this, this.playEnd);\r\n            this.mArmature.destroy(dc);\r\n            this.mArmature = null;\r\n        }\r\n        super.destroy(dc);\r\n    }\r\n}","/**Created by the LayaAirIDE*/\r\nimport { DragonBoneAnim } from \"../common/DragonBoneAnim\";\r\nimport { ui } from \"../ui/layaMaxUI\";\r\nimport { GameUtils } from \"../utils/GameUtils\";\r\nimport { Debug } from './../common/Debug';\r\n\r\n/**\r\n * loading\r\n */\r\nexport class LoadingView extends ui.sceneUI.LoadingViewUI {\r\n\tprivate static _inst: LoadingView;\r\n\tprivate onShowAnim: boolean;\r\n\tprivate anim: DragonBoneAnim;\r\n\tpublic static isLoading: boolean;//标记是否显示了loading\r\n\r\n\tconstructor(onShowAnim: boolean) {\r\n\t\tsuper();\r\n\t\tthis.onShowAnim = onShowAnim;\r\n\t\tthis.initView();\r\n\t}\r\n\t/**\r\n\t * 显示loading\r\n\t * @param onShowAnim 是否立刻显示loading动画(默认延迟显示)\r\n\t */\r\n\tpublic static show(onShowAnim?: boolean) {\r\n\t\tif (this._inst) return;\r\n\t\tthis._inst = new LoadingView(onShowAnim);\r\n\t\tthis._inst.zOrder = Laya.Dialog.manager.zOrder + 1;\r\n\t\tthis._inst.mouseEnabled = true;\r\n\t\tLaya.stage.addChild(this._inst);\r\n\t\tthis.isLoading = true;\r\n\t}\r\n\r\n\t/**\r\n\t * 隐藏loading\r\n\t */\r\n\tpublic static hide() {\r\n\t\tif (this._inst) {\r\n\t\t\tthis._inst.destroy(true);\r\n\t\t\tthis._inst = null;\r\n\t\t}\r\n\t\tthis.isLoading = false;\r\n\t}\r\n\tprivate initView() {\r\n\t\tthis.width = Laya.stage.width;\r\n\t\tthis.height = Laya.stage.height;\r\n\t\t//背景框\r\n\t\tlet bg = new Laya.Sprite();\r\n\t\tbg.graphics.drawRect(0, 0, Laya.stage.width, Laya.stage.height, \"#000000\");\r\n\t\tbg.alpha = 0;//this._alp;需求将遮罩背景去掉:https://jira.mtkefu.com/browse/TGQP-2782?filter=-1\r\n\t\tthis.addChildAt(bg, 0);\r\n\r\n\t\tthis.anim = new DragonBoneAnim();\r\n\t\tthis.anim.parseInit({ skUrl: \"./assets/animation/loading/xiaoLoding.sk\", pngUrl: \"./assets/animation/loading/xiaoLoding.png\" });\r\n\t\tthis.abox.addChild(this.anim);\r\n\t\tif (!this.onShowAnim) {\r\n\t\t\tthis.anim.alpha = 0;//用于优化loading显示\r\n\t\t\tLaya.Tween.to(this.anim, { alpha: 1 }, 300, nu
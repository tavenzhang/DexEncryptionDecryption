uester.getHttpData(cmd, this, this.getCurBack);\r\n\t\t\t}\r\n\t\t});\r\n\t\t//删除当前\r\n\t\tEventManager.addTouchScaleListener(this.delCurBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tif (this.curItem) {\r\n\t\t\t\tGameMgr.showCircleLoading(true);\r\n\t\t\t\tlet cmd = ConfObjRead.getConfUrl().cmd.delMail + this.curItem.mailId;\r\n\t\t\t\tHttpRequester.deleteHttpData(cmd, this, this.delCurBack);\r\n\t\t\t}\r\n\t\t});\r\n\t}\r\n\t//请求邮件列表\r\n\tprivate reqMailList() {\r\n\t\tGameMgr.showCircleLoading(true);\r\n\t\tHttpRequester.getHttpData(ConfObjRead.getConfUrl().cmd.getMailList, this, this.responseMailList);\r\n\t}\r\n\t//一键领取数据返回\r\n\tprivate getAllBack(suc: boolean, jobj: any) {\r\n\t\tGameMgr.showCircleLoading(false);\r\n\t\tif (suc) {\r\n\t\t\tlet money = jobj.amount || 0;\r\n\t\t\tif (money > 0) MailAwardDlg.show(money);\r\n\t\t\tthis.awardCount = 0;\r\n\t\t\tthis.updateGetAndDelBtn();\r\n\t\t\tthis.reqMailList();\r\n\t\t}\r\n\t}\r\n\t//一键删除数据返回\r\n\tprivate delAllBack(suc: boolean, jobj: any) {\r\n\t\tGameMgr.showCircleLoading(false);\r\n\t\tif (suc) {\r\n\t\t\tthis.resetList();\r\n\t\t\tthis.hintGroup.visible = true;\r\n\t\t}\r\n\t}\r\n\t//领取当前邮件返回数据\r\n\tprivate getCurBack(suc: boolean, jobj: any) {\r\n\t\tGameMgr.showCircleLoading(false);\r\n\t\tif (suc) {\r\n\t\t\tlet money = jobj.amount || 0;\r\n\t\t\tif (money > 0) MailAwardDlg.show(this.curDetailsVo.amount);\r\n\t\t\tif (this.curItem) this.curItem.markReceived();\r\n\t\t\tthis.awardCount--;\r\n\t\t\tthis.updateGetAndDelBtn();\r\n\t\t\t//刷新显示\r\n\t\t\tif (this.curDetailsVo) this.reqDetail(this.curDetailsVo.mailId);\r\n\t\t}\r\n\t}\r\n\t//删除当前邮件返回数据\r\n\tprivate delCurBack(suc: boolean, jobj: any) {\r\n\t\tif (suc) {\r\n\t\t\tlet dex = this.itemArr.indexOf(this.curItem);\r\n\t\t\tthis.curItem.removeSelf();\r\n\t\t\tthis.curItem = null;\r\n\t\t\tthis.itemArr.forEach((value, id) => {\r\n\t\t\t\tif (id > dex) Laya.Tween.to(value, { y: (id - 1) * (value.height + 8) }, 300);\r\n\t\t\t});\r\n\t\t\tthis.itemArr.splice(dex, 1);\r\n\t\t\tLaya.timer.once(320, this.itemList, this.itemList.refresh);\r\n\t\t\tif (this.itemArr.length > 0) {//删除后默认第一条为选中状态\r\n\t\t\t\tlet def = this.itemArr[0];\r\n\t\t\t\tthis.selectItem(def);\r\n\t\t\t} else {\r\n\t\t\t\tthis.hintGroup.visible = true;\r\n\t\t\t\tthis.resetList();\r\n\t\t\t}\r\n\t\t}\r\n\t\tGameMgr.showCircleLoading(false);\r\n\t}\r\n\t//邮件列表数据返回\r\n\tprivate responseMailList(suc: boolean, jobj: any) {\r\n\t\tGameMgr.showCircleLoading(false);\r\n\t\tif (suc) {\r\n\t\t\tthis.mlistData = jobj.datas;\r\n\t\t\tlet total: number = jobj.totalCount;\r\n\t\t\tif (total > 0) {\r\n\t\t\t\tthis.addMailList();\r\n\t\t\t\tthis.hintGroup.visible = false;\r\n\t\t\t} else {\r\n\t\t\t\tthis.hintGroup.visible = true;\r\n\t\t\t\tthis.resetList();\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n\t//添加邮件列表\r\n\tprivate addMailList() {\r\n\t\tthis.resetList();\r\n\r\n\t\tlet item: MailItemView;\r\n\t\tlet lw: number = this.itemList.width;\r\n\t\tthis.mlistData.forEach((value, index) => {\r\n\t\t\titem = new MailItemView();\r\n\t\t\titem.readData(value);\r\n\t\t\titem.x = lw - item.width >> 1;\r\n\t\t\titem.y = index * (item.height + 8);\r\n\t\t\tthis.itemList.addChild(item);\r\n\t\t\tthis.itemArr.push(item);\r\n\t\t\tif (index == 0) {//默认选中第一条\r\n\t\t\t\tthis.selectItem(item);\r\n\t\t\t}\r\n\t\t\tif (item.award) {\r\n\t\t\t\tthis.awardCount++;\r\n\t\t\t}\r\n\t\t});\r\n\t\tthis.updateGetAndDelBtn();\r\n\t\tthis.itemList.on(Laya.Event.CLICK, this, this.itemHandler);\r\n\t}\r\n\tprivate updateGetAndDelBtn() {\r\n\t\tthis.getAllBtn.visible = Boolean(this.awardCount > 0);\r\n\t\tthis.delAllBtn.visible = Boolean(this.awardCount <= 0);\r\n\t}\r\n\t//重置邮件列表\r\n\tprivate resetList() {\r\n\t\tthis.awardCount = 0;\r\n\t\tif (this.itemList.numChildren > 0) this.itemList.removeChildren();\r\n\t\tthis.itemArr.length = 0;\r\n\t\tthis.curItem = null;\r\n\t\tthis.curDetailsVo = null;\r\n\t\tthis.detailsGroup.visible = false;\r\n\t\tthis.getAllBtn.visible = false;\r\n\t\tthis.delAllBtn.visible = false;\r\n\t}\r\n\t//点击邮件\r\n\tprivate itemHandler(e: Laya.Event) {\r\n\t\tif (e.target instanceof MailItemView) {\r\n\t\t\tSoundPlayer.enterPanelSound();\r\n\t\t\tlet cur = e.target as MailItemView;\r\n\t\t\tthis.selectItem(cur);\r\n\t\t}\r\n\t}\r\n\tprivate selectItem(cur: MailItemView) {\r\n\t\tif (!cur.selected) {\r\n\t\t\tthis.reqDetail(cur.mailId);\r\n\t\t\tcur.selected = true;\r\n\t\t\tif (this.curItem) {\r\n\t\t\t\tthis.curItem.selected = false;\r\n\t\t\t}\r\n\t\t\tthis.curItem = cur;\r\n\t\t}\r\n\t}\r\n\t//请求邮件详情\r\n\tprivate reqDetail(id: number) {\r\n\t\tlet cmd: string = ConfObjRead.getConfUrl().cmd.getMailDetail + id;\r\n\t\tGameMgr.showCircleLoading(true);\r\n\t\tHttpRequester.getHttpData(cmd, this, (suc: boolean, jobj: any) => {\r\n\t\t\tGameMgr.showCircleLoading(false);\r\n\t\t\tif (suc) {\r\n\t\t\t\tthis.showDetails(jobj);\r\n\t\t\t}\r\n\t\t});\r\n\t}\r\n\t//显示详情\r\n\tprivate showDetails(vo: MailItemVo) {\r\n\t\tif (this.curItem) this.curItem.markRead();\r\n\r\n\t\tthis.curDetailsVo = vo;\r\n\t\tthis.ctitleTxt.text = vo.title;\r\n\t\tthis.contentTxt.text = vo.content || \"\";\r\n\r\n\t\tlet state: MailState = MailState[vo.state];\r\n\t\tlet showAward: boolean;//是否显示奖励相关\r\n\t\tif (vo.amount && vo.amount > 0) {\r\n\t\t\tif (state == MailState.UNCLAIMED || state == MailState.READUNCLAIMED) {\r\n\t\t\t\tshowAward = true;\r\n\t\t\t}\r\n\t\t}\r\n\r\n\t\tthis.awardGroup.visible = showAward;\r\n\t\tthis.getCurBtn.visible = showAward;\r\n\t\tthis.delCurBtn.visible = !showAward;\r\n\r\n\t\tif (showAward) {\r\n\t\t\tlet scl: number = 0.42;//字体需要缩放处理\r\n\t\t\tif (!this.bitFont) {\r\n\t\t\t\tthis.bitFont = new BitmapFont(ResConfig.bitFont_mail);\r\n\t\t\t\tthis.bitFont.scale(scl, scl);\r\n\t\t\t\tthis.bitFont.text = \"0\";\r\n\t\t\t\tthis.goldBox.addChild(this.bitFont);\r\n\t\t\t\tthis.bitFont.y = this.goldBox.height - this.bitFont.height * scl >> 1;\r\n\t\t\t}\r\n\t\t\tthis.bitFont.text = \"x\" + vo.amount;\r\n\t\t\tthis.bitFont.x = this.goldBox.width - this.bitFont.width * scl >> 1;\r\n\t\t\tthis.ctxtList.height = 210;\r\n\t\t} else {\r\n\t\t\tthis.ctxtList.height = 376;\r\n\t\t}\r\n\r\n\t\tthis.detailsGroup.visible = true;\r\n\t}\r\n\tonClosed(type?: string) {\r\n\t\tthis.itemList.off(Laya.Event.CLICK, this, this.itemHandler);\r\n\t\tEventManager.removeAllEvents(this);\r\n\t\tif (this.bitFont) this.bitFont.destroy();\r\n\t\tthis.itemArr = null;\r\n\t\tthis.mlistData = null;\r\n\t\tsuper.onClosed(type);\r\n\t\tthis.destroy(true);\r\n\t\t// PageManager.clearDlgRes(DlgCmd.email);\r\n\t}\r\n}\r\n","import { HttpRequester } from \"./HttpRequester\";\r\nimport { ConfObjRead } from \"../common/ConfObjRead\";\r\nimport { GameMgr } from \"../utils/GameMgr\";\r\n\r\n//vip详情数据\r\nexport interface VipInfoVo {\r\n    openState: boolean;//业主vip开关\r\n    currentLevel?: any;//当前vip等级数据\r\n    nextLevel?: any;//下一级vip等级数据\r\n    vipLevelUserVo?: any;//用户当前vip相关数据\r\n    vipRewardItemVoList?: VipRewardVo[];//用户当前vip奖励列表\r\n}\r\n//等级列表信息\r\nexport interface VipLevelVo {\r\n    name: string;//vip名称\r\n    id?: number;\r\n    betValid: number;//有效打码量(累计有效投注) \r\n    recharge: number;//充值金额(累计充值)\r\n    amountGive: number;//普级礼金\r\n    weekGive: number;//周礼金\r\n    monthGive: number;//月礼金\r\n    weekGiveBetCondition?: number;//周礼金领取投注条件\r\n    weekGiveRechargeCondition?: number;//周礼金领取充值条件\r\n    monthGiveBetCondition?: number;//月礼金领取投注条件\r\n    monthGiveRechargeCondition?: number;//月礼金领取充值条件\r\n}\r\n//可领取奖励\r\nexport interface VipRewardVo {\r\n    ableToGet: number;// 领取状态 1 可领取 2 已领取 3 未达条件 4 领取条件 ,\r\n    bet: number;// 当前打码数,\r\n    betCondition: number;// 要求打码数,\r\n    id: number;// ID，唯一标识,\r\n    name: string;// VIP名称,\r\n    recharge: number;// 当前充值数,\r\n    rechargeCondition: number;// 要求充值数,\r\n    rewardCount: number;// 礼金数量,\r\n    type: number;// 礼金类型-- 遵照RewardType枚举值 1晋级2周俸3月俸\r\n}\r\n/*\r\n* 个人中心相关数据\r\n* 备注：针对当前登录用户，相关数据只获取一次，而不需要时时获取\r\n*/\r\nexport class PersonalModel {\r\n    public static vipData: VipInfoVo;\r\n    public static vipLevels: VipLevelVo[];\r\n    /**\r\n     * 获取vip数据\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static getVipData(caller?: any, callback?: Function) {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.vipData, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                this.vipData = jobj;\r\n            }\r\n            if (!this.vipData) this.vipData = { openState: false };\r\n            if (caller && callback) callback.call(caller);\r\n        });\r\n    }\r\n    /**\r\n     * 获取所有vip等级信息\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static getVipLevels(caller?: any, callback?: Function) {\r\n        if (this.vipLevels) {\r\n            if (caller && callback) callback.call(caller);\r\n            return;\r\n        }\r\n\r\n        GameMgr.showCircleLoading(true);\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.vipLevels, this, (suc: boolean, jobj: any) => {\r\n            GameMgr.showCircleLoading(false);\r\n            if (suc) {\r\n                this.vipLevels = jobj;\r\n            }\r\n            if (caller && callback) callback.call(caller);\r\n        });\r\n    }\r\n    /**\r\n     * 当前登录用户领取单个等级奖励\r\n     * @param levelId 等级奖励id \r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static getVIPAward(levelId: number, caller?: any, callback?: Function): void {\r\n        GameMgr.showCircleLoading(true);\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.vipaward, this, (suc: boolean, jobj: any) => {\r\n            GameMgr.showCircleLoading(false);\r\n            if (caller && callback) callback.call(caller, suc, jobj);\r\n        }, \"&unreceivedLevelId=\" + levelId);\r\n    }\r\n    /**\r\n     * 当前登录用户领取俸禄奖励\r\n     * @param type 2周俸,3月俸\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static getVIPSalary(type: number, caller?: any, callback?: Function): void {\r\n        GameMgr.showCircleLoading(true);\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.vipsalary, this, (suc: boolean, jobj: any) => {\r\n            GameMgr.showCircleLoading(false);\r\n            if (caller && callback) callback.call(caller, suc, jobj);\r\n        }, \"&type=\" + type);\r\n    }\r\n    public static getAccountDetails( data,caller?: any, callback?: Function): void {\r\n        GameMgr.showCircleLoading(true);\r\n        HttpRequester.postHttpData(ConfObjRead.httpCmd.accountDetails, data,this, (suc: boolean, jobj: any) => {\r\n            GameMgr.showCircleLoading(false);\r\n            if (caller && callback) callback.call(caller, suc, jobj);\r\n        });\r\n    }\r\n    //切换账号时需要清理的数据\r\n    public static clear() {\r\n        this.vipData = null;\r\n        this.vipLevels = null;\r\n    }\r\n}","import { ResConfig } from \"../../data/ResConfig\";\r\n\r\n/**\r\n * 头像icon\r\n */\r\nexport class HeadIconUI extends Laya.Sprite {\r\n    private iconHead: Laya.Image;\r\n    private iconSelecte: Laya.Image;//选中框\r\n    public iconID: string;\r\n\r\n    constructor(id: string) {\r\n        super();\r\n        this.iconID = id;\r\n        this.mouseEnabled = true;\r\n        this.initView();\r\n    }\r\n    private initView() {\r\n        let iw = 100;\r\n        let ih = 100;\r\n        this.iconHead = new Laya.Image();\r\n        this.iconHead.skin = ResConfig.getHeadSkinByID(this.iconID);\r\n        this.addChildAt(this.iconHead, 0);\r\n        this.iconHead.size(iw, ih);\r\n        this.iconHead.pos(10, 10);\r\n\r\n        this.iconSelecte = new Laya.Image();\r\n        this.iconSelecte.skin = \"ui/fullMyCenter/img_grzx_gx01.png\";\r\n        this.addChild(this.iconSelecte);\r\n        this.iconSelecte.pos(iw - this.iconSelecte.width, ih - this.iconSelecte.height);\r\n        this.iconSelecte.visible = false;\r\n    }\r\n    public selecteIcon(bl: boolean) {\r\n        this.iconSelecte.visible = bl;\r\n    }\r\n    public get numID(): number {\r\n        if (this.iconID.indexOf(\"0\") == 0) {\r\n            return Number(this.iconID.substr(1, 1));\r\n        }\r\n        return Number(this.iconID);\r\n    }\r\n}\r\n","/**Created by the LayaAirIDE*/\r\n\r\nimport { Common } from \"../../../common/Common\";\r\nimport { ConfObjRead } from \"../../../common/ConfObjRead\";\r\nimport { Debug } from \"../../../common/Debug\";\r\nimport { SaveManager } from \"../../../common/SaveManager\";\r\nimport { HttpRequester } from \"../../../data/HttpRequester\";\r\nimport { ResConfig } from \"../../../data/ResConfig\";\r\nimport { EventManager, EventType } from \"../../../events/EventManager\";\r\nimport { ui } from \"../../../ui/layaMaxUI\";\r\nimport { GameMgr } from \"../../../utils/GameMgr\";\r\nimport { ComponentTool } from \"../../../utils/GameUtils\";\r\nimport { SoundPlayer } from \"../../../utils/SoundPlayer\";\r\nimport { HeadIconUI } from \"../../comp/HeadIconUI\";\r\n\r\n/**\r\n * 修改头像\r\n */\r\nexport class SetHeadDlg extends ui.sceneUI.dlg.center.SetHeadDlgUI {\r\n\t//头像图标总数\r\n\tprivate readonly totalIcon: number = 12;\r\n\tprivate curIconID: string = \"01\";\r\n\tprivate chooseIcon: HeadIconUI;\r\n\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t\tthis.initView();\r\n\t}\r\n\t/**\r\n\t * 入口\r\n\t */\r\n\tpublic static show() {\r\n\t\tlet dlg = new SetHeadDlg();\r\n\t\tdlg.popup(false, true);\r\n\t}\r\n\tprivate initView() {\r\n\t\tthis.changeBtn.visible = false;\r\n\t\tComponentTool.setPanelSlide(this.itemPanel);\r\n\r\n\t\tthis.setHeadIcon();\r\n\t\tthis.creatHeadList();\r\n\r\n\t\t//关闭\r\n\t\tEventManager.addTouchScaleListener(this.closeBtn, this, () => {\r\n\t\t\tSoundPlayer.closeSound();\r\n\t\t\tthis.close(null);\r\n\t\t});\r\n\t\t//更换头像\r\n\t\tEventManager.addTouchScaleListener(this.changeBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tthis.requestHeadSave();\r\n\t\t});\r\n\t\tthis.itemPanel.on(Laya.Event.MOUSE_UP, this, this.itemHandler);\r\n\t}\r\n\tprivate itemHandler(evt: 
\r\n            if (caller) {\r\n                obj.caller = caller;\r\n                obj.callback = callback;\r\n            }\r\n            return;\r\n        }\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.gamelist, this, (suc: boolean, jobj: any) => {\r\n            if (showloading) GameMgr.showCircleLoading(false);\r\n            if (suc) {\r\n                Debug.log(\"--->>>gameList-\" + vo.gameId + \":\", jobj);//classify==1 classifySort==2\r\n                this.classifyPool[vo.gameId] = jobj.datas;\r\n            }\r\n            let req: any = this.gameReqList[vo.gameId];\r\n            if (caller && callback) callback.call(caller, suc, jobj);\r\n            else {\r\n                if (req.caller && req.callback) {\r\n                    req.callback.call(req.caller, suc, jobj);\r\n                    Debug.log(\"重复请求回调:\", vo.gameId, req);\r\n                }\r\n            }\r\n            this.gameReqList[vo.gameId] = null;\r\n            req = null;\r\n            if (this.defMenu && vo.gameId == this.defMenu.gameId) {//如果默认菜单对应的游戏图标数据已经加载完毕\r\n                if (PageManager.curInLogin) GameMgr.initLobby();\r\n            }\r\n        }, params);\r\n\r\n        this.gameReqList[vo.gameId] = { caller: caller, callback: callback };\r\n    }\r\n    /**\r\n     * 请求游戏分类菜单\r\n     * @param caller \r\n     * @param callback \r\n     * @param showloading 是否显示Loading\r\n     * @param isflush 是否为刷新菜单\r\n     */\r\n    public static reqClassifyMenu(caller?: any, callback?: Function, showloading?: boolean, isflush: boolean = false) {\r\n        if (showloading) GameMgr.showCircleLoading();\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.gameMenu, this, (suc: boolean, jobj: any) => {\r\n            if (showloading) GameMgr.showCircleLoading(false);\r\n            if (suc) {\r\n                this.menuData = jobj.data;\r\n                this.defMenu = this.menuData[0];\r\n                LobbyModel.reqGameList(this.defMenu);\r\n                if (!isflush) {\r\n                    this.menuData.forEach((vo, index) => {//预加载游戏列表数据\r\n                        if (index > 0) LobbyModel.reqGameList(vo);\r\n                    });\r\n                }\r\n                Debug.log(\"--->>>gameMenu:\", this.menuData);\r\n                if (caller && callback) callback.call(caller);\r\n            } else {\r\n                this.errorMenuIndex++;\r\n                if (this.errorMenuIndex <= 3) {//尝试3次请求\r\n                    Laya.timer.once(1000, this, this.reqClassifyMenu, [caller, callback]);\r\n                } else {\r\n                    Toast.showToast(\"数据多次请求异常,请稍后重试\");\r\n                    if (caller && callback) callback.call(caller);\r\n                    if (PageManager.curInLogin) GameMgr.initLobby();\r\n                }\r\n            }\r\n        });\r\n    }\r\n    /**\r\n     * 获取cdnUrl,用于三方平台icon加载\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static reqCdnUrl(caller?: any, callback?: Function) {\r\n        if (this.cdnUrl && this.cdnUrl.indexOf(\"http\") != -1) {\r\n            Debug.log(\"cdnurl:>>>\", this.cdnUrl);\r\n            if (caller && callback) callback.call(caller);\r\n            return;\r\n        }\r\n        GameMgr.showCircleLoading();\r\n        if (!AppData.brandID) {\r\n            Debug.error(\"brandId异常\");\r\n        }\r\n        let url = ConfObjRead.httpCmd.cdnUrlByBrandID + AppData.brandID;\r\n        HttpRequester.getHttpData(url, this, (suc: boolean, jobj: any, hr: any) => {\r\n            GameMgr.showCircleLoading(false);\r\n            if (suc && jobj) {\r\n                if (jobj.http) this.cdnUrl = jobj.http.response;\r\n                else {\r\n                    try {\r\n                        this.cdnUrl = hr.http.response;\r\n                    } catch (e) { }\r\n                }\r\n                Debug.log(\"cdn:>>>\", this.cdnUrl);\r\n            }\r\n            if (caller && callback) callback.call(caller, suc);\r\n        });\r\n    }\r\n    /**\r\n     * 跳转三方游戏\r\n     * @param vo \r\n     */\r\n    public static gotoOhterGame(vo: GameIconVo) {\r\n        if (!LobbyModel.useSingleDownload) GameMgr.showCircleLoading(true, true);\r\n        let data = [\"gamePlatform\", vo.gamePlatform];\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.otherGameURI + vo.classifyThirdGameId + \"/\" + vo.id + \"/\", this, (suc: boolean, jobj: any) => {\r\n            if (!GameUtils.isNativeApp) GameMgr.showCircleLoading(false);\r\n            if (suc) {\r\n                if (jobj.data && jobj.data.gameUrl) {\r\n                    PostMHelp.jumpOtherGame(jobj.data.gameUrl, vo);\r\n                    Debug.log(\"jumpOtherGame:\", jobj.data.gameUrl);\r\n                } else {\r\n                    if (jobj.code == 2407) {//充值限制\r\n                        SystemDlg.show(jobj.message,\r\n                            {\r\n                                caller: this,\r\n                                rightBtnCallback: () => {\r\n                                    InnerJumpUtil.doJump(DlgCmd.recharge);\r\n                                }\r\n                            }\r\n                        );\r\n                    } else {\r\n                        Toast.showToast(\"游戏跳转失败:\" + jobj.message);\r\n                    }\r\n                    DownLoadView.hide();\r\n                    GameMgr.showCircleLoading(false);\r\n                }\r\n            } else {\r\n                DownLoadView.hide();\r\n                GameMgr.showCircleLoading(false);\r\n            }\r\n        }, null, data);\r\n    }\r\n    /**\r\n     * 进入游戏前先检测有效余额与实际余额\r\n     * @param vo \r\n     * @param selfGame 是否自有游戏 \r\n     */\r\n    public static checkValidBalance(vo: GameIconVo, selfGame: boolean) {\r\n        //显示Loading\r\n        if (LobbyModel.useSingleDownload) DownLoadView.show(false);\r\n        let obj = Common.balanceInfo;\r\n        if (obj && obj.failedCollectAmount > 0) {//判断是否有三方游戏的余额未回收\r\n            let tipstr = \"由于您在之前游戏中异常退出，部分余额已锁定，锁定余额将在游戏结束后返还\";\r\n            if (obj.failedCollectAmount == Common.userBalance) tipstr = \"由于您在之前游戏中异常退出，余额已被锁定，锁定余额将在游戏结束后返还\";\r\n            TipsDlg.show(tipstr, this, () => {\r\n                selfGame ? this.checkInnerAccess(vo) : LobbyModel.gotoOhterGame(vo);\r\n            });\r\n        } else {\r\n            selfGame ? this.checkInnerAccess(vo) : LobbyModel.gotoOhterGame(vo);\r\n        }\r\n    }\r\n    /**\r\n     * 自有游戏准入限制\r\n     */\r\n    public static checkInnerAccess(vo: GameIconVo) {\r\n        if (!LobbyModel.useSingleDownload) GameMgr.showCircleLoading(true);\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.gameAccess + vo.id, this, (suc: boolean, jobj: any) => {\r\n            if (!LobbyModel.useSingleDownload) GameMgr.showCircleLoading(false);\r\n            if (suc) {\r\n                if (jobj.code == 2407) {//充值限制\r\n                    DownLoadView.hide();//关闭加载界面\r\n                    SystemDlg.show(jobj.message,\r\n                        {\r\n                            caller: this,\r\n                            rightBtnCallback: () => {\r\n                                InnerJumpUtil.doJump(DlgCmd.recharge);\r\n                            }\r\n                        }\r\n                    );\r\n                } else {\r\n                    Tools.jump2game(vo.url);\r\n                }\r\n            } else {\r\n                Debug.error(\"准入限制接口失败>>>\", vo.name);\r\n                DownLoadView.hide();//关闭加载界面\r\n            }\r\n        });\r\n    }\r\n    //请求vconsole开关\r\n    public static getVconsoleOpen() {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.vconsole, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                Debug.httpDebug = Boolean(jobj.data);\r\n            }\r\n        }, \"&environment=dev,sit,uat\");\r\n    }\r\n    /**\r\n     * 检查活动公告是否需要默认弹出\r\n     */\r\n    public static checkActivity() {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.attention_newPop, this, (suc: boolean, jobj: any) => {\r\n            if (suc && jobj.code == 1001) {\r\n                for (let i = 0; i < jobj.data.length; i++) {\r\n                    if (jobj.data[i].noticeType == \"SHARE_DAILY\")\r\n                        QueueTask.addQueue(QueueType.shartDlg);\r\n                    else if (jobj.data[i].noticeType == \"ROULETTE_DRAW\")\r\n                        QueueTask.addQueue(QueueType.rouletteDlg);\r\n                    else if (jobj.data[i].noticeType == \"POPULARIZE\")\r\n                        QueueTask.addQueue(QueueType.activityDlg, { noticeCate: 2, noticeid: jobj.data[i].noticeid });\r\n                    else if (jobj.data[i].noticeType == \"GAME\")\r\n                        QueueTask.addQueue(QueueType.activityDlg, { noticeCate: 0, noticeid: jobj.data[i].noticeid });\r\n                }\r\n            }\r\n            LobbyModel.openQueueTaskDlg();\r\n        });\r\n    }\r\n    /**\r\n     * 按顺序弹出默认弹出的弹框\r\n     */\r\n    public static openQueueTaskDlg(): void {\r\n        if (QueueTask.queueList.length == 0)\r\n            return;\r\n        //检测绑定送金\r\n        let isopen: boolean = QueueTask.checkQueue([QueueType.bindPhoneActiv]);\r\n        if (isopen)\r\n            return;\r\n        isopen = QueueTask.checkQueue([QueueType.shartDlg]);\r\n        if (isopen)\r\n            return;\r\n        isopen = QueueTask.checkQueue([QueueType.rouletteDlg]);\r\n        if (isopen)\r\n            return;\r\n        isopen = QueueTask.checkQueue([QueueType.activityDlg]);\r\n    }\r\n    /**\r\n     * 检查是否有新的活动\r\n     */\r\n    public static checkUnreadNotice(caller?: any, callback?: Function) {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.attention_new, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                //type0-公告，type1-活动 dial-是否显示转盘\r\n                let hotobj = { type0: false, type1: false, dial: false, dia2: false };\r\n                jobj.forEach((data, index) => {\r\n                    if (data.noticeList) {\r\n                        data.noticeList.forEach(data => {\r\n                            if (data.bread === false) {//标记为有未读数据\r\n                                if (data.noticeActivityType != ActiveType[ActiveType.ROULETTE_DRAW] && data.noticeActivityType != ActiveType[ActiveType.SHARE_DAILY])\r\n                                    hotobj[\"type\" + index] = true;\r\n                            }\r\n                            if (index == 1) {//判断活动中有无转盘的配置\r\n                                if (data.noticeActivityType == ActiveType[ActiveType.ROULETTE_DRAW]) {\r\n                                    hotobj.dial = true;\r\n                                }\r\n                                if (data.noticeActivityType == ActiveType[ActiveType.SHARE_DAILY])\r\n                                    hotobj.dia2 = true;\r\n                            }\r\n                        });\r\n                    }\r\n                });\r\n\r\n                if (caller && callback) callback.call(caller, hotobj);\r\n            }\r\n        });\r\n    }\r\n    /**\r\n     * 绑定手机相关数据\r\n     */\r\n    public static reqBindInfo(caller?: any, callback?: Function) {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.getBindInfo, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                GameData.bindVo = jobj;\r\n                if (caller && callback) callback.call(caller, GameData.bindVo.reward);\r\n                else EventManager.dispath(EventType.BINDPHONE_INFO);\r\n            }\r\n        });\r\n    }\r\n    /**\r\n     * users\r\n     * flushBalance 是否刷新余额\r\n     */\r\n    public static reqUserInfo(flushBalance: boolean = true) {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.userinfobalance, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                Common.userInfo = jobj;\r\n                Common.setLoginPlatform(jobj.loginPlatform);\r\n                EventManager.dispath(EventType.GETUSERS_INFO);\r\n                if (flushBalance) this.refreshMoney();\r\n            }\r\n        });\r\n    }\r\n    /**\r\n     * current数据\r\n     * @param isLogin 是否为刚登录后的请求\r\n     */\r\n    public static reqUserCurrentInfo(isLogin?: boolean) {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.userinfo, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                Common.userInfo_current = jobj;\r\n                if (jobj.username == jobj.nickname || !Tools.trim(jobj.nickname)) {\r\n                    LobbyModel.reqInitNickInfo();\r\n                } else {\r\n                    EventManager.dispath(EventType.SHOW_NICK_NAME);\r\n                }\r\n                let vo: LoginVo = SaveManager.getObj().get(SaveManager.KEY_LASTLOGININFO, null);\r\n                if (jobj.certifiedPhone) {//如果玩家绑定过手机,则将缓存的用户名改为手机号\r\n                    LoginModel.loginType = LoginMethod.account;\r\n                    if (vo) {\r\n                        vo.phone = jobj.phoneNumber;\r\n                        vo.type = LoginMethod.account;\r\n                        SaveManager.getObj().save(SaveManager.KEY_LASTLOGININFO, vo);\r\n                    } else {\r\n                        Debug.error(\"正式账号登录成功,但没有缓存数据\");\r\n                    }\r\n                } else {\r\n                    LoginModel.loginType = LoginMethod.visitor;\r\n                    if (vo && vo.type != LoginMethod.visitor) {\r\n                        vo.type = LoginMethod.visitor;\r\n                        vo.phone = null;\r\n                        SaveManager.getObj().save(SaveManager.KEY_LASTLOGININFO, vo);\r\n                    } else if (!vo) {\r\n                        Debug.error(\"非正式账号登录成功,但没有缓存数据\");\r\n                    }\r\n                }\r\n                if (isLogin) this.sendOnline();\r\n                EventManager.dispath(EventType.GETUSER_CURRENT);\r\n            }\r\n        });\r\n    }\r\n    /**\r\n     * 请求一个随机昵称\r\n     * @param caller \r\n     * @param callback \r\n     * @param showloading \r\n     */\r\n    public static reqNickName(caller?: any, callback?: Function, showloading?: boolean) {\r\n        if (showloading) GameMgr.showCircleLoading(true);\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.getNickName, this, (suc: boolean, jobj: any) => {\r\n            if (showloading) GameMgr.showCircleLoading(false);\r\n            let str = \"\";\r\n            if (suc) {\r\n                str = jobj.data || \"\";\r\n            }\r\n            if (caller && callback) callback.call(caller, str);\r\n        });\r\n    }\r\n    /**\r\n     * 初始化获取昵称\r\n     */\r\n    public static reqInitNickInfo() {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.initNickName, this, (suc: boolean, jobj: any) => {\r\n            Debug.log(\"昵称>>>：\", suc, jobj);\r\n            if (suc) {\r\n                Common.userInfo_current.nickname = jobj.data.nickName;\r\n                EventManager.dispath(EventType.SHOW_NICK_NAME);\r\n            }\r\n        });\r\n    }\r\n    /**\r\n     * 检测昵称修改次数\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static checkSetNickCount(caller?: any, callback?: Function) {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.setNickCount, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                if (caller && callback) callback.call(caller, jobj.data);\r\n            }\r\n        });\r\n    }\r\n    /**\r\n     * 头像数据\r\n     */\r\n    public static reqAvatarInfo() {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.avatorget, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                Common.avatorInfo = jobj;\r\n                var tempId = jobj.avatar;\r\n                if (!tempId) tempId = \"05\";\r\n                tempId = Tools.FormatNumber(parseInt(tempId), 2);\r\n                Common.avatorInfo.avatorId = tempId;\r\n\r\n                SaveManager.getObj().save(SaveManager.KEY_AVATOR_ID, tempId);\r\n\r\n                EventManager.dispath(EventType.GETAVATOR_INFO);\r\n            }\r\n        });\r\n    }\r\n    /**\r\n     * 余额刷新缓存(从老虎机返回大厅才用)\r\n     */\r\n    public static refreshMoneyCache(): void {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.clearTotalBalance, this, (suc: boolean, jobj: any) => { });\r\n    }\r\n    /**\r\n     * 余额刷新(获取用户的余额信息包含免转金额的回收详情)\r\n     */\r\n    public static refreshMoney(caller?: any, callback?: Function) {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.recoverBalanceInfo, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                Common.userBalance = jobj.data.totalAmount;\r\n                Common.balanceInfo = jobj.data;\r\n                Debug.log(\"刷新余额：\", Common.userBalance);\r\n                EventManager.dispath(EventType.FLUSH_MONEY);\r\n            }\r\n            if (caller && callback) callback.call(caller);\r\n        });\r\n    }\r\n    /**\r\n     * 获取绑定卡相关信息\r\n     */\r\n    public static getCardInfo(caller?: any, callback?: Function, retry?: boolean) {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.getCardInfo, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                Common.cardInfo = jobj;\r\n                if (caller && callback) {\r\n                    callback.call(caller);\r\n                }\r\n            } else {\r\n                //如果失败了重现尝试一次\r\n                if (!retry) Laya.timer.once(600, this, this.getCardInfo, [caller, callback, true]);\r\n            }\r\n            if (!retry) this.getCardDetailInfo();\r\n        });\r\n    }\r\n    /**\r\n     * 获取未读邮件\r\n     */\r\n    public static getUnreadMail() {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.getUnreadMail, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                EventManager.dispath(EventType.CHECK_UNREADMAIL, jobj);\r\n            }\r\n        });\r\n    }\r\n    /**\r\n     * 获取银行卡绑定详细信息\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    private static getCardDetailInfo() {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.getbankCardInfo, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                let arr: IBackCardVo[] = jobj.bankAccounts;\r\n                arr.forEach(value => {\r\n                    if (value.bankCode == \"ZHB\") {\r\n                        Common.alipayInfo = value;\r\n                    } else {\r\n                        Common.bankInfo = value;\r\n                    }\r\n                });\r\n                EventManager.dispath(EventType.GET_BACKCARD_DETAIL);\r\n            }\r\n        });\r\n    }\r\n    /**\r\n     * 检测维护公告\r\n     */\r\n    public static checkMaintenanceNotice(caller?: any, callback?: Function) {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.vindicateNotice, this, (suc: boolean, jobj: any) => {\r\n            let bl: boolean = true;//是否没有维护公共\r\n            Debug.log(\"维护公告：\", suc, jobj);\r\n            if (suc) {\r\n                if (jobj.maintenanceState && jobj.maintenanceDto) {\r\n                    GameUpdateNotice.show(jobj.maintenanceDto || {});\r\n                    bl = false;\r\n                }\r\n            }\r\n            if (caller && callback) callback.call(caller, bl);\r\n        });\r\n    }\r\n\r\n    /**\r\n     * 获取每日分享活动按钮是否显示\r\n     */\r\n    public static getActivityVisible(): void {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.isOpenShare, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                if (jobj.http)\r\n                    EventManager.dispath(EventType.ACTIVITY_VISIBLE, false);\r\n                else\r\n                    EventManager.dispath(EventType.ACTIVITY_VISIBLE, jobj);\r\n            }\r\n        });\r\n    }\r\n\r\n    /**\r\n     * 领取了VIP奖励通知管端处理邮件\r\n     */\r\n    public static noticeSendMail(type: string, amount: number, caller?: any, callback?: Function): void {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.vipMailSend, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                LobbyModel.refreshMoney();\r\n            }\r\n        }, \"&amount=\" + String(amount) + \"&messageType=\" + type);\r\n    }\r\n    //通知后端，用于做异地登录\r\n    public static sendOnline() {\r\n        let params: string = \"&userId=\" + Common.userInfo_current.id;\r\n        params += \"&username=\" + Common.userInfo_current.username;\r\n        params += \"&brand=\" + AppData.brandID;\r\n        Debug.log(\"online:\", params);\r\n        HttpRequester.postHttpData(ConfObjRead.httpCmd.online, null, this, (suc: boolean, jobj: any) => {\r\n            // Debug.log(suc, jobj);\r\n        }, false, params);\r\n    }\r\n    /**\r\n     * 请求轮播图数据\r\n     */\r\n    public static requestCycelData() {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.getCarouselInfo, this, (suc: boolean, jobj) => {\r\n            if (suc) {\r\n                let flush: boolean = false;//是否需要刷新数据\r\n                let arr: CycelVo[] = jobj.carousels;\r\n                if (!this.cycelData) {\r\n                    this.cycelData = arr;\r\n                    flush = true;\r\n                }\r\n                else {\r\n                    if (this.cycelData.length != arr.length) flush = true;\r\n                    else {\r\n                        this.cycelData.forEach((vo, index) => {\r\n                            if (vo.carouselUrl != arr[index].carouselUrl) flush = true;\r\n                        });\r\n                    }\r\n                }\r\n                EventManager.dispath(EventType.CAROUSE_DATA, flush);\r\n            }\r\n        });\r\n    }\r\n    /**\r\n     * 每次登录后通知后端\r\n     */\r\n    public static init() {\r\n        let params = \"&jump=\" + Common.bNewlogin;\r\n        HttpRe
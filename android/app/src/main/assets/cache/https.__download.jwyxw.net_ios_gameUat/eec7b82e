r.showDlg(DlgCmd.agentCenter);\r\n\t\t});\r\n\t\t//提现\r\n\t\tEventManager.addTouchScaleListener(this.withdrawBtn, this, () => {\r\n\t\t\tSoundPlayer.enterPanelSound();\r\n\t\t\tInnerJumpUtil.doJump(DlgCmd.withdraw);\r\n\t\t});\r\n\t\t//充值\r\n\t\tEventManager.addTouchScaleListener(this.payBtn, this, () => {\r\n\t\t\tSoundPlayer.enterPanelSound();\r\n\t\t\tInnerJumpUtil.doJump(DlgCmd.recharge);\r\n\t\t});\r\n\t\t//全局消息监听\r\n\t\tEventManager.register(EventType.GETBINDAWARD_SUCC, this, this.hideBindBtn);\r\n\t\tEventManager.register(EventType.GETUSER_CURRENT, this, this.checkBindPhone);\r\n\t\tEventManager.register(EventType.BINDPHONE_INFO, this, this.checkBindPhone);\r\n\t\tEventManager.register(EventType.GETUSERS_INFO, this, this.checkAgentBtn);\r\n\t\tEventManager.register(EventType.CHECK_UNREADMAIL, this, this.checkUnreadMail);\r\n\t\tEventManager.register(EventType.ACTIVITY_VISIBLE, this, this.setActivityVisible);\r\n\t\tEventManager.register(\"closeNotice\", this, this.checkUnreadNotice);\r\n\t}\r\n\tprivate getActivityVisible(): void {\r\n\t\tLobbyModel.getActivityVisible();\r\n\t}\r\n\tprivate setActivityVisible(obj): void {\r\n\t\tDebug.log(\"分享按钮>>>\", obj);\r\n\t\tthis.songjinbiBtn.visible = obj;\r\n\t\tif (obj) {\r\n\t\t\tthis.layoutTopRightBtn();\r\n\t\t}\r\n\t}\r\n\t//活动公告红点显示逻辑\r\n\tprivate checkUnreadNotice() {\r\n\t\tLobbyModel.checkUnreadNotice(this, (obj: { type0: boolean, type1: boolean, dial: boolean, dia2: boolean }) => {\r\n\t\t\tif (this.destroyed) return;\r\n\t\t\tthis.actHot.visible = obj.type1;\r\n\t\t\tthis.noticeHot.visible = obj.type0;\r\n\t\t\tthis.cirBtn.visible = obj.dial;\r\n\t\t\tthis.layoutTopRightBtn();\r\n\t\t});\r\n\t}\r\n\t//邮件红点显示逻辑\r\n\tprivate checkUnreadMail(jobj: any) {\r\n\t\tlet total: number = jobj.total || 0;\r\n\t\tthis.mailHot.visible = Boolean(total > 0);\r\n\t}\r\n\t//代理按钮显示逻辑\r\n\tprivate checkAgentBtn() {\r\n\t\tif (Common.userInfo) {\r\n\t\t\tthis.agentBtn.visible = Common.userInfo.userRole != \"PLAYER\";\r\n\t\t}\r\n\t}\r\n\t//升级成功后的逻辑\r\n\tprivate hideBindBtn() {\r\n\t\tthis.bindBtn.visible = false;\r\n\t\tthis.layoutTopRightBtn();\r\n\t\tthis.userInfo.checkVisitorMark();\r\n\t\tthis.userInfo.flushMoney();\r\n\t\tLobbyModel.reqUserCurrentInfo();\r\n\t}\r\n\t//绑定升级送金显示逻辑\r\n\tprivate checkBindPhone() {\r\n\t\tif (!Common.userInfo_current || !GameData.bindVo) return;\r\n\t\tthis.userInfo.checkVisitorMark();\r\n\t\tif (LoginModel.isVisitor && !GameData.bindVo.receive) {//只要不是正式账号(如果解绑手机后也是不需要显示的)\r\n\t\t\tthis.bindBtn.visible = true;\r\n\t\t\tthis.layoutTopRightBtn();\r\n\t\t\t//显示逻辑已经放到活动公告关闭后,如果没有活动公告则直接显示\r\n\t\t\tif (this.isFirst) QueueTask.addQueue(QueueType.bindPhoneActiv);\r\n\t\t}\r\n\t}\r\n\r\n\t//-----------------------public-------------------------------\r\n\t/**\r\n\t * 播放入场动画\r\n\t */\r\n\tpublic playEntryAnim(first: boolean = true) {\r\n\t\tthis.userInfo.y = -this.userInfo.height * 1.5;\r\n\t\tthis.trLayout.y = -this.trLayout.height * 1.5;\r\n\t\tthis.bottomGroup.y = Laya.stage.height + this.bottomGroup.height;\r\n\t\tthis.brLayout.y = Laya.stage.height;\r\n\r\n\t\tlet easeing = Laya.Ease.cubicOut;\r\n\t\tlet time = 350;\r\n\t\tLaya.Tween.to(this.userInfo, { y: 0 }, time, easeing, null, 100);\r\n\t\tLaya.Tween.to(this.trLayout, { y: 20 }, time, easeing, null, 100);\r\n\t\tLaya.Tween.to(this.bottomGroup, { y: Laya.stage.height - this.bottomGroup.height }, time, easeing, null, 100);\r\n\t\tLaya.Tween.to(this.brLayout, { y: 633 }, time, easeing, null, 100);\r\n\t\tthis.gameList.playEntryAnim(!first);\r\n\t\tthis.isFirst = first;\r\n\t\tif (first) Laya.timer.once(1500, this, () => {\r\n\t\t\t//检查显示默认弹框\r\n\t\t\tLobbyModel.checkActivity();\r\n\t\t});\r\n\t}\r\n\t/**\r\n\t * 从游戏返回到大厅\r\n\t */\r\n\tpublic gameToHall() {\r\n\t\tDebug.log(\"返回大厅--->恢复动画播放\");\r\n\t\t//恢复动画播放\r\n\t\tthis.animArr.forEach(anim => anim.resume());\r\n\t\tthis.playEntryAnim(false);\r\n\t\tthis.leftLight.initPlay();\r\n\t\tthis.rightLight.initPlay(500);\r\n\t\t//刷新数据\r\n\t\tthis.userInfo.flushMoney();\r\n\t\tthis.checkUnreadNotice();\r\n\t\tWithDrawModel.getBetInfo();//刷新数据\r\n\t\t//延迟请求非必要数据\r\n\t\tlet arr = [LobbyCmd.unReadMail, LobbyCmd.globalMaintain, LobbyCmd.cycelData];\r\n\t\tarr.forEach((cmd, index) => {\r\n\t\t\tLaya.timer.once(1500 + index * 1500, LobbyModel, LobbyModel.reqDataCmd, [cmd], false);\r\n\t\t});\r\n\t}\r\n\t/**\r\n\t * 从大厅进入游戏\r\n\t */\r\n\tpublic hallToGame() {\r\n\t\tDebug.log(\"进入游戏--->暂停动画播放\");\r\n\t\tthis.animArr.forEach(anim => anim.paused());\r\n\t\tthis.cirbox.pause();\r\n\t\tthis.leftLight.stopAnim();\r\n\t\tthis.rightLight.stopAnim();\r\n\t}\r\n\t/**\r\n\t * 用于进出游戏显示和隐藏界面相关\r\n\t * @param value \r\n\t */\r\n\tpublic showUI(value: boolean) {\r\n\t\tthis.userInfo.visible = value;\r\n\t\tthis.trLayout.visible = value;\r\n\t\tthis.bottomGroup.visible = value;\r\n\t\tthis.brLayout.visible = value;\r\n\t\tthis.gameList.visible = value;\r\n\t}\r\n\t//------------------------布局相关---------------------------------------------\r\n\t//按钮布局\r\n\tprivate layoutBottomLeftBtn() {\r\n\t\tlet gap = 46;\r\n\t\tlet start = this.btn_yeb.x + this.btn_yeb.width / 2;\r\n\t\tthis.btn_rank.visible = false;\r\n\t\tlet arr = [//从左到右排列\r\n\t\t\tthis.btn_rank,\r\n\t\t\tthis.btn_mail,\r\n\t\t\tthis.btn_notice,\r\n\t\t\tthis.btn_kefu\r\n\t\t];\r\n\t\tlet prev: Laya.Sprite;\r\n\t\tarr.forEach((btn, index) => {\r\n\t\t\tif (btn.visible) {\r\n\t\t\t\tif (prev) btn.x = prev.x + prev.width / 2 + btn.width / 2 + gap;\r\n\t\t\t\telse btn.x = start + btn.width / 2 + gap;\r\n\t\t\t\tprev = btn;\r\n\t\t\t}\r\n\t\t});\r\n\t}\r\n\t//按钮布局\r\n\tprivate layoutTopRightBtn() {\r\n\t\tlet gap = 26;\r\n\t\tlet start = this.actBtn.x - this.actBtn.width / 2;\r\n\t\tlet arr = [//从右到左排列\r\n\t\t\tthis.cirBtn,\r\n\t\t\tthis.songjinbiBtn,\r\n\t\t\tthis.bindBtn,\r\n\t\t\tthis.agentBtn\r\n\t\t];\r\n\t\tlet prev: Laya.Sprite;\r\n\t\tarr.forEach((btn, index) => {\r\n\t\t\tif (btn.visible) {\r\n\t\t\t\tif (prev)\r\n\t\t\t\t\tbtn.x = prev.x - prev.width / 2 - btn.width / 2 - gap;\r\n\t\t\t\telse\r\n\t\t\t\t\tbtn.x = start - btn.width / 2 - gap;\r\n\t\t\t\tprev = btn;\r\n\t\t\t}\r\n\t\t});\r\n\t}\r\n\t//根据分辨率布局\r\n\tprivate dpiLayout() {\r\n\t\tthis.width = Laya.stage.width;\r\n\t\tthis.userInfo.x = GameUtils.getScreencOffset(-40, 0);\r\n\t\tthis.bottomGroup.x = GameUtils.getScreencOffset(-58, 0);\r\n\t\tthis.brLayout.right = this.trLayout.right = GameUtils.getScreencOffset(20, 78);\r\n\t\tthis.gameList.x = this.gameList.gapx;\r\n\t}\r\n\t/**\r\n\t * 释放销毁\r\n\t */\r\n\tpublic destroy() {\r\n\t\tEventManager.removeAllEvents(this);\r\n\t\tthis.animArr.forEach(anim => anim.destroy(true));\r\n\t\tsuper.destroy(true);\r\n\t}\r\n}\r\n","import { Common } from \"../common/Common\";\r\nimport { ConfObjRead } from '../common/ConfObjRead';\r\nimport { Debug } from '../common/Debug';\r\nimport { PostMHelp } from '../common/PostMHelp';\r\nimport { SaveManager } from '../common/SaveManager';\r\nimport { LobbyCmd, LobbyModel } from \"../data/LobbyModel\";\r\nimport { ResConfig } from \"../data/ResConfig\";\r\nimport { EventManager } from '../events/EventManager';\r\nimport { GameMgr } from \"../utils/GameMgr\";\r\nimport { SoundPlayer } from \"../utils/SoundPlayer\";\r\nimport { EventType } from './../events/EventManager';\r\nimport { DownLoadView } from './DownLoadView';\r\nimport { LobbyNewView } from \"./LobbyNewView\";\r\nimport { PageManager } from \"./PageManager\";\r\n\r\n/*\r\n* 大厅场景\r\n*/\r\nexport class LobbyScene extends Laya.Sprite {\r\n\r\n    private view: LobbyNewView;\r\n\r\n    constructor() {\r\n        super();\r\n    }\r\n    //进入游戏时释放资源占用\r\n    private clearLobby() {\r\n        if (this.view) {\r\n            this.view.showUI(false);\r\n            this.view.hallToGame();\r\n            PageManager.clearLobbyRes();\r\n            PageManager.clearLoginRes();\r\n            if (LobbyModel.useSingleDownload) DownLoadView.hide();\r\n        }\r\n    }\r\n    //\r\n    private creatLobby() {\r\n        GameMgr.showCircleLoading(false);//防止从游戏返回大厅时还有loading\r\n        if (LobbyModel.useSingleDownload) DownLoadView.hide();\r\n\r\n        if (!this.view) {\r\n            this.initUI();\r\n        } else {\r\n            this.view.showUI(true);\r\n            this.view.gameToHall();\r\n        }\r\n    }\r\n    private initUI(): void {\r\n        //添加大厅视图\r\n        this.view = new LobbyNewView();\r\n        this.addChild(this.view);\r\n        //请求相关接口\r\n        LobbyModel.reqBindInfo();\r\n        LobbyModel.reqUserInfo(false);\r\n        LobbyModel.reqUserCurrentInfo(true);\r\n        LobbyModel.reqAvatarInfo();\r\n        //延迟请求非必要数据\r\n        let arr = [LobbyCmd.vipData, LobbyCmd.bindCard, LobbyCmd.init, LobbyCmd.init5];\r\n        arr.forEach((cmd, index) => {\r\n            Laya.timer.once(1500 + index * 1500, LobbyModel, LobbyModel.reqDataCmd, [cmd], false);\r\n        });\r\n    }\r\n\r\n    private static IS_PLAYED_MUSIC: boolean = false;\r\n    /**\r\n     * 初始化背景音乐\r\n     */\r\n    public static initBgMusic(): void {\r\n        if (this.IS_PLAYED_MUSIC) {\r\n            return;\r\n        }\r\n\r\n        this.IS_PLAYED_MUSIC = true;\r\n        Laya.loader.load([ResConfig.musicUrl], new Laya.Handler(this, () => {\r\n            SoundPlayer.UpdateBGM();\r\n        }));\r\n    }\r\n    /**\r\n     * 初始化大厅\r\n     */\r\n    public onLoaded() {\r\n        if (!Common.access_token) {\r\n            GameMgr.loginOut();\r\n            return;\r\n        }\r\n        //检测维护公告\r\n        LobbyModel.checkMaintenanceNotice(this, (suc: boolean) => {\r\n            if (suc) {\r\n                PostMHelp.game_common({ do: \"playMusicEffect\", param: \"welcome_lobby\" });\r\n                Debug.log(\"play-welcome_lobby-app>>>\");\r\n            }\r\n        });\r\n        Common.backUrl = ConfObjRead.getConfUrl().url.backlobby;\r\n        SaveManager.getObj().initCommon(Common.access_token, ConfObjRead.getConfUrl().url.apihome);\r\n        Common.confObj.url = ConfObjRead.getConfUrl().url;\r\n\r\n        this.creatLobby();\r\n\r\n        LobbyModel.getVconsoleOpen();\r\n        EventManager.register(EventType.GAMETOHALL, this, this.creatLobby);\r\n        EventManager.register(EventType.HALLTOGAME, this, this.clearLobby);\r\n        Debug.log(\"lobby-appdata:\", AppData);\r\n    }\r\n    public destroy(b: boolean): void {\r\n        EventManager.removeAllEvents(this);\r\n        if (this.view) {\r\n            this.view.destroy();\r\n            this.view = null;\r\n        }\r\n        super.destroy(b);\r\n    }\r\n}\r\n","import { Common } from \"../common/Common\";\r\nimport { ConfObjRead } from \"../common/ConfObjRead\";\r\nimport { Debug } from \"../common/Debug\";\r\nimport { PostMHelp } from \"../common/PostMHelp\";\r\nimport { SaveManager } from '../common/SaveManager';\r\nimport { ToolsApp } from \"../common/ToolsApp\";\r\nimport { GameData, JoinLobbyType } from \"../data/GameVos\";\r\nimport { HttpRequester } from \"../data/HttpRequester\";\r\nimport { LobbyModel } from \"../data/LobbyModel\";\r\nimport { LoginMethod, LoginModel, LoginVo } from \"../data/LoginModel\";\r\nimport { ResConfig } from '../data/ResConfig';\r\nimport { EventManager, EventType } from '../events/EventManager';\r\nimport { ui } from \"../ui/layaMaxUI\";\r\nimport { GameUtils } from \"../utils/GameUtils\";\r\nimport { SoundPlayer } from '../utils/SoundPlayer';\r\nimport { GameMgr } from './../utils/GameMgr';\r\nimport { GameUpdateMgr } from './../utils/GameUpdateMgr';\r\nimport { AccountLoginDlg } from './dlg/login/AccountLoginDlg';\r\nimport { LobbyScene } from \"./LobbyScene\";\r\nimport { PageManager } from \"./PageManager\";\r\nimport { Toast } from './toast/Toast';\r\n\r\n/**\r\n * 新登录界面\r\n */\r\nexport class LoginScene extends ui.sceneUI.LoginSceneUI {\r\n\tprivate loginBtns: Laya.Image[];//登录方式按钮组\r\n\tprivate cacheInfo: LoginVo;//缓存的登录信息\r\n\tprivate eventInited: boolean;\r\n\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t\tthis.initLayout();\r\n\t\tthis.initDisplay();\r\n\t\tthis.startLoading();\r\n\t\tLaya.stage.on(Laya.Event.RESIZE, this, this.onResize);\r\n\t}\r\n\tprivate onResize() {\r\n\t\tthis.initLayout();\r\n\t}\r\n\tprivate startLoading() {\r\n\t\tif (LoginModel.isLoaded) {//说明是从大厅退出到登录界面\r\n\t\t\t//先获取开关\r\n\t\t\tLoginModel.getGTestOpen();\r\n\t\t\tthis.checkLoginType();\r\n\t\t} else {\r\n\t\t\tGameUpdateMgr.readGameVer();\r\n\t\t\tthis.progress.visible = true;\r\n\t\t\tlet conf = Laya.loader.getRes(\"./assets/conf/assets_lobby.json\");\r\n\t\t\tlet list: string[] = conf.list;\r\n\t\t\t//预加载游戏图标龙骨动画和大厅背景\r\n\t\t\tlist = list.concat(ConfObjRead.getIconList());\r\n\t\t\tlist.push(\"ui/lobby/bg_dating.jpg\");\r\n\t\t\t//加载数据\r\n\t\t\tLaya.loader.load(\r\n\t\t\t\tlist,\r\n\t\t\t\tLaya.Handler.create(this, this.loadFinish, null, false),\r\n\t\t\t\tLaya.Handler.create(this, this.loadProgress, null, false)\r\n\t\t\t).on(Laya.Event.ERROR, this, this.loadErr);\r\n\t\t}\r\n\t}\r\n\tprivate loadErr(e: Laya.Event) {\r\n\t\tDebug.error(\"load-err:\", e);\r\n\t}\r\n\tprivate loadProgress(value: number) {\r\n\t\tthis.progress.value = value;\r\n\t\tlet jd = Math.floor(value * 100);\r\n\t\tif (jd > 99) jd = 99;\r\n\t\tthis.progressTxt.text = \"正在加载资源\" + jd + \"%\";\r\n\t}\r\n\t//资源加载完毕\r\n\tprivate loadFinish() {\r\n\t\tDebug.log(\"loadFinish\");\r\n\t\tPostMHelp.initGame();\r\n\t\tPageManager.initDlgMap();\r\n\t\tCommon.confObj = ConfObjRead.getConfCommon();\r\n\t\tCommon.isWeb = Boolean(Common.confObj.web);\r\n\t\tGameData.joinLobbyType = JoinLobbyType.loginJoin;\r\n\t\tLoginModel.isLoaded = true;\r\n\r\n\t\tLobbyScene.initBgMusic();\r\n\t\t//防止url替换失败\r\n\t\tif (GameUtils.isNativeApp && !AppData.urlJSON) {\r\n\t\t\tLaya.timer.once(2000, this, this.copyNativeUrls);\r\n\t\t\tHttpRequester.addLog(\"无app数据,延迟2s获取\");\r\n\t\t} else {\r\n\t\t\tthis.copyNativeUrls();\r\n\t\t}\r\n\t}\r\n\tprivate copyNativeUrls() {\r\n\t\tToolsApp.copyNativeAdress();\r\n\t\t//先获取开关\r\n\t\tLoginModel.getGTestOpen(this, this.useTokenLogin);\r\n\t}\r\n\t//------------------token登录流程-------------------------\r\n\t/**\r\n\t * login-使用token登录\r\n\t */\r\n\tprivate useTokenLogin() {\r\n\t\tLoginModel.loginByToken(this, (suc: boolean, curToken: string) => {\r\n\t\t\tif (suc) {\r\n\t\t\t\tLobbyModel.reqCdnUrl(this, () => {//先请求三方图标cdn\r\n\t\t\t\t\tLobbyModel.reqClassifyMenu();//先请求菜单数据\r\n\t\t\t\t});\r\n\t\t\t} else {\r\n\t\t\t\tif (curToken) LoginModel.flushToken(this, this.flushTokenResult);\r\n\t\t\t\telse {//没有缓存token,说明是主动退出到登录界面的\r\n\t\t\t\t\tthis.checkLoginType();\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t});\r\n\t}\r\n\t//刷新token的结果\r\n\tprivate flushTokenResult(suc: boolean) {\r\n\t\tDebug.error(\"刷新token结果:\", suc);\r\n\t\tif (suc) {\r\n\t\t\tLoginModel.loginByToken(this, this.useNewTokenLoginResult);\r\n\t\t} else {\r\n\t\t\tthis.checkLoginType();\r\n\t\t}\r\n\t}\r\n\t//使用刷新后的token登录情况\r\n\tprivate useNewTokenLoginResult(suc: boolean) {\r\n\t\tif (suc) {\r\n\t\t\tGameMgr.initLobby();\r\n\t\t} else {\r\n\t\t\tthis.checkLoginType();\r\n\t\t}\r\n\t}//end-----------------------------------------------------\r\n\t//检查使用哪种登录方式\r\n\tpublic checkLoginType() {\r\n\t\tthis.progress.visible = false;\r\n\t\tthis.cacheInfo = SaveManager.getObj().get(SaveManager.KEY_LASTLOGININFO, null);\r\n\t\tif (this.cacheInfo) this.showFastStartView();\r\n\t\telse this.showOtherLoginView();\r\n\r\n\t\tthis.initEvents();\r\n\t}\r\n\t//显示快速开始界面\r\n\tprivate showFastStartView() {\r\n\t\tthis.fastGroup.alpha = 0;\r\n\t\tthis.otherBtn.alpha = 0;\r\n\t\tthis.fastGroup.visible = true;\r\n\t\tthis.otherBtn.visible = true;\r\n\t\tthis.btnGroup.visible = false;\r\n\t\tthis.userTxt.text = this.cacheInfo.phone || this.cacheInfo.user;//优先展示手机号\r\n\t\tthis.visitorMark.visible = Boolean(this.cacheInfo.type != LoginMethod.account);\r\n\t\tLaya.Tween.to(this.fastGroup, { alpha: 1 }, 500);\r\n\t\tLaya.Tween.to(this.otherBtn, { alpha: 1 }, 500);\r\n\t}\r\n\t//显示其他登录方式\r\n\tprivate showOtherLoginView() {\r\n\t\tthis.fastGroup.visible = false;\r\n\t\tthis.otherBtn.visible = false;\r\n\t\tthis.btnGroup.visible = true;\r\n\t\t//显示登录模式按钮\r\n\t\tthis.loginBtns.forEach(btn => {\r\n\t\t\tbtn.visible = true;\r\n\t\t\tbtn.scale(0, 0);\r\n\t\t\tLaya.Tween.to(btn, { scaleX: 1, scaleY: 1, alpha: 1 }, 500, Laya.Ease.elasticOut);\r\n\t\t});\r\n\t}\r\n\tprivate initEvents() {\r\n\t\tif (this.eventInited) return;\r\n\t\t//快速开始\r\n\t\tEventManager.addTouchScaleListener(this.fastStart, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tthis.fastLogin();\r\n\t\t});\r\n\t\t//切换账号\r\n\t\tEventManager.addTouchScaleListener(this.toggleBtn, this, () => {\r\n\t\t\tSoundPlayer.enterPanelSound();\r\n\t\t\tAccountLoginDlg.show();\r\n\t\t});\r\n\t\t//切换其他方式登录\r\n\t\tEventManager.addTouchScaleListener(this.otherBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tthis.showOtherLoginView();\r\n\t\t});\r\n\t\t//-----------------\r\n\t\t//游客登录\r\n\t\tEventManager.addTouchScaleListener(this.visitorBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tlet vo: LoginVo = SaveManager.getObj().get(SaveManager.KEY_VISTORINFO, null);\r\n\t\t\tif (vo) this.doVisitorLogin(vo, false);\r\n\t\t\telse {\r\n\t\t\t\tDebug.error(\"没有缓存游客账号,创建中...\");\r\n\t\t\t\t//先获取init5\r\n\t\t\t\tLoginModel.getInit5(this, (suc: boolean, status?: number) => {\r\n\t\t\t\t\tif (suc) {\r\n\t\t\t\t\t\tLoginModel.creatVisitorAccount(this, this.creatVisitorResult);\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\t\t\t}\r\n\t\t});\r\n\t\t//微信登录\r\n\t\tEventManager.addTouchScaleListener(this.wechatBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tLoginModel.weChatCertification(0);\r\n\t\t});\r\n\t\t//账号登录\r\n\t\tEventManager.addTouchScaleListener(this.accountBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tif (this.cacheInfo) this.showFastStartView();\r\n\t\t\telse AccountLoginDlg.show();\r\n\t\t});\r\n\t\t//监听微信登录消息\r\n\t\tEventManager.register(EventType.WeChatLogin, this, this.doWXLogin);\r\n\t\tthis.eventInited = true;\r\n\t}\r\n\t/**\r\n\t * login-快速登录\r\n\t */\r\n\tprivate fastLogin() {\r\n\t\t//先获取init5\r\n\t\tLoginModel.getInit5(this, (suc: boolean, status?: number) => {\r\n\t\t\tif (suc) {\r\n\t\t\t\tLoginModel.accountLogin(this.cacheInfo, this, this.accountLoginResult);\r\n\t\t\t}\r\n\t\t});\r\n\t}\r\n\tprivate accountLoginResult(suc: boolean) {\r\n\t\tif (suc) {\r\n\t\t\tGameMgr.initLobby();\r\n\t\t} else {//需要验证码\r\n\t\t\tAccountLoginDlg.show(this.cacheInfo);\r\n\t\t}\r\n\t}\r\n\t//创建游客账号结果返回\r\n\tprivate creatVisitorResult(vo: LoginVo, isNew?: boolean) {\r\n\t\tif (vo && vo.user && vo.pwd) {\r\n\t\t\tthis.doVisitorLogin(vo, isNew);\r\n\t\t} else {//创建游客账号失败\r\n\t\t\tif (vo && vo.user) {//如果有用户名没有密码，这种情况是解绑手机后创建游客时后端使用了当前解绑的手机做为游客，这个时候后端没法返回密码，先临时解决，后期产品重现规划todo\r\n\t\t\t\tlet lastvo: LoginVo = SaveManager.getObj().get(SaveManager.KEY_LASTLOGININFO, null);\r\n\t\t\t\tif (!lastvo) lastvo = SaveManager.getObj().get(SaveManager.KEY_VISTORINFO, null);\r\n\t\t\t\tif (lastvo && lastvo.user == vo.user) {\r\n\t\t\t\t\tvo.pwd = lastvo.pwd;\r\n\t\t\t\t\tthis.doVisitorLogin(vo, isNew);\r\n\t\t\t\t\tDebug.log(\"warn:使用解绑后的账户进行游客登录\");\r\n\t\t\t\t} else {//跳转到账号登录界面\r\n\t\t\t\t\tAccountLoginDlg.show(null, vo.user);\r\n\t\t\t\t\tToast.showToast(\"手机号已解绑，为了您的安全考虑，请输入密码登录\");\r\n\t\t\t\t}\r\n\t\t\t} else {\r\n\t\t\t\tDebug.error(\"创建游客账号失败\", vo, isNew);\r\n\t\t\t\tToast.showToast(\"游客创建失败\");\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n\t/**\r\n\t * login-游客登录\r\n\t * @param vo \r\n\t */\r\n\tprivate doVisitorLogin(vo: LoginVo, isNew?: boolean) {\r\n\t\t//先获取init5\r\n\t\tLoginModel.getInit5(this, (suc: boolean, status?: number) => {\r\n\t\t\tif (suc) {\r\n\t\t\t\tLoginModel.visitorLogin(vo, isNew);\r\n\t\t\t}\r\n\t\t});\r\n\t}\r\n\t/**\r\n\t * login-微信登录\r\n\t * @param obj \r\n\t */\r\n\tprivate doWXLogin(obj: any) {\r\n\t\tif (!obj.data) {\r\n\t\t\tDebug.error(\"微信登录数据获取失败:\", obj);\r\n\t\t\treturn;\r\n\t\t}\r\n\t\tif (!obj.data.uid) {\r\n\t\t\tToast.showToast(\"微信ID获取失败\");\r\n\t\t\treturn;\r\n\t\t}\r\n\t\t//先获取init5\r\n\t\tLoginModel.getInit5(this, (
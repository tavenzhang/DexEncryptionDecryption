      window.top.postMessage(msg, \"*\");\r\n            if (AppData.IS_NATIVE_APP) {\r\n                  window.top.postMessage(JSON.stringify({ action: \"JumpGame\", ...data }), \"*\");\r\n            }\r\n      }\r\n\r\n      public static jumpToUrl(data = {}) {\r\n            if (isNative) return;\r\n            window.top.postMessage(JSON.stringify({ action: \"JumpUrl\", ...data }), \"*\");\r\n      }\r\n\r\n      public static goBack(data = {}) {\r\n            if (isNative) return;\r\n            window.top.postMessage(JSON.stringify({ action: \"game_back\", ...data }), \"*\");\r\n      }\r\n\r\n      public static game_recharge(data = {}) {\r\n            if (isNative) return;\r\n            window.top.postMessage(JSON.stringify({ action: \"game_recharge\", ...data }), \"*\");\r\n      }\r\n\r\n      public static game_redraw(data = {}) {\r\n            if (isNative) return;\r\n            window.top.postMessage(JSON.stringify({ action: \"game_redraw\", ...data }), \"*\");\r\n      }\r\n\r\n      public static game_account(data = {}) {\r\n            if (isNative) return;\r\n            window.top.postMessage(JSON.stringify({ action: \"game_account\", ...data }), \"*\");\r\n\r\n      }\r\n\r\n      public static game_custom(data = {}) {\r\n            if (isNative) return;\r\n            window.top.postMessage(JSON.stringify({ action: \"game_custom\", ...data }), \"*\");\r\n      }\r\n\r\n      public static game_Http(data = {}) {\r\n            if (isNative) return;\r\n            window.top.postMessage(JSON.stringify({ action: \"http\", ...data }), \"*\");\r\n      }\r\n\r\n      public static debugInfo(data = {}) {\r\n            if (isNative) return;\r\n            window.top.postMessage(JSON.stringify({ action: \"debugInfo\", ...data }), \"*\");\r\n      }\r\n\r\n      public static game_share(data = {}) {\r\n            if (isNative) return;\r\n            window.top.postMessage(JSON.stringify({ action: \"game_share\", ...data }), \"*\");\r\n      }\r\n\r\n      //添加通用action 处理 免得越来越多\r\n      public static game_common(data = {}) {\r\n            if (isNative) return;\r\n            window.top.postMessage(JSON.stringify({ action: \"game_common\", ...data }), \"*\");\r\n      }\r\n      //通知loading显示游戏内容\r\n      public static initGame() {\r\n            if (isNative) return;\r\n            window.top.postMessage(JSON.stringify({ action: \"showGame\" }), \"*\");\r\n      }\r\n      /**\r\n       * 跳转三方游戏\r\n       * @param data \r\n       */\r\n      public static jumpOtherGame(data: any, vo: GameIconVo) {\r\n            if (isNative) return;\r\n            if (vo.vertical) {\r\n                  var b = new MyBase64();\r\n                  let url = b.decode(data);\r\n                  GameUtils.openAppWeb(url, vo.alias);\r\n            } else {\r\n                  window.top.postMessage(JSON.stringify({ action: \"JumpThirdGame\", data: data }), \"*\");\r\n            }\r\n      }\r\n      //判断是否使用内嵌网页打开\r\n      private static checkAppWeb(): string {\r\n            let id = LobbyModel.menuVo.gameId;\r\n            let type: string = null;\r\n            switch (id) {\r\n                  case GameMenuType.lottery.toString(): type = \"cp\"; break;\r\n                  case GameMenuType.realPerson.toString(): type = \"zr\"; break;\r\n            }\r\n            return type;\r\n      }\r\n}","import { ConfObjRead } from \"../common/ConfObjRead\";\r\nimport { Debug } from \"../common/Debug\";\r\nimport { MyUid } from \"../common/MyUid\";\r\nimport { PostMHelp } from \"../common/PostMHelp\";\r\nimport { Tools } from \"../common/Tools\";\r\nimport { DlgCmd } from \"../data/GameVos\";\r\nimport { HttpRequester } from \"../data/HttpRequester\";\r\nimport { LoginMethod, LoginModel } from \"../data/LoginModel\";\r\nimport { ResConfig } from \"../data/ResConfig\";\r\nimport { SystemDlg } from \"../view/dlg/SystemDlg\";\r\nimport { TipsDlg } from '../view/dlg/TipsDlg';\r\nimport { LoadingView } from '../view/LoadingView';\r\nimport { PageManager } from \"../view/PageManager\";\r\nimport { Toast } from \"../view/toast/Toast\";\r\nimport { GameMgr } from './GameMgr';\r\nimport { SoundPlayer } from './SoundPlayer';\r\n\r\n/*\r\n* name;\r\n*/\r\nexport class GameUtils {\r\n    public static outTime = 15000;//请求超时时间\r\n    //最小和最大间隔(用于需要全屏适配的ui)\r\n    public static minGap: number = 28;\r\n    public static maxGap: number = 78;//安全边距\r\n    public static get scaleX(): number {\r\n        return Laya.stage.width / 1334;\r\n    }\r\n    /**\r\n     * 获取位置偏移量\r\n     */\r\n    public static get posOffset(): number {\r\n        var width: number = 0;\r\n        //获得屏幕的长宽比\r\n        var bi = Laya.stage.width / Laya.stage.height;\r\n        if (bi < 1.777778) {\r\n            width = this.minGap;\r\n        }\r\n        else if (bi > 2.165333) {\r\n            width = this.maxGap;\r\n        }\r\n        else {\r\n            width = (bi - 1.777778) * (this.maxGap - this.minGap) / (2.165333 - 1.777778) + this.minGap;\r\n        }\r\n        return width;\r\n    }\r\n    /**\r\n     * 获取不同手机端分辨率的位置偏移量(后续将废弃posOffset)\r\n     * @param min \r\n     * @param max \r\n     */\r\n    public static getScreencOffset(min: number, max: number): number {\r\n        var width: number = 0;\r\n        //获得屏幕的长宽比\r\n        var scl = Laya.stage.width / Laya.stage.height;\r\n        let minScl = 1.778666;\r\n        let maxScl = 2.165333;\r\n        if (scl <= minScl) {\r\n            width = min;\r\n        }\r\n        else if (scl >= maxScl) {\r\n            width = max;\r\n        }\r\n        else {\r\n            width = (scl - minScl) * (max - min) / (maxScl - minScl) + min;\r\n        }\r\n        return width;\r\n    }\r\n    /**\r\n     * 限制num的取值区间\r\n     * @param cur \r\n     * @param min \r\n     * @param max \r\n     */\r\n    public static borderValue(cur: number, min: number, max: number): number {\r\n        if (cur > max) cur = max;\r\n        else if (cur < min) cur = min;\r\n        return cur;\r\n    }\r\n    /**\r\n     * 判断是否运行在nativeApp\r\n     */\r\n    public static get isNativeApp(): boolean {\r\n        return AppData.IS_NATIVE_APP || Debug.isapp;\r\n    }\r\n    /**\r\n     * 获取设备号\r\n     */\r\n    public static get deviceToken(): string {\r\n        let uid = MyUid.getUid();\r\n        let device: string;\r\n        if (this.isNativeApp) device = AppData.deviceToken;\r\n        else device = MyUid.getUid();\r\n        if ((!device || device.length < 5) && this.isNativeApp) {\r\n            Debug.error(\"deviceToken获取异常,data=\", AppData);\r\n            HttpRequester.addLog(\"deviceToken获取异常,data=\" + AppData);\r\n        }\r\n        return device;\r\n    }\r\n    /**\r\n     * 获取邀请码\r\n     */\r\n    public static get affcode(): string {\r\n        return this.isNativeApp ? AppData.affCode : \"\";\r\n    }\r\n    /**\r\n     * 加密密码\r\n     * @param pwd \r\n     */\r\n    public static encryptPwd(pwd: string): string {\r\n        return window['SecretUtils'].rsaEncodePWD(pwd);\r\n    }\r\n    /**\r\n     * 判断是否native播放背景音乐\r\n     */\r\n    public static get isAppSound(): boolean {\r\n        return AppData.isAppSound;\r\n    }\r\n    /**\r\n     * 获取app版本号\r\n     */\r\n    public static get appVer(): string {\r\n        return \"App \" + AppData.appVersion;\r\n    }\r\n    /**\r\n     * 检查字符串是否为空\r\n     * @param str \r\n     * @param hint \r\n     */\r\n    public static checkStr(str: string, hint: string = null): boolean {\r\n        if (!str || str == \"\") {\r\n            if (hint) Toast.showToast(hint);\r\n            return false;\r\n        }\r\n        return true;\r\n    }\r\n    /**\r\n     * 给密码输入框添加切换事件\r\n     * @param input \r\n     * @param cbox \r\n     */\r\n    public static onChangePwd(input: Laya.TextInput, cbox: Laya.CheckBox) {\r\n        cbox.on(Laya.Event.CHANGE, this, () => {\r\n            SoundPlayer.clickSound();\r\n            GameUtils.onShowPwd(input);\r\n        });\r\n    }\r\n    /**\r\n     * 显示或者隐藏密码\r\n     */\r\n    public static onShowPwd(pwd: Laya.TextInput) {\r\n        switch (pwd.type) {\r\n            case 'text':\r\n                pwd.type = 'password';\r\n                break;\r\n            case 'password':\r\n                pwd.type = 'text';\r\n                break;\r\n        }\r\n\r\n        pwd.focus = true;\r\n        //为了兼容自定义键盘(如果不这样，密码和明文切换后在没失去焦点前没法弹出自定义键盘)\r\n        Laya.timer.once(100, this, () => {//100\r\n            pwd.focus = false;\r\n        })\r\n    }\r\n    //添加长按事件(用于调试)\r\n    public static addLongPress(obj: Laya.Sprite, caller: any, callback: Function) {\r\n        let downFun = () => {\r\n            obj.on(Laya.Event.MOUSE_UP, this, upFun);\r\n            obj.on(Laya.Event.MOUSE_OUT, this, upFun);\r\n            Laya.timer.once(2000, this, callFun);\r\n        };\r\n        let upFun = () => {\r\n            obj.off(Laya.Event.MOUSE_UP, this, upFun);\r\n            obj.off(Laya.Event.MOUSE_OUT, this, upFun);\r\n            Laya.timer.clear(this, callFun);\r\n        }\r\n        let callFun = () => {\r\n            if (caller && callback) {\r\n                callback.call(caller);\r\n                if (obj) obj.off(Laya.Event.MOUSE_DOWN, this, downFun);\r\n            }\r\n        }\r\n        obj.on(Laya.Event.MOUSE_DOWN, this, downFun);\r\n    }\r\n    /**\r\n     * 保存图片\r\n     * @param sp \r\n     * @param fileName \r\n     * @param type \r\n     */\r\n    public static saveImage(sp: Laya.Sprite, fileName: string = null, type: string = \"png\") {\r\n        let iw: number = 60;\r\n        let box: Laya.Sprite = new Laya.Sprite();\r\n        box.graphics.drawRect(0, 0, sp.width + iw, sp.height + iw, \"#ffffff\");\r\n        box.size(sp.width + iw, sp.height + iw);\r\n        box.addChild(sp);\r\n        sp.pos(box.width - sp.width >> 1, box.height - sp.height >> 1);\r\n\r\n        let htmlC: Laya.HTMLCanvas = box.drawToCanvas(box.width, box.height, 0, 0);\r\n        let base64: string = htmlC.toBase64(\"image/\" + type, 0.85);\r\n        PostMHelp.game_common({ do: \"saveImage\", param: base64 });\r\n    }\r\n    //----------http超时相关\r\n    private static timeObj: any = { abc: 0 };\r\n    public static addTimeOut(url: string, caller?: any, callback?: Function, outTimeCallback?: boolean) {\r\n        let tween = Laya.Tween.to(this.timeObj, { abc: 1 }, this.outTime, Laya.Ease.linearNone, Laya.Handler.create(this, this.timeOut, [url, caller, callback, outTimeCallback]));\r\n        this.timeObj[url] = tween;\r\n    }\r\n    private static timeOut(url: string, caller?: any, callback?: Function, outTimeCallback?: boolean) {\r\n        delete this.timeObj[url];\r\n        for (let item of HttpRequester.httpRequestList) {\r\n            if (item && item.hashUrl == url) {\r\n                let index = HttpRequester.httpRequestList.indexOf(item);\r\n                HttpRequester.httpRequestList.splice(index, 1);\r\n                if (LoadingView.isLoading) {\r\n                    GameMgr.showCircleLoading(false);\r\n                    Toast.showToast(\"网络请求超时,请稍后再试\");\r\n                    Debug.error(\"timeOut:\", url);\r\n                    if (caller && callback && outTimeCallback) callback.call(caller, false, { timeOut: 1 });\r\n                }\r\n                return;\r\n            }\r\n        }\r\n    }\r\n    public static clearTimeout(url: string) {\r\n        let tween: Laya.Tween = this.timeObj[url];\r\n        if (tween) {\r\n            Laya.Tween.clear(tween);\r\n            delete this.timeObj[url];\r\n        }\r\n    }\r\n    /**\r\n     * 跳转网页\r\n     * @param url \r\n     */\r\n    public static openWeb(url: string) {\r\n        if (GameUtils.isNativeApp) PostMHelp.game_common({ name: \"openWeb\", param: url });\r\n        else window.open(url);\r\n    }\r\n    //app内嵌网页\r\n    public static openAppWeb(url: string, type?: string) {\r\n        if (GameUtils.isNativeApp) PostMHelp.game_common({ name: \"openAppWeb\", param: url, type: type });\r\n        else window.open(url);\r\n    }\r\n}\r\n/**\r\n * 格式化工具\r\n */\r\nexport class FormatTool {\r\n    /**\r\n     * 千分位分割，例如：$12,456,789\r\n     * @param  money 数字\r\n     * @param  head 数字头标识\r\n     * @param  separator 间隔符号\r\n     * @param  number 间隔位数\r\n     */\r\n    public static dollarFormat(money: number, head: string = \"$\", separator: string = \",\", number: number = 3): string {\r\n        if (money == undefined) return;\r\n        if (money < 0) {\r\n            head += \"-\";\r\n            money = -money;\r\n        }\r\n        let str: string = money.toString();\r\n        let arr: string[] = str.split(\".\");\r\n        let inter: string = arr[0];\r\n        let pot: string = arr.length > 1 ? \".\" + arr[1] : \"\";\r\n        if (inter.length <= number) {\r\n            return head + inter + pot;\r\n        }\r\n        let num: number = inter.length % number;\r\n        if (num != 0) {\r\n            head += inter.substr(0, num) + separator;\r\n        }\r\n        let reg: RegExp = new RegExp('\\\\d{' + number + '}', 'g');\r\n        inter = inter.substr(num).replace(reg, '$&' + separator);\r\n        return head + inter.substr(0, inter.length - 1) + pot;\r\n    }\r\n    /**\r\n     * 将跑马灯文本解析为html富文本\r\n     * @param str \r\n     */\r\n    public static parseHtmlFormat(str: string): string {\r\n        if (!ResConfig.gameRoomFont) ResConfig.gameRoomFont = ConfObjRead.getConfCommon().paomadengConfig;\r\n\r\n        let html: string = str;\r\n        let strArr: string[];\r\n        //标签标记:玩家名，游戏名，房间名，金币\r\n        let nameLabel = \"@\", gameLabel = \"^\", roomLabel = \"$\", goldLabel = \"%\";\r\n\r\n        //解析玩家名\r\n        if (this.checkLabel(nameLabel, html)) {\r\n            strArr = html.split(nameLabel);\r\n            html = strArr[0] + \"<span style='color:#36ff07;font-weight:bold;'>\" + this.space + strArr[1] + this.space + \"</span>\" + strArr[2];\r\n        }\r\n\r\n        //解析游戏名\r\n        if (this.checkLabel(gameLabel, html)) {\r\n            strArr = html.split(gameLabel);\r\n            let txt: string = strArr[1];\r\n            let bitUrl: string = ResConfig.gameRoomFont[txt];\r\n            if (bitUrl) txt = \"<img src=\" + this.addQuotes(bitUrl) + \"></img>\";\r\n            else txt = \"<span style='color:#FCD937;font-weight:bold;'>\" + txt + \"</span>\";\r\n            html = strArr[0] + this.space + txt + strArr[2] || \"\";\r\n        }\r\n\r\n        //解析房间名\r\n        if (this.checkLabel(roomLabel, html)) {\r\n            strArr = html.split(roomLabel);\r\n            let txt: string = strArr[1];\r\n            let bitUrl: string = ResConfig.gameRoomFont[txt];\r\n            if (bitUrl) txt = \"<img src=\" + this.addQuotes(bitUrl) + \"></img>\";\r\n            else txt = \"<span style='color:#F8BF87;font-weight:bold;'>\" + txt + \"</span>\";\r\n            html = strArr[0] + txt + this.space + strArr[2] || \"\";\r\n        }\r\n\r\n        //解析金币\r\n        if (this.checkLabel(goldLabel, html)) {\r\n            strArr = html.split(goldLabel);\r\n            let goldStr: string = \"\";\r\n            // let goldArr = strArr[1].split(\"\");\r\n            // goldArr.forEach(value => {//位图字体\r\n            //     goldStr += \"<img src=\" + this.addQuotes(ResConfig.bitFont_notice[value]) + \"></img>\";\r\n            // });\r\n            //直接用富文本字体\r\n            goldStr = \"<span style='color:#FFED72;font-weight:bold;fontSize:26'>\" + strArr[1] + \"</span>\";\r\n            html = strArr[0] + this.space + \"<img src='ui/paomadeng/goldIcon.png' width='26' height='26'></img>\" + goldStr + this.space + strArr[2] || \"\";\r\n        }\r\n        return html;\r\n    }\r\n    //检测标签\r\n    private static checkLabel(label: string, target: string): boolean {\r\n        let start = target.indexOf(label);\r\n        let end = target.lastIndexOf(label);\r\n        if (start != -1 && end != -1) return true;\r\n        return false;\r\n    }\r\n    //给字符串加单引号\r\n    public static addQuotes(str: string): string {\r\n        return '\\'' + str + '\\'';\r\n    }\r\n    //获取html格式空格符\r\n    public static get space(): string {\r\n        return \"&nbsp;\";\r\n    }\r\n}\r\n/**\r\n * 加载工具\r\n */\r\nexport class LoadTool {\r\n    /**\r\n     * 加载图片\r\n     * @param img \r\n     * @param url \r\n     * @param width 指定的宽度\r\n     * @param height 指定的高度\r\n     * @param ratioScale 是否等比缩放(否则强制拉伸)\r\n     */\r\n    public static loadImage(img: Laya.Image, url: string, width?: number, height?: number, ratioScale: boolean = true) {\r\n        if (!img || !url) return;\r\n        let texture: Laya.Texture = Laya.loader.getRes(url);\r\n        let scl: number = 1;\r\n        if (texture) {\r\n            img.skin = url;\r\n            if (ratioScale) {\r\n                scl = Math.min(width / texture.width, height / texture.height);\r\n                img.scale(scl, scl);\r\n            } else {\r\n                img.scale(width / texture.width, height / texture.height);\r\n            }\r\n            img.name = \"cache\";\r\n        } else {\r\n            Laya.loader.load(url, Laya.Handler.create(this, () => {\r\n                if (!img || img.destroyed) return;\r\n                if (img.name == \"cache\") return;//防止img对象使用缓存时导致后续的覆盖\r\n                if (img.dataSource && img.dataSource != url) return;//防止img对象使用缓存时导致后续的覆盖\r\n                texture = Laya.loader.getRes(url);\r\n                if (!texture) {\r\n                    Debug.error(\"texture获取异常:\", url);\r\n                    return;\r\n                }\r\n                img.skin = url;\r\n                if (ratioScale) {\r\n                    scl = Math.min(width / texture.width, height / texture.height);\r\n                    img.scale(scl, scl);\r\n                } else {\r\n                    img.scale(width / texture.width, height / texture.height);\r\n                }\r\n            }), null, Laya.Loader.IMAGE);\r\n        }\r\n    }\r\n}\r\nexport class InnerJumpUtil {\r\n    /**\r\n     * 游戏内部跳转\r\n     * 注意：弹窗后续走的是动态加载，统一入口为PageManager.showDlg,需要注意相关配置和入口调用\r\n     * @param cmd \r\n     */\r\n    public static doJump(cmd: DlgCmd) {\r\n        if (cmd == undefined) return;\r\n        switch (cmd) {\r\n            case DlgCmd.activityCenter:\r\n            case DlgCmd.noticeCenter: {\r\n                PageManager.showDlg(cmd, cmd);\r\n                break;\r\n            }\r\n            case DlgCmd.agentCenter:\r\n            case DlgCmd.email:\r\n            case DlgCmd.personCenter: {\r\n                PageManager.showDlg(cmd);\r\n                break;\r\n            } case DlgCmd.recharge: {//充值界面\r\n                if (LoginModel.loginType != LoginMethod.account) {\r\n                    SystemDlg.show(\"温馨提示：建议先绑定手机号，避免账号丢失\",\r\n                        {\r\n                            color: \"#ffba25\",\r\n                            size: 30,\r\n                            leftBtnSkin: \"ui/common/img_anniu_jxcz.png\",\r\n                            rightBtnSkin: \"ui/common/img_anniu_ljbd.png\",\r\n                            caller: this,\r\n                            leftBtnCallback: () => {//继续充值\r\n                                Tools.jump2module(ConfObjRead.getConfUrl().url.g_recharge, \"recharge\");\r\n                            },\r\n                            rightBtnCallback: () => {//立即绑定\r\n                                PageManager.showDlg(DlgCmd.bindPhone);\r\n                            }\r\n                        }\r\n                    );\r\n                    return;\r\n                }\r\n                Tools.jump2module(ConfObjRead.getConfUrl().url.g_recharge, \"recharge\");\r\n                break;\r\n            } case DlgCmd.service: {//客服\r\n                Tools.jump2module(ConfObjRead.getConfUrl().url.g_custom, \"custom\");\r\n                break;\r\n            } case DlgCmd.withdraw: {//提现\r\n                if (LoginModel.loginType != LoginMethod.account) {\r\n                    TipsDlg.show(\"亲爱的老板\\n使用提现功能需要先升级为正式账号哦~\", this, this.openAccountUpgrade);\r\n                    return;\r\n                }\r\n                Tools.jump2module(ConfObjRead.getConfUrl().url.g_redraw, \"redraw\");\r\n                break;\r\n            }\r\n        }\r\n    }\r\n    private static openAccountUpgrade() {\r\n        PageManager.showDlg(DlgCmd.bindPhone);\r\n    }\r\n}\r\n\r\n/**\r\n * gtest图像验证\r\n */\r\nexport class GtestTool {\r\n    //图像验证背景,用于替代js原生背景(因为原生背景点击后会消失)\r\n    private static gbg: Laya.Sprite;\r\n    /**\r\n     * gtest验证\r\n     */\r\n    public static getGtestInfo(caller?: any, callback?: Function) {\r\n        let url = ConfObjRead.apihome + ConfObjRead.httpCmd.gtest;\r\n        let header = [\"Accept\", \"*/*\"];\r\n\r\n        GameMgr.showCircleLoading();\r\n\r\n        HttpRequester.doRequest(url, header, null, this, (suc: boolean, jobj: any) => {\r\n            Debug.log(\"startCaptcha:\", true);\r\n            if (suc) {\r\n                /**\r\n                 * 以屏幕分辨率的高为414,gtest宽为300来计算，让gtest界面在各种分辨率下都能显示全\r\n                 * 通过调整refer来改变gtest界面在屏幕中的上下间隔，值越大其间隔越小\r\n                 */\r\n                let screenScl = Laya.Browser.clientHeight / 414;\r\n                let refer = 306;\r\n                let n_w = Math.floor(refer * screenScl);\r\n                initGeetest({\r\n                    //必须参数\r\n                    gt: jobj.gt,\r\n                    challenge: jobj.challenge,\r\n                    offline: !jobj.success,\r\n                    new_captcha: true,\r\n                    //可选参数\r\n                    product: \"bind\", //隐藏验证按钮(或者注释掉captchaObj.appendTo也可以)\r\n                    next_width: n_w + \"px\",//\r\n                }, (captchaObj) => {\r\n                    // Debug.error(\"init-gt\", captchaObj);\r\n                    captchaObj.onReady(() => {\r\n                        GameMgr.showCircleLoading(false);\r\n                        Debug.log(\"gt-onReady\");\r\n                        captchaObj.verify();//主动显示验证图\r\n\r\n                        let boxarr = document.getElementsByClassName('geetest_panel');//获取父容器\r\n                        if (boxarr) {\r\n                            const arr = document.getElementsByClassName('geetest_panel_ghost');//验证图背景\r\n                            if (arr && arr[0]) {//删除原生js背景\r\n                                this.showGbg();\r\n                                boxarr[boxarr.length - 1].removeChild(arr[0]);\r\n                                Debug.log(\"remove-gbg\");\r\n                            }\r\n                        }\r\n                    }).onSuccess(() => {\r\n                        let data: { geetest_challenge, geetest_seccode, geetest_validate } = captchaObj.getValidate();\r\n                        Debug.log(\"gt-onSuccess:\", data);\r\n                        this.clearGtestDiv();\r\n                        if (caller && callback) callback.call(caller, true, data);\r\n                    }).onError(() => {\r\n                        Debug.error(\"gt-onError\");\r\n                        if (caller && callback) callback.call(caller, false);\r\n                    }).onClose(() => {\r\n                        Debug.log(\"gt-onClose\");\r\n                        this.clearGtestDiv();\r\n                        if (caller && callback) callback.call(caller, false);\r\n                    })\r\n                });\r\n            } else {\r\n                GameMgr.showCircleLoading(false);\r\n            }\r\n        }, \"get\");\r\n    }\r\n    //显示自定义背景\r\n    private static showGbg() {\r\n        if (!this.gbg) {\r\n            this.gbg = new Laya.Sprite();\r\n            this.gbg.graphics.drawRect(0, 0, Laya.stage.width, Laya.stage.height, \"#000000\");\r\n            this.gbg.alpha = 0.6;\r\n            this.gbg.zOrder = Laya.Dialog.manager.zOrder + 12;\r\n        }\r\n        Laya.stage.addChild(this.gbg);\r\n    }\r\n    private static clearGtestDiv() {\r\n        if (this.gbg) t
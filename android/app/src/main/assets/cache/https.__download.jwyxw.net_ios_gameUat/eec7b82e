imer();\r\n\t\t\t\t} else {\r\n\t\t\t\t\tthis.startRequestTimer();\r\n\t\t\t\t}\r\n\t\t\t} else {\r\n\t\t\t\tthis.startRequestTimer();\r\n\t\t\t}\r\n\t\t}, \"get\");\r\n\t}\r\n\t//添加消息队列\r\n\tprivate addMsgItems(list: any[]) {\r\n\t\tlist.forEach(value => {\r\n\t\t\tif (value.notice) this.msgList.push(value.notice);\r\n\t\t});\r\n\r\n\t\tif (this.msgList.length > 0) {\r\n\t\t\tthis.divTxt.innerHTML = FormatTool.parseHtmlFormat(this.msgList.shift());\r\n\t\t\tthis.divTxt.x = this.msgSp.width;\r\n\t\t\tthis.startRollingTimer();\r\n\t\t}\r\n\t}\r\n\t//启动数据请求计时器\r\n\tprivate startRequestTimer() {\r\n\t\tif (this.runTimerReq) return;\r\n\t\tLaya.timer.once(10000, this, this.requestRollInfo);\r\n\t\tthis.runTimerReq = true;\r\n\t}\r\n\tprivate stopRequestTimer() {\r\n\t\tif (this.runTimerReq) {\r\n\t\t\tLaya.timer.clear(this, this.requestRollInfo);\r\n\t\t\tthis.runTimerReq = false;\r\n\t\t}\r\n\t}\r\n\t//启动消息滚动计时器\r\n\tprivate startRollingTimer() {\r\n\t\tif (this.runTimerRoll) return;\r\n\t\tLaya.timer.frameLoop(1, this, this.doRoll);\r\n\t\tthis.runTimerRoll = true;\r\n\t}\r\n\tprivate stopRollingTimer() {\r\n\t\tif (this.runTimerRoll) {\r\n\t\t\tLaya.timer.clear(this, this.doRoll);\r\n\t\t\tthis.runTimerRoll = false;\r\n\t\t}\r\n\t}\r\n\tprivate doRoll() {\r\n\t\tthis.divTxt.x -= 2;\r\n\t\tif (this.divTxt.x <= -this.divTxt.contextWidth) {\r\n\t\t\tif (this.msgList.length > 0) {\r\n\t\t\t\tthis.divTxt.innerHTML = FormatTool.parseHtmlFormat(this.msgList.shift());\r\n\t\t\t\tthis.divTxt.x = this.msgSp.width;\r\n\t\t\t} else {\r\n\t\t\t\tthis.startRequestTimer();\r\n\t\t\t\tthis.stopRollingTimer();\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n\t/**\r\n\t * 销毁\r\n\t */\r\n\tpublic destroy() {\r\n\t\tthis.stopRequestTimer();\r\n\t\tthis.stopRollingTimer();\r\n\t\tsuper.destroy(true);\r\n\t}\r\n}\r\n","import { BitmapFont } from \"../../common/BitmapFont\";\r\nimport { Common } from \"../../common/Common\";\r\nimport { Tools } from \"../../common/Tools\";\r\nimport { DlgCmd } from \"../../
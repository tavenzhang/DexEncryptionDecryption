* @param data \r\n     * @param caller \r\n     * @param callback \r\n     * @param encryHeader 是否使用加密头(需要加密的接口设为true)\r\n     */\r\n    public static postHttpData(cmd: string, data: any = null, caller: any, callback: Function, encryHeader?: boolean, urlParams?: string) {\r\n        let url = ConfObjRead.apihome + cmd;\r\n        if (Common.access_token) url += \"?access_token=\" + Common.access_token;\r\n        if (urlParams) url += urlParams;\r\n\r\n        let jsonStr = null;\r\n        if (data) jsonStr = JSON.stringify(data);\r\n\r\n        let header = [\"Content-Type\", \"application/json; charset=utf-8\", \"Accept\", \"*/*\"];\r\n        if (encryHeader) header = this.getEncryHeader();\r\n        else {\r\n            if (Common.access_token) header.push(\"Authorization\", \"bearer \" + Common.access_token);\r\n        }\r\n\r\n        this.doRequest(url, header, jsonStr, caller, callback, \"post\");\r\n    }\r\n\r\n    /**\r\n     * 通用delete方式请求\r\n     * @param cmd \r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static deleteHttpData(cmd: string, caller: any, callback: Function) {\r\n        let url = ConfObjRead.getConfUrl().url.apihome;\r\n        url += cmd;\r\n        if (Common.access_token) url += \"?access_token=\" + Common.access_token;\r\n\r\n        let header = [\"Accept\", \"application/json\"];\r\n        this.doRequest(url, header, null, caller, callback, \"delete\");\r\n    }\r\n    /**\r\n     * 通用put方式请求\r\n     * @param cmd \r\n     * @param data \r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static putHttpData(cmd: string, data: any = null, caller: any, callback: Function) {\r\n        let url = ConfObjRead.getConfUrl().url.apihome;\r\n        url += cmd;\r\n        if (Common.access_token) url += \"?access_token=\" + Common.access_token;\r\n\r\n        let jsonStr = null;\r\n        if (data) jsonStr = JSON.stringify(data);\r\n\r\n        let header = [\"Content-Type\", \"application/json; charset=utf-8\", \"Accept\", \"*/*\"];\r\n        if (Common.access_token) header.push(\"Authorization\", \"bearer \" + Common.access_token);\r\n        this.doRequest(url, header, jsonStr, caller, callback, \"put\");\r\n    }\r\n    /**\r\n     * 开始请求-后续统一走这里方面定位问题\r\n     * @param url \r\n     * @param header \r\n     * @param jsonStr \r\n     * @param caller \r\n     * @param callback \r\n     * @param method \r\n     */\r\n    public static doRequest(url: string, header: any, jsonStr: any, caller?: any, callback?: Function, method: string = \"post\", outTimeCallback?: boolean) {\r\n        this.httpConnect(\r\n            url,\r\n            this,\r\n            (s: any, stat: string, hr: any) => {\r\n                let suc: boolean = false;\r\n                let jobj: any;\r\n                if (stat == \"complete\") {\r\n                    try {\r\n                        jobj = JSON.parse(s);\r\n                    } catch (e) {\r\n                        jobj = s;\r\n                    }\r\n                    if (!jobj) jobj = hr;//兼容数据不是json格式的情况\r\n                    suc = true;\r\n                } else {\r\n                    let status = hr.http.status;\r\n                    jobj = hr;\r\n                    if (!status) {//断网情况\r\n                        suc = false;\r\n                        Toast.showToast(\"当前网络异常，请检查您的网络环境。\");\r\n                        Debug.error(\"request-err:\", \"\\n--->url:\" + url, \"\\n--->header:\", header, \"\\n--->params:\", jsonStr, \"\\n--->http:\", hr.http);\r\n                    } else {\r\n                        if (status >= 200 && status < 300) {//后端认为此范围为成功\r\n                            suc = true;\r\n                        } else {\r\n                            Debug.error(\"request-err:\", \"\\n--->url:\" + url, \"\\n--->header:\", header, \"\\n--->params:\", jsonStr, \"\\n--->http:\", hr.http);\r\n                            if (status == 401) {\r\n                                GameMgr.loginOut(true);\r\n                                Toast.showToast(\"登录过期,请重新登录\");\r\n                                GameMgr.showCircleLoading(false);\r\n                                return;\r\n                            }\r\n                            let response = hr.http.response;\r\n                            try {\r\n                                let obj = JSON.parse(response);\r\n                                Toast.showToast(obj.message || (\"未知错误:\" + status));\r\n                            } catch (e) {\r\n                                Toast.showToast(\"未知错误:\" + status);\r\n                            }\r\n                        }\r\n                    }\r\n                }\r\n\r\n                if (caller && callback) callback.apply(caller, [suc, jobj, hr]);\r\n            },\r\n            header, jsonStr, method, \"json\");\r\n    }\r\n    /**\r\n     * 开始请求\r\n     * @param urls \r\n     * @param caller \r\n     * @param callback \r\n     * @param header \r\n     * @param data \r\n     * @param metod \r\n     * @param restype \r\n     * @param outTimeCallback 请求超时后是否执行回调通知\r\n     */\r\n    public static httpConnect(urls: string, caller: any, callback: any, header: any = null, data: any = null, metod: any = \"get\", restype: any = \"json\", outTimeCallback?: boolean) {\r\n        let url = urls ? urls : \"\";\r\n        let hashUrl = url + \"_\" + JSON.stringify(header ? header : {}) + \"_\" + JSON.stringify(data ? data : {});\r\n        for (let item of this.httpRequestList) {\r\n            if (item && item.hashUrl == hashUrl) {\r\n                return;\r\n            }\r\n        }\r\n\r\n        //针对cdnurl接口特殊处理,因为这个接口的数据在response中,和app那边的json数据模式不兼容\r\n        let cndId = urls.indexOf(\"getCdnUrl\");\r\n        //app 使用本地app 代理请求 网页使用原来的\r\n        if (AppData.IS_NATIVE_APP && cndId == -1) {\r\n            let httpData = { hashUrl, caller, callback };\r\n            this.httpRequestList.push(httpData);\r\n            PostMHelp.game_Http({ url, header, data, metod, restype, hashUrl });\r\n            if (LoadingView.isLoading) {\r\n                GameUtils.addTimeOut(hashUrl, caller, callback, outTimeCallback);\r\n            }\r\n        } else {\r\n            var hr: Laya.HttpRequest = new Laya.HttpRequest();\r\n            hr.once(Laya.Event.COMPLETE, this, this.httpRequestComplete, [caller, callback, hr, hashUrl]);\r\n            hr.once(Laya.Event.ERROR, this, this.httpRequestError, [caller, callback, hr, hashUrl]);\r\n\r\n            if (header) {\r\n                hr.send(url, data, metod, restype, header);\r\n            } else {\r\n                hr.send(url, null, metod, restype);\r\n            }\r\n        }\r\n    }\r\n    private static httpRequestComplete(caller: any, callback: any, hr: any, url: string, e: any): void {\r\n        callback.apply(caller, [e, 'complete', hr]);\r\n    }\r\n    private static httpRequestError(caller: any, callback: any, hr: any, url: string, e: any): void {\r\n        let status = hr.http.status;\r\n        if (status >= 200 && status < 300) {\r\n            callback.apply(caller, [e, 'complete', hr]);\r\n        } else {\r\n            callback.apply(caller, [e, 'error', hr]);\r\n        }\r\n    }\r\n    //获取加密相关的头部信息\r\n    public static getEncryHeader(): any[] {\r\n        return window['SecretUtils'].getEncryHeader(Common.gatewayInfo.tsDiff, MyUid.newUid(), Common.gatewayInfo.sid, GameUtils.deviceToken);\r\n    }\r\n}","/**\r\n * 事件管理器\r\n * 用于添加触摸缩放按钮效果和全局事件管理以及系统事件\r\n */\r\nexport class EventManager {\r\n    private static _inst: EventManager;\r\n    private pool: any = {};\r\n    private pushPool: any = {};\r\n    private downPos: Laya.Point = new Laya.Point();\r\n    /**\r\n     * 全局事件派发器\r\n     */\r\n    private static eventer: Laya.EventDispatcher = new Laya.EventDispatcher();\r\n    /**\r\n     * 防止按钮重复点击的时间间隔\r\n     */\r\n    public static delayClickTime: number = 500;\r\n\r\n    public static get inst(): EventManager {\r\n        if (!EventManager._inst) {\r\n            EventManager._inst = new EventManager();\r\n        }\r\n        return EventManager._inst;\r\n    }\r\n    /**\r\n     * 储存并监听laya引擎事件\r\n     * @param vo \r\n     */\r\n    public static pushEvent(target: Laya.Node, type: string, thisObj: any, callback: Function, args?: any[]) {\r\n        let self = EventManager.inst;\r\n        if (thisObj[\"$_GID\"] == undefined) thisObj[\"$_GID\"] = Laya.Utils.getGID();\r\n        let arr: any[] = self.pushPool[thisObj[\"$_GID\"]];\r\n        if (!arr) arr = [];\r\n        target.on(type, thisObj, callback, args);\r\n        arr.push({ target: target, type: type, thisobj: thisObj, callback: callback });\r\n        self.pushPool[thisObj[\"$_GID\"]] = arr;\r\n    }\r\n    /**\r\n     * 派发全局事件\r\n     * @param type \r\n     * @param data \r\n     */\r\n    public static dispath(type: string, data?: any) {\r\n        this.eventer.event(type, [data]);\r\n    }\r\n    /**\r\n     * 注册全局事件\r\n     * @param type \r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static register(type: string, caller: any, callback: Function) {\r\n        this.eventer.on(type, caller, callback);\r\n        EventManager.inst.registerInst(type, caller, callback);\r\n    }\r\n    private registerInst(type: string, caller: any, callback: Function) {\r\n        if (EventManager.eventer[\"$_GID\"] == undefined) EventManager.eventer[\"$_GID\"] = Laya.Utils.getGID();\r\n        let arr: CustomObj[] = this.pool[EventManager.eventer[\"$_GID\"]];\r\n        if (!arr) arr = [];\r\n        let callerList = arr.filter(value => value.thisobj == caller);\r\n        let typeList = callerList ? callerList.filter(value => value.type == type) : null;\r\n        if (!(typeList && typeList.length > 0)) {//避免重复添加\r\n            let cobj = new CustomObj(EventManager.eventer, callback, caller, null, 1);\r\n            cobj.type = type;\r\n            arr.push(cobj);\r\n            this.pool[EventManager.eventer[\"$_GID\"]] = arr;\r\n        }\r\n    }\r\n    //------------------------\r\n    /**\r\n     * 添加带触摸缩放效果的点击事件\r\n     * @param target \r\n     * @param thisObject \r\n     * @param listener \r\n     * @param value 触摸时的改变值\r\n     * @param setAlpha 是否改变透明度，否则改变缩放 \r\n     * @param useClickRestrict 是否使用点击约束(默认启用)\r\n     */\r\n    public static addTouchScaleListener(target: Laya.Node, thisobj: any, listener: Function, params: any = null, value: number = 1.05, setAlpha?: boolean, useClickRestrict: boolean = true) {\r\n        EventManager.inst.addScaleListener(target, thisobj, listener, params, value, setAlpha, useClickRestrict);\r\n    }\r\n    /**\r\n     * 删除按钮事件(建议统一调用removeAllEvents)\r\n     * @param target \r\n     */\r\n    public static removeBtnEvent(target: Laya.Node) {\r\n        EventManager.inst.removeEvent(target);\r\n    }\r\n    /**\r\n     * 删除thisobj关联的所以按钮事件\r\n     * @param thisobj \r\n     */\r\n    public static removeAllEvents(thisobj: any) {\r\n        EventManager.inst.removeAllEvents(thisobj);\r\n    }\r\n\r\n    public addScaleListener(target: Laya.Node, thisobj: any, listener: Function, params: any = null, scale: number = 1.05, setAlpha?: boolean, useClickRestrict: boolean = true) {\r\n        if (!target) return;\r\n        if (scale == 1) {\r\n            target.on(Laya.Event.CLICK, this, this.clickHandler);\r\n        } else {\r\n            target.on(Laya.Event.MOUSE_DOWN, this, this.onScaleTouch);\r\n            target.on(Laya.Event.MOUSE_OUT, this, this.onScaleTouch);\r\n            target.on(Laya.Event.MOUSE_UP, this, this.onScaleTouch);\r\n        }\r\n        if (thisobj[\"$_GID\"] == undefined) thisobj[\"$_GID\"] = Laya.Utils.getGID();\r\n        if (target[\"$_GID\"] == undefined) target[\"$_GID\"] = Laya.Utils.getGID();\r\n        let arr: any[] = this.pool[thisobj[\"$_GID\"]];\r\n        if (!arr) arr = [];\r\n        if (arr.indexOf(target) == -1) arr.push(target);\r\n\r\n        this.pool[target[\"$_GID\"]] = new CustomObj(target, listener, thisobj, params, scale, setAlpha, useClickRestrict)\r\n        this.pool[thisobj[\"$_GID\"]] = arr;\r\n    }\r\n    public removeEvent(target: Laya.Node) {\r\n        if (!target) return;\r\n\r\n        let obj: CustomObj = this.pool[target[\"$_GID\"]];\r\n        if (obj) {\r\n            let arr: any[] = this.pool[obj.thisobj[\"$_GID\"]];\r\n            if (arr && arr.indexOf(target) != -1) {\r\n                let id: number = arr.indexOf(target);\r\n                arr.splice(id, 1);\r\n                if (arr.length == 0) {\r\n                    delete this.pool[obj.thisobj[\"$_GID\"]];\r\n                    arr = null;\r\n                }\r\n            }\r\n            if (obj.sclNum == 1) target.off(Laya.Event.CLICK, this, this.clickHandler);\r\n            else {\r\n                target.off(Laya.Event.MOUSE_DOWN, this, this.onScaleTouch);\r\n                target.off(Laya.Event.MOUSE_OUT, this, this.onScaleTouch);\r\n                target.off(Laya.Event.MOUSE_UP, this, this.onScaleTouch);\r\n            }\r\n            delete this.pool[obj[\"$_GID\"]];\r\n            obj.dispose();\r\n            obj = null;\r\n        }\r\n    }\r\n    //删除全部事件\r\n    public removeAllEvents(thisobj: any) {\r\n        let arr: any[] = this.pool[thisobj[\"$_GID\"]];\r\n        if (arr) {\r\n            //删除按钮事件\r\n            for (let i = 0; i < arr.length; i++) {\r\n                this.removeEvent(arr[i]);\r\n                i--;\r\n            }\r\n        }\r\n\r\n        //删除对于的全局事件\r\n        let events: CustomObj[] = this.pool[EventManager.eventer[\"$_GID\"]];\r\n        if (events) {\r\n            let evts = events.filter(value => value.thisobj == thisobj);\r\n            let cobj: CustomObj;\r\n            for (let j = 0; j < evts.length; j++) {\r\n                cobj = evts[j];\r\n                if (cobj.thisobj) {\r\n                    EventManager.eventer.off(cobj.type, cobj.thisobj, cobj.listener);\r\n                }\r\n                let id = events.indexOf(cobj);\r\n                events.splice(id, 1);\r\n            }\r\n        }\r\n\r\n        //删除其他事件(laya系统事件)\r\n        let pushEvts: PushEventVo[] = this.pushPool[thisobj[\"$_GID\"]];\r\n        if (pushEvts) {\r\n            pushEvts.forEach(vo => {\r\n                vo.target.off(vo.type, vo.thisObj, vo.callback);\r\n            });\r\n            delete this.pushPool[thisobj[\"$_GID\"]];\r\n        }\r\n    }\r\n    private clickHandler(e: Laya.Event) {\r\n        let btn: Laya.Node = e.currentTarget as Laya.Node;\r\n        let obj: CustomObj = this.pool[btn[\"$_GID\"]];\r\n        if (!obj) return;\r\n        obj.docallback(e);\r\n    }\r\n    private onScaleTouch(e: Laya.Event) {\r\n        let btn: Laya.Node = e.currentTarget as Laya.Node;\r\n        let obj: CustomObj = this.pool[btn[\"$_GID\"]];\r\n        if (!obj) return;\r\n        switch (e.type) {\r\n            case Laya.Event.MOUSE_DOWN: {\r\n                obj.isTapBegin = true;\r\n                this.downPos.setTo(e.stageX, e.stageY);\r\n                let value: number = obj.sclNum;\r\n                if (obj.isSetAlpha) Laya.Tween.to(btn, { alpha: value }, 100);\r\n                else Laya.Tween.to(btn, { scaleX: value, scaleY: value }, 100);\r\n                break;\r\n\r\n            } case Laya.Event.MOUSE_OUT: {\r\n                if (obj.isSetAlpha) Laya.Tween.to(btn, { alpha: 1 }, 100);\r\n                else Laya.Tween.to(btn, { scaleX: 1, scaleY: 1 }, 100);\r\n                obj.isTapBegin = false;\r\n                break;\r\n\r\n            } case Laya.Event.MOUSE_UP: {\r\n                if (obj.isSetAlpha) Laya.Tween.to(btn, { alpha: 1 }, 100);\r\n                else Laya.Tween.to(btn, { scaleX: 1, scaleY: 1 }, 100);\r\n                let dist: number = this.downPos.distance(e.stageX, e.stageY);\r\n                if (dist < 20 && obj.isTapBegin) {//触发点击事件\r\n                    obj.docallback(e);\r\n                }\r\n                obj.isTapBegin = false;\r\n                break;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\nexport class CustomObj {\r\n    public isTapBegin: boolean = false;\r\n    public target: Laya.EventDispatcher = null;\r\n    public listener: Function = null;\r\n    public thisobj: Laya.Node = null;\r\n    public argObject: any = null\r\n    public sclNum: number;\r\n    public isSetAlpha: boolean;//是否改变透明度，否则改变缩放\r\n    public type: string = null;\r\n    private useClickRestrict: boolean;//是否启用点击约束\r\n\r\n    private isclick: boolean = true;\r\n\r\n    constructor(target: Laya.EventDispatcher, listener: Function, thisobj: any, argObject?: any, sclNum?: number, setAlpha?: boolean, useClickRestrict?: boolean) {\r\n        this.target = target;\r\n        this.listener = listener;\r\n        this.sclNum = sclNum;\r\n        this.isSetAlpha = setAlpha;\r\n        this.thisobj = thisobj;\r\n        this.argObject = argObject;\r\n        this.useClickRestrict = useClickRestrict;\r\n        this[\"$_GID\"] = Laya.Utils.getGID();\r\n        if (this.thisobj[\"$_GID\"] == undefined) this.thisobj[\"$_GID\"] = Laya.Utils.getGID();\r\n    }\r\n    public docallback(evt: Laya.Event) {\r\n        if (!this.isclick && this.useClickRestrict) return;\r\n        if (this.thisobj && this.listener) {\r\n            this.listener.call(this.thisobj, evt, this.argObject);\r\n            if (this.useClickRestrict) {\r\n                this.isclick = false;\r\n                Laya.timer.once(EventManager.delayClickTime, this, this.resetState);\r\n            }\r\n        }\r\n    }\r\n    private resetState() {\r\n        this.isclick = true;\r\n    }\r\n    public dispose() {\r\n        Laya.timer.clear(this, this.resetState);\r\n        this.target = null;\r\n        this.listener = null;\r\n        this.thisobj = null;\r\n        this.argObject = null;\r\n    }\r\n}\r\n\r\nexport class EventType {\r\n    public static FLUSH_USERINFO: string = \"flushUserInfo\";\r\n    public
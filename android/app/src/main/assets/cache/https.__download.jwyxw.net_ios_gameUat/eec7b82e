 => {\r\n            SoundPlayer.clickSound();\r\n            PageManager.showDlg(DlgCmd.ChessGame, 2);\r\n        });\r\n        EventManager.addTouchScaleListener(this.box4, this, () => {\r\n            SoundPlayer.clickSound();\r\n            PageManager.showDlg(DlgCmd.ChessGame, 3);\r\n        });\r\n    }\r\n\r\n    public init(): void {\r\n        this.panel.hScrollBarSkin = \"\";\r\n        let panelx = this.panel.localToGlobal(new Laya.Point(this.panel.x, this.panel.y)).x;\r\n        this.panel.width = Laya.stage.width - panelx;\r\n    }\r\n\r\n    public show(startX: number, list: GameIconVo[]): void {\r\n        this.panel.hScrollBar.value = 0;\r\n        let box: Laya.Box;\r\n        let isshow: boolean = false;\r\n        let vo: GameIconVo;\r\n        for (let i = 0; i < 4; i++) {\r\n            for (let j = 0; j < this.gameId.length; j++) {\r\n                if (this.gameId[i] == String(list[j].id)) {\r\n                    isshow = true;\r\n                    vo = list[j];\r\n                    break;\r\n                }\r\n            }\r\n            box = this[\"box\" + String(i + 1)];\r\n            if (isshow == false) {\r\n                box.visible = false;\r\n                continue;\r\n            }\r\n            if (GameState[vo.state] == GameState.PAUSE) {\r\n                this[\"pauseIcon\" + String(i + 1)].visible = true;\r\n                box.disabled = true;\r\n            } else if (GameState[vo.state] == GameState.EXPECTATION) {\r\n                this[\"expectIcon\" + String(i + 1)].visible = true;\r\n                box.disabled = true;\r\n            } else {\r\n                box.disabled = false;\r\n                this[\"pauseIcon\" + String(i + 1)].visible = false;\r\n                this[\"expectIcon\" + String(i + 1)].visible = false;\r\n            }\r\n            box.visible = true;\r\n            box.x = startX;\r\n            box.alpha = 0;\r\n            Laya.Tween.to(box, { x: i * 302 + 136, alpha: 1 }, 350, Laya.Ease.cubicOut, null, i * 60 + 10);\r\n            startX += 302;\r\n        }\r\n        this.visible = true;\r\n    }\r\n\r\n    public hide(): void {\r\n        this.visible = false;\r\n        if (this.panel.hScrollBar)\r\n            this.panel.hScrollBar.value = 0;\r\n    }\r\n}\r\n","import { ui } from \"../../../../ui/layaMaxUI\";\r\n\r\n\r\nexport default class Roullette extends ui.sceneUI.dlg.notice.roulette.GameRouletteUI {\r\n\r\n\t//轮盘组件\r\n\tprotected itemLabels: Laya.Label[];\r\n\tprotected shellList: Laya.Box[];\r\n\r\n\t/**\r\n\t * -1未初始化 0停止 1待机 2游戏 3停止中 4回弹中 5结算中\r\n\t */\r\n\tprotected state: number = -1;\r\n\r\n\t//旋转参数\r\n\tprotected speedStandby: number = 3;\t//待机时速度\r\n\tprotected speedGame: number = 540; \t//抽奖时速度速度\r\n\tprotected speedOver: number = 200;\t//结束回弹速度\r\n\tprotected overRat: number = 20;\t\t//超过角度\r\n\tprotected targetRat: number = 0;\t//目标结束角度\r\n\r\n\r\n\t//转盘数据List\r\n\tprotected lotteryList: any[];\r\n\t//礼物数量数据列表\r\n\tprotected curAwardCountList: number[];\r\n\t//请求旋转积分\r\n\tpublic curPoint: number;\r\n\t/**\r\n\t * 轮盘类型(1:白银,2:黄金,3:钻石) \r\n\t */\r\n\tpublic rouletteLevel: number = 0;\r\n\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t\tthis.initView();\r\n\t}\r\n\tprivate initView() {\r\n\t\t//获取组件\r\n\t\tthis.itemLabels = [];\r\n\t\tfor (let index = 0; index < this.labelBox.numChildren; index++) {\r\n\t\t\tconst itemLabel = this.labelBox.getChildByName(\"amount\" + index) as Laya.Label;\r\n\t\t\titemLabel.text = \"-\";\r\n\t\t\tthis.itemLabels.push(itemLabel);\r\n\t\t}\r\n\t\tthis.shellList = [];\r\n\t\tfor (let index = 0; index < 3; index++) {\r\n\t\t\tconst shell = this.getChildByName(\"shell\" + index) as Laya.Box;\r\n\t\t\tshell.visible = false;\r\n\t\t\tthis.shellList.push(shell);\r\n\t\t}\r\n\t\tthis.shellList[0].visible = true;\r\n\t\t//打开帧更新\r\n\t\tthis.frameLoop(1, this, this.updateRotate);\r\n\t\t//\r\n\t\tthis.state = 0;\r\n\t}\r\n\tpublic destroy() {\r\n\t\tthis.state = -1;\r\n\t\tthis.clearTimer(this, this.updateRotate);\r\n\t\tthis.closeAllEct();\r\n\t\tsuper.destroy(true);\r\n\t}\r\n\r\n\tprotected closeAllEct() {\r\n\t\tthis.ectBtnGo.stop();\r\n\t\tthis.ectBtnGo.visible = false;\r\n\t\tthis.animBtnLight.stop();\r\n\t\tthis.animBtnLight.visible = false;\r\n\t\tthis.etcWinLight.stop();\r\n\t\tthis.etcWinLight.visible = false;\r\n\t}\r\n\r\n\t//缓动帧调用\r\n\tprotected updateRotate() {\r\n\t\tif (this.state <= 0 || this.state >= 5) return;\r\n\t\tswitch (this.state) {\r\n\t\t\tcase 1: {\r\n\t\t\t\tvar addRotate = Laya.timer.delta / 1000 * this.speedStandby;\r\n\t\t\t\tthis.plateNode.rotation += addRotate;\r\n\t\t\t\tbreak;\r\n\t\t\t}\r\n\t\t\tcase 2: {\r\n\t\t\t\tvar addRotate = Laya.timer.delta / 1000 * this.speedGame;\r\n\t\t\t\tthis.plateNode.rotation += addRotate;\r\n\t\t\t\tbreak;\r\n\t\t\t}\r\n\t\t\tcase 3: {\r\n\t\t\t\tvar addRotate = Laya.timer.delta / 1000 * this.speedGame;\r\n\t\t\t\tthis.plateNode.rotation += addRotate;\r\n\t\t\t\tif (this.plateNode.rotation >= this.targetRat) {\r\n\t\t\t\t\tthis.state = 4;\r\n\t\t\t\t\tthis.targetRat += this.overRat;\r\n\t\t\t\t}\r\n\t\t\t\tbreak;\r\n\t\t\t}\r\n\t\t\tcase 4: {\r\n\t\t\t\tif (this.plateNode.rotation < this.targetRat) {\r\n\t\t\t\t\tvar addRotate = Laya.timer.delta / 1000 * this.speedOver;\r\n\t\t\t\t\tthis.plateNode.rotation += addRotate;\r\n\t\t\t\t\tif (this.plateNode.rotation >= this.targetRat) {\r\n\t\t\t\t\t\tthis.targetRat -= this.overRat;\r\n\t\t\t\t\t}\r\n\t\t\t\t} else {\r\n\t\t\t\t\tvar addRotate = Laya.timer.delta / 1000 * this.speedOver;\r\n\t\t\t\t\tthis.plateNode.rotation -= addRotate;\r\n\t\t\t\t\tif (this.plateNode.rotation <= this.targetRat) {\r\n\t\t\t\t\t\tthis.plateNode.rotation = this.targetRat;\r\n\t\t\t\t\t\tthis.state = 5;\r\n\t\t\t\t\t\tthis.etcWinLight.play(0, false, \"win\");\r\n\t\t\t\t\t\tthis.etcWinLight.visible = true;\r\n\t\t\t\t\t\tthis.event(\"roulleteStoped\", this);\r\n\t\t\t\t\t\tthis.etcWinLight.on(Laya.Event.COMPLETE, this, function onComplete(): void {\r\n\t\t\t\t\t\t\tthis.etcWinLight.play(0, true, \"light\");\r\n\t\t\t\t\t\t\tthis.etcWinLight.offAll(Laya.Event.COMPLETE);\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t\tbreak;\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n\r\n\t/**\r\n\t * 设置转盘数据\r\n\t * @param prizes 礼物数量列表\r\n\t * @param reqPoints 花费积分数量\r\n\t */\r\n\tpublic SetData(lotteryList) {\r\n\t\t//设置转盘礼物数据\r\n\t\tthis.lotteryList = lotteryList;\r\n\t\tthis.rouletteLevel = -1;\r\n\t\tthis.ChangeType(1);\r\n\t}\r\n\r\n\t/**\r\n\t * 更换转盘类型\r\n\t * @param type (1:白银,2:黄金,3:钻石) \r\n\t */\r\n\tpublic ChangeType(type: number) {\r\n\t\tif (!this.lotteryList) return;\r\n\t\tif (this.rouletteLevel != type) {\r\n\t\t\tthis.rouletteLevel = type;\r\n\t\t\tvar rindex = this.rouletteLevel - 1;\r\n\t\t\tthis.curAwardCountList = this.lotteryList[rindex].prizeLevelList;\r\n\t\t\tfor (let index = 0; index < this.curAwardCountList.length; index++) {\r\n\t\t\t\tconst awardCount = this.curAwardCountList[index];\r\n\t\t\t\tvar itemLabe = this.itemLabels[index];\r\n\t\t\t\titemLabe.text = awardCount.toString();\r\n\t\t\t}\r\n\t\t\tthis.labelBox.reCache();\r\n\t\t\t//设置花费积分数据\r\n\t\t\tthis.curPoint = this.lotteryList[rindex].requiredPoints;\r\n\t\t\tthis.reqNumLabel.text = this.curPoint.toString();\r\n\t\t\t//设置转盘外壳\r\n\t\t\tfor (let index = 0; index < this.shellList.length; index++) {\r\n\t\t\t\tconst shell = this.shellList[index];\r\n\t\t\t\tshell.visible = (index == rindex);\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n\r\n\t/**\r\n\t * 待机\r\n\t */\r\n\tpublic Standby() {\r\n\t\t//转盘待机\r\n\t\tthis.state = 1;\r\n\t\t//播放按钮闪光与心跳特效\r\n\t\tthis.ectBtnGo.play(0, true, \"heart\");\r\n\t\tthis.ectBtnGo.visible = true;\r\n\t\tthis.animBtnLight.play(0, true, \"light\");\r\n\t\tthis.animBtnLight.visible = true;\r\n\t\t//关闭胜利特效\r\n\t\tthis.etcWinLight.stop();\r\n\t\tthis.etcWinLight.visible = false;\r\n\t}\r\n\r\n\t/**\r\n\t * 游戏中\r\n\t */\r\n\tpublic PlayGame() {\r\n\t\tthis.state = 2;\r\n\t\tthis.targetRat = 0;\r\n\t\t//\r\n\t\tthis.ectBtnGo.play(0, false, \"heart\");\r\n\t\tthis.ectBtnGo.visible = true;\r\n\t\tthis.animBtnLight.stop();\r\n\t\tthis.animBtnLight.visible = false;\r\n\t}\r\n\r\n\t/**\r\n\t * 结束游戏\r\n\t * @param targetAwardCount 奖励数量\r\n\t * @param completed 停止后回调\r\n\t */\r\n\tpublic StopGame(targetAwardCount: number) {\r\n\t\tthis.state = 3;\r\n\t\tvar targetIndex = 0;\r\n\t\tfor (let index = 0; index < this.curAwardCountList.length; index++) {\r\n\t\t\tconst awardCount = this.curAwardCountList[index];\r\n\t\t\tif (targetAwardCount == awardCount) {\r\n\t\t\t\ttargetIndex = index;\r\n\t\t\t\tbreak;\r\n\t\t\t}\r\n\t\t}\r\n\t\tvar turns = Math.floor(this.plateNode.rotation / 360);\r\n\t\tthis.plateNode.rotation -= turns * 360;\r\n\t\tthis.targetRat = (targetIndex + 1) * (360 / this.itemLabels.length) + 360;\r\n\t}\r\n\r\n\tpublic Reset() {\r\n\t\tthis.state = 0;\r\n\t\t//关闭所有特效\r\n\t\tthis.closeAllEct();\r\n\t\t//复位转盘\r\n\t\tthis.plateNode.rotation = 0;\r\n\t}\r\n}\r\n","import { LobbyModel } from \"../../../data/LobbyModel\";\r\nimport { PersonalModel, VipInfoVo, VipLevelVo, VipRewardVo } from \"../../../data/PersonalModel\";\r\nimport { EventManager, EventType } from \"../../../events/EventManager\";\r\n/**Created by the LayaAirIDE*/\r\nimport { ui } from \"../../../ui/layaMaxUI\";\r\nimport { SoundPlayer } from \"../../../utils/SoundPlayer\";\r\nimport { PageManager } from \"../../PageManager\";\r\nimport { DlgCmd } from './../../../data/GameVos';\r\n\r\n/**\r\n * vip礼金展示\r\n */\r\nexport default class VipGiftsPanel extends ui.sceneUI.dlg.personaCenter.VipGiftsPanelUI {\r\n\tprivate vo: VipLevelVo;\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t\tthis.initView();\r\n\t}\r\n\tprivate initView() {\r\n\t\tEventManager.addTouchScaleListener(this.weekBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tPersonalModel.getVIPSalary(2, this, this.weekSalaryBack);\r\n\t\t});\r\n\t\tEventManager.addTouchScaleListener(this.monthBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tPersonalModel.getVIPSalary(3, this, t
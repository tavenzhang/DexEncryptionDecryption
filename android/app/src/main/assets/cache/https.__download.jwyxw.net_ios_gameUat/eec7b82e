RatioItem extends ui.sceneUI.dlg.agent.CRatioItemUI {\r\n\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t\tthis.bgIcon.visible = false;\r\n\t}\r\n\tpublic initData(data: CRatioVo, index: number) {\r\n\t\tthis.numTxt.text = data.level.toString();\r\n\t\tthis.rangeTxt.text = this.getMoneyRange(data, index);\r\n\t\tlet money: number = this.getRateMoney(data);\r\n\t\tthis.paysTxt.text = money.toString();\r\n\t\tthis.blTxt.text = (money / 100) + \"%\";\r\n\t}\r\n\tprivate getRateMoney(obj: any): number {\r\n\t\tvar percent = parseFloat(obj.rate) / 100;\r\n\t\tvar rs = (percent * 10000);\r\n\r\n\t\trs = Math.round(rs);\r\n\t\trs = parseInt(rs + \"\");\r\n\r\n\t\treturn rs;\r\n\t}\r\n\tprivate getMoneyRange(obj: any, i): string {\r\n\t\tvar lower = Number(obj.lower) * 10000;\r\n\t\tvar upper = Number(obj.upper) * 10000;\r\n\r\n\t\tvar pre = this.FormatMoney(lower, 0);\r\n\t\tvar f = \" ≤ \";\r\n\r\n\t\tif (i == 0) {\r\n\t\t\tvar f = \" < \";\r\n\t\t}\r\n\r\n\t\tvar x = \"X\";\r\n\t\tvar mid = \"\";\r\n\r\n\t\tvar end = this.FormatMoney(upper, 0);\r\n\r\n\t\tif (lower > 0) {\r\n\t\t\tmid = f + x;\r\n\t\t} else {\r\n\t\t\tpre = \"\";\r\n\t\t\tmid = x;\r\n\t\t}\r\n\t\tif (upper >= 0) {\r\n\t\t\tmid = mid + \" < \";\r\n\t\t} else {\r\n\t\t\tend = \"\";\r\n\t\t}\r\n\r\n\t\tif (isNaN(upper)) {\r\n\t\t\tupper = Number.MAX_VALUE;\r\n\t\t}\r\n\r\n\t\tif (AgentModel.todaysPerformance >= lower && AgentModel.todaysPerformance < upper) {\r\n\t\t\tthis.bgIcon.visible = true;\r\n\t\t}\r\n\t\treturn pre + mid + end;\r\n\t}\r\n\tprivate FormatMoney(num: number, len: number = 0): string {\r\n\t\tvar r: string = \"\";\r\n\r\n\t\tif (num >= 10000) {\r\n\t\t\t//超过一万\r\n\t\t\tvar n: number = num / 10000;\r\n\t\t\tvar tt = n.toFixed(len + 1);\r\n\r\n\t\t\tvar pointPos = tt.indexOf(\".\"); //点在字符串tt的哪里\r\n\t\t\t//点的后面截取len+1位\r\n\t\t\tr = tt.substring(0, pointPos + len);\r\n\t\t} else {\r\n\t\t\tif (num >= 0) {\r\n\t\t\t\treturn num + \"\";\r\n\t\t\t} else {\r\n\t\t\t\treturn \"\";\r\n\t\t\t}\r\n\t\t}
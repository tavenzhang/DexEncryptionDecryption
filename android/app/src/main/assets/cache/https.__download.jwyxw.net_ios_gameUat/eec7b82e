Icon();\r\n\r\n\t\tlet msc = SaveManager.getObj().get(SaveManager.KEY_MUSIC_SWITCH, 1);\r\n\t\tthis.musicBtn.selected = Boolean(msc == 1);\r\n\t\tlet sdx = SaveManager.getObj().get(SaveManager.KEY_SFX_SWITCH, 1);\r\n\t\tthis.soundBtn.selected = Boolean(sdx == 1);\r\n\r\n\t\t//events---------------\r\n\t\tthis.initEvents();\r\n\t}\r\n\tprivate initEvents() {\r\n\t\t//热更新按钮\r\n\t\tEventManager.addTouchScaleListener(this.updateBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tPostMHelp.game_common({ do: \"appUpate\", param: \"\" });\r\n\t\t\tCommon.isUpate = false;\r\n\t\t});\r\n\t\t//更换头像\r\n\t\tEventManager.addTouchScaleListener(this.setHeadBtn, this, () => {\r\n\t\t\tSoundPlayer.enterPanelSound();\r\n\t\t\tSetHeadDlg.show();\r\n\t\t});\r\n\t\t//账户信息\r\n\t\tEventManager.addTouchScaleListener(this.accInfoBtn, this, () => {\r\n\t\t\tSoundPlayer.enterPanelSound();\r\n\t\t\tif (LoginModel.loginType != LoginMethod.account) {//游客不能打开\r\n\t\t\t\tTipsDlg.show(\"亲爱的老板\\n使用账户信息功能需要先升级为正式账号哦~\", this, this.openAccountUpgrade);\r\n\t\t\t\treturn;\r\n\t\t\t}\r\n\t\t\tAccountInfoDlg.show();\r\n\t\t});\r\n\t\t//切换账号\r\n\t\tEventManager.addTouchScaleListener(this.backAccBtn, this, () => {\r\n\t\t\tSoundPlayer.enterPanelSound();\r\n\t\t\tSystemDlg.show(\"确定切换账号吗？\",\r\n\t\t\t\t{\r\n\t\t\t\t\tcaller: GameMgr,\r\n\t\t\t\t\trightBtnCallback: GameMgr.loginOut\r\n\t\t\t\t}\r\n\t\t\t);\r\n\t\t});\r\n\t\t//复制id\r\n\t\tEventManager.addTouchScaleListener(this.idCopyBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tPostMHelp.game_common({ \"do\": \"copylink\", \"param\": this.idTxt.text, \"hint\": \"复制成功\" });\r\n\t\t});\r\n\t\t//复制账号\r\n\t\tEventManager.addTouchScaleListener(this.accCopyBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tPostMHelp.game_common({ \"do\": \"copylink\", \"param\": this.accTxt.text, \"hint\": \"复制成功\" });\r\n\t\t});\r\n\t\t//修改昵称\r\n\t\tEventManager.addTouchScaleListener(this.setNickBtn, this, () => {\r\n\t\t\tSoundPlayer.enter
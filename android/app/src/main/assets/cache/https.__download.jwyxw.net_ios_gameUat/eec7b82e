行播放音效\r\n     * 目前成立条件：ios系统版本号>=13\r\n     */\r\n    public static usePlay(): boolean {\r\n        if (!Laya.Browser.onIOS) return false;\r\n        if (this.uaReaded) return this._usePlay;\r\n\r\n        let ua = Laya.Browser.userAgent;\r\n        let matches = ua.toLowerCase().match(/cpu [^\\d]*\\d.*like mac os x/);\r\n        if (!matches || matches.length == 0) {\r\n            this._usePlay = false;\r\n        }\r\n        let value = matches[0];\r\n        let ver = parseInt(value.match(/\\d+(_\\d)*/)[0]) || 0;\r\n        if (ver >= 13) this._usePlay = true;\r\n        this.uaReaded = true;\r\n        return this._usePlay;\r\n    }\r\n    /**\r\n     * 播放音效\r\n     * @param url \r\n     */\r\n    public static play(url: string, ) {\r\n        if (!this.inited) this.initAudio();\r\n        if (!this.canUse) {\r\n            console.error(\"初始化失败,不能播放音效!\");\r\n            return;\r\n        }\r\n\r\n        if (!this._sound) {\r\n            this._sound = new eWebAudioSound();\r\n            this._sound.on(Laya.Event.COMPLETE, this, () => {\r\n                let sc = this._sound.play(0, 1);\r\n                sc.once(eEvent.SOUND_COMPLETE, this, () => {\r\n                    console.debug(\"play-end>>>\");\r\n                });\r\n            });\r\n        }\r\n        this._sound.load(url);\r\n    }\r\n    private static initAudio() {\r\n        var canUseWebAudio = window[\"AudioContext\"] || window[\"webkitAudioContext\"] || window[\"mozAudioContext\"];\r\n        if (canUseWebAudio) {\r\n            try {\r\n                //防止某些chrome版本创建异常问题\r\n                eWebAudioDecode.ctx = new (window[\"AudioContext\"] || window[\"webkitAudioContext\"] || window[\"mozAudioContext\"])();\r\n                console.error(\"init-audio-context\");\r\n            }\r\n            catch (e) {\r\n                canUseWebAudio = false;\r\n            }\r\n        }\r\n        this.inited = true;\r\n        this.canUse = canUseWebAudio;\r\n    }\r\n}\r\nexport class eWebAudioSound extends Laya.EventDispatcher {\r\n    private loaded: boolean = false;\r\n    private audioBuffer: any;\r\n    public url: string;\r\n\r\n    constructor() {\r\n        super();\r\n\r\n    }\r\n    public get length() {\r\n        if (this.audioBuffer) {\r\n            return this.audioBuffer.duration;\r\n        }\r\n        throw new Error(\"sound not loaded!\");\r\n    }\r\n    /**\r\n     * 加载\r\n     * @param url \r\n     */\r\n    public load(url: string) {\r\n        let self = this;\r\n        this.url = url;\r\n        if (true && !url) {\r\n            throw new Error(\"sound url is null\");\r\n        }\r\n        var request = new XMLHttpRequest();\r\n        request.open(\"GET\", url, true);\r\n        request.responseType = \"arraybuffer\";\r\n        request.addEventListener(\"load\", function () {\r\n            var ioError = (request.status >= 400);\r\n            if (ioError) {\r\n                self.event(eEvent.IO_ERROR);\r\n            }\r\n            else {\r\n                eWebAudioDecode.decodeArr.push({\r\n                    \"buffer\": request.response,\r\n                    \"success\": onAudioLoaded,\r\n                    \"fail\": onAudioError,\r\n                    \"self\": self,\r\n                    \"url\": self.url\r\n                });\r\n                eWebAudioDecode.decodeAudios();\r\n            }\r\n        });\r\n        request.addEventListener(\"error\", function () {\r\n            self.event(eEvent.IO_ERROR);\r\n        });\r\n        request.send();\r\n        function onAudioLoaded() {\r\n            self.loaded = true;\r\n            self.event(Laya.Event.COMPLETE);\r\n        }\r\n        function onAudioError() {\r\n            self.event(eEvent.IO_ERROR);\r\n        }\r\n    }\r\n    public play(startTime?: number, loops?: number) {\r\n        startTime = +startTime || 0;\r\n        loops = +loops || 0;\r\n        if (true && this.loaded == false) {\r\n            throw new Error(\"sound no loaded\");\r\n        }\r\n        var channel = new eWebAudioSoundChannel();\r\n        channel.$url = this.url;\r\n        channel.$loops = loops;\r\n        channel.$audioBuffer = this.audioBuffer;\r\n        channel.$startTime = startTime;\r\n        channel.$play();\r\n        eSystem.$pushSoundChannel(channel);\r\n        return channel;\r\n    }\r\n    public close() {\r\n\r\n    }\r\n    public static MUSIC = \"music\";\r\n    public static EFFECT = \"effect\";\r\n}\r\n\r\nexport class eWebAudioDecode {\r\n    public static decodeArr: any[] = [];\r\n    public static isDecoding: boolean = false;\r\n\r\n    public static ctx: any;//AudioContext|webkitAudioContext|mozAudioContext\r\n\r\n    public static decodeAudios() {\r\n        if (eWebAudioDecode.decodeArr.length <= 0) {\r\n            return;\r\n        }\r\n        if (eWebAudioDecode.isDecoding) {\r\n            return;\r\n        }\r\n        eWebAudioDecode.isDecoding = true;\r\n        var decodeInfo = eWebAudioDecode.decodeArr.shift();\r\n        eWebAudioDecode.ctx.decodeAudioData(decodeInfo[\"buffer\"], function (audioBuffer) {\r\n            decodeInfo[\"self\"].audioBuffer = audioBuffer;\r\n            if (decodeInfo[\"success\"]) {\r\n                decodeInfo[\"success\"]();\r\n            }\r\n            eWebAudioDecode.isDecoding = false;\r\n            eWebAudioDecode.decodeAudios();\r\n        }, function () {\r\n            console.error('sound decode error');\r\n            if (decodeInfo[\"fail\"]) {\r\n                decodeInfo[\"fail\"]();\r\n            }\r\n            eWebAudioDecode.isDecoding = false;\r\n            eWebAudioDecode.decodeAudios();\r\n        });\r\n    }\r\n}\r\n\r\nexport class eWebAudioSoundChannel extends Laya.EventDispatcher {\r\n    public $startTime: number = 0;\r\n    private bufferSource: any = null;\r\n    private context: any;\r\n    private gain: any;\r\n    private isStopped: boolean = false;\r\n    private _currentTime: number = 0;\r\n    private _volume: number = 1;\r\n    public $loops: number = 0;\r\n    private _startTime: number = 0;\r\n    public $audioBuffer: any;\r\n    public $url: string = \"\";\r\n    private onPlayEnd: Function;\r\n\r\n    constructor() {\r\n        super();\r\n        this.context = eWebAudioDecode.ctx;\r\n        if (this.context[\"createGain\"]) {\r\n            this.gain = this.context[\"createGain\"]();\r\n        } else {\r\n            this.gain = this.context[\"createGainNode\"]();\r\n        }\r\n        let self = this;\r\n        this.onPlayEnd = function () {\r\n            if (self.$loops == 1) {\r\n                self.stop();\r\n                self.event(eEvent.SOUND_COMPLETE);\r\n                return;\r\n            }\r\n            if (self.$loops > 0) {\r\n                self.$loops--;\r\n            }\r\n            self.$play();\r\n        }\r\n    }\r\n\r\n    public $play() {\r\n        if (this.isStopped) {\r\n            console.error(\"isStopped\");\r\n            return;\r\n        }\r\n        if (this.bufferSource) {\r\n            this.bufferSource.onended = null;\r\n            this.bufferSource = null;\r\n        }\r\n        var context = this.context;\r\n        var gain = this.gain;\r\n        var bufferSource = context.createBufferSource();\r\n        this.bufferSource = bufferSource;\r\n        bufferSource.buffer = this.$audioBuffer;\r\n        bufferSource.connect(gain);\r\n        gain.connect(context.destination);\r\n        bufferSource.onended = this.onPlayEnd;\r\n        this._startTime = Date.now();\r\n        this.gain.gain.value = this._volume;\r\n        bufferSource.start(0, this.$startTime);\r\n        this._currentTime = 0;\r\n    }\r\n    public stop() {\r\n        if (this.bufferSource) {\r\n            var sourceNode = this.bufferSource;\r\n            if (sourceNode.stop) {\r\n                sourceNode.stop(0);\r\n            } else {\r\n                sourceNode.noteOff(0);\r\n            }\r\n            sourceNode.onended = null;\r\n            sourceNode.disconnect();\r\n            this.bufferSource = null;\r\n            this.$audioBuffer = null;\r\n        }\r\n        if (!this.isStopped) {\r\n            eSystem.$popSoundChannel(this);\r\n        }\r\n        this.isStopped = true;\r\n    }\r\n    public get volume(): number {\r\n        return this._volume;\r\n    }\r\n    public set volume(value: number) {\r\n        if (this.isStopped) {\r\n            console.error(\"isStopped\");\r\n            return;\r\n        }\r\n        this._volume = value;\r\n        this.gain.gain.value = value;\r\n    }\r\n}\r\n\r\nexport class eEvent {\r\n    public static SOUND_COMPLETE: string = \"sound_complete\";//声音播放完毕\r\n    public static IO_ERROR: string = \"io_error\";\r\n}\r\n//用于替代替代:egret.sys\r\nexport class eSystem {\r\n    private static usingChannel: any[] = [];\r\n    public static $pushSoundChannel(channel) {\r\n        if (this.usingChannel.indexOf(channel) < 0) {\r\n            this.usingChannel.push(channel);\r\n        }\r\n    }\r\n    public static $popSoundChannel(channel) {\r\n        var index = this.usingChannel.indexOf(channel);\r\n        if (index >= 0) {\r\n            this.usingChannel.splice(index, 1);\r\n            return true;\r\n        }\r\n        return false;\r\n    }\r\n}","import { Debug } from \"../common/Debug\";\r\nimport { PostMHelp } from \"../common/PostMHelp\";\r\nimport { SaveManager } from \"../common/SaveManager\";\r\nimport { ResConfig } from \"../data/ResConfig\";\r\nimport { GameUtils } from \"./GameUtils\";\r\nimport { eSoundMgr } from './SoundMgr';\r\n\r\n\r\nexport enum SoundConfig {\r\n    welcomeLobby,//大厅欢迎\r\n    welcomeAgent,//代理欢迎\r\n    welcomeDial,//转盘欢迎\r\n    welcomePay,//充值欢迎\r\n    welcomeShare,//分享欢迎\r\n    roulePlay,//转盘转动声音\r\n    rouleReward,//转盘奖励声音\r\n}\r\n\r\n/*\r\n* 声音音效播放器\r\n*/\r\nexport class SoundPlayer {\r\n\r\n    private static curPlaySound: { [key: string]: boolean } = {};\r\n\r\n    public static soundValue: number = 1;\r\n    public static musicValue: number = 1;\r\n    public static soundSwitch: number = 1;\r\n    public static musicSwitch: number = 1;\r\n\r\n    private static soundMap = {\r\n        [SoundConfig.welcomeLobby]: \"assets/raw/welcome_lobby.mp3\",\r\n        [SoundConfig.welcomeAgent]: \"assets/raw/welcome_agent.mp3\",\r\n        [SoundConfig.welcomeDial]: \"assets/raw/welcome_dial.mp3\",\r\n        [SoundConfig.welcomePay]: \"assets/raw/welcome_pay.mp3\",\r\n        [SoundConfig.welcomeShare]: \"assets/raw/welcome_share.mp3\",\r\n        [SoundConfig.roulePlay]: \"assets/raw/sfx_turn.mp3\",\r\n        [SoundConfig.rouleReward]: \"assets/raw/sfx_coin.mp3\",\r\n    }\r\n\r\n    /***************************************************************** 内部相关接口 ********************************************************/\r\n    /** 关闭大厅背景音乐 */\r\n    private static stopLobbyBGM() {\r\n        //App\r\n        if (GameUtils.isAppSound) {\r\n            PostMHelp.game_common({ do: \"playBgMusic\", param: false });\r\n        } else {\r\n            Laya.SoundManager.stopMusic();\r\n        }\r\n    }\r\n    /** 播放大厅背景音乐 */\r\n    private static playLobbyBGM() {\r\n        Laya.SoundManager.musicVolume = this.musicValue;\r\n        Debug.log(\"playBgMusic-app:\", GameUtils.isAppSound);\r\n        //App\r\n        if (GameUtils.isAppSound) {\r\n            PostMHelp.game_common({ do: \"playBgMusic\", param: true });\r\n            //引擎\r\n        } else {\r\n            Laya.SoundManager.playMusic(ResConfig.musicUrl);\r\n        }\r\n    }\r\n    //更新音效\r\n    private static updateSound() {\r\n        return;//播放的时候有判断，所以就不执行后面的步骤，用于解决关闭音效开关的时候不播放声音\r\n        //设置引擎播放音量\r\n        Laya.SoundManager.setSoundVolume(this.soundValue);\r\n        //关闭所有音效\r\n        if (this.soundSwitch == 0) {\r\n            Laya.SoundManager.stopAllSound();\r\n        }\r\n    }\r\n\r\n    /***************************************************************** 设置相关接口 ********************************************************/\r\n    //初始化音频设置\r\n    public static initSoundSetting() {\r\n        //读取最新本地缓存文件\r\n        SaveManager.getObj().refreshSaveObj();\r\n        //保存音频设置\r\n        this.soundValue = SaveManager.getObj().get(SaveManager.KEY_SFX_VL, 1);\r\n        this.soundSwitch = SaveManager.getObj().get(SaveManager.KEY_SFX_SWITCH, 1);\r\n        this.musicValue = SaveManager.getObj().get(SaveManager.KEY_MUSIC_VL, 1);\r\n        this.musicSwitch = SaveManager.getObj().get(SaveManager.KEY_MUSIC_SWITCH, 1);\r\n        //这里以开关为准重置音量值，重置上海设置\r\n        this.musicValue = this.musicSwitch;\r\n        this.soundValue = this.soundSwitch;\r\n        SaveManager.getObj().set(SaveManager.KEY_MUSIC_VL, this.musicValue);\r\n        SaveManager.getObj().set(SaveManager.KEY_SFX_VL, this.soundValue);\r\n        SaveManager.getObj().saveData();\r\n    }\r\n\r\n    //更新音频设置（这里做兼容上海）\r\n    public static CompatibleSetting() {\r\n        //读取最新本地缓存文件\r\n        SaveManager.getObj().refreshSaveObj();\r\n        //读取最新数据\r\n        var soundValue = SaveManager.getObj().get(SaveManager.KEY_SFX_VL, 1);\r\n        var soundSwitch = SaveManager.getObj().get(SaveManager.KEY_SFX_SWITCH, 1);\r\n        var musicValue = SaveManager.getObj().get(SaveManager.KEY_MUSIC_VL, 1);\r\n        var musicSwitch = SaveManager.getObj().get(SaveManager.KEY_MUSIC_SWITCH, 1);\r\n        //开关未发生变化\r\n        if (this.soundSwitch == soundSwitch) {\r\n            //音量发生变化，说明是上海的改了设置\r\n            if (this.soundValue != soundValue) {\r\n                this.soundValue = soundValue;\r\n                this.soundSwitch = this.soundValue > 0 ? 1 : 0;\r\n            }\r\n            this.updateSound();\r\n            //开关变化，说明是我方老虎机友军改变了设置\r\n        } else {\r\n            this.ChangeSoundSwitch(soundSwitch);\r\n        }\r\n\r\n        //同理音效\r\n        if (this.musicSwitch == musicSwitch) {\r\n            //音量发生变化，说明是上海改了设置\r\n            if (this.musicValue != musicValue) {\r\n                this.musicValue = musicValue;\r\n                this.musicSwitch = this.musicValue > 0 ? 1 : 0;\r\n            }\r\n            this.UpdateBGM();\r\n            //开关变化，说明是我方老虎机友军改变了设置\r\n        } else {\r\n            this.ChangeMusicSwitch(musicSwitch);\r\n        }\r\n\r\n        //保存兼容后设置\r\n        SaveManager.getObj().set(SaveManager.KEY_SFX_VL, this.soundValue);\r\n        SaveManager.getObj().set(SaveManager.KEY_SFX_SWITCH, this.soundSwitch);\r\n        SaveManager.getObj().set(SaveManager.KEY_MUSIC_VL, this.musicValue);\r\n        SaveManager.getObj().set(SaveManager.KEY_MUSIC_SWITCH, this.musicSwitch);\r\n        SaveManager.getObj().saveData();\r\n    }\r\n\r\n    public static ChangeSoundSwitch(soundSwitch: number) {\r\n        if (this.soundSwitch == soundSwitch) return;\r\n        this.soundSwitch = soundSwitch;\r\n        this.soundValue = this.soundSwitch > 0 ? 1 : 0;\r\n        this.updateSound();\r\n    }\r\n\r\n    public static ChangeMusicSwitch(musicSwitch: number) {\r\n        if (this.musicSwitch == musicSwitch) return;\r\n        this.musicSwitch = musicSwitch;\r\n        this.musicValue = this.musicSwitch > 0 ? 1 : 0;\r\n        this.UpdateBGM();\r\n    }\r\n\r\n\r\n    /***************************************************************** 播放相关接口 ********************************************************/\r\n    //更新背景音乐\r\n    public static UpdateBGM() {\r\n        //设置引擎播放音量\r\n        Laya.SoundManager.musicVolume = this.musicValue;\r\n        //关闭大厅背景音乐\r\n        if (GameUtils.isAppSound) {\r\n            if (this.musicSwitch == 1 && this.musicValue > 0) {\r\n                this.playLobbyBGM();\r\n            } else {\r\n                this.stopLobbyBGM();\r\n            }\r\n        } else {\r\n            if (this.musicSwitch == 0) {\r\n                this.stopLobbyBGM();\r\n            } else {\r\n                this.playLobbyBGM();\r\n            }\r\n        }\r\n    }\r\n\r\n    /** 关闭所有音频 */\r\n    public static StopAllSounds() {\r\n        Laya.SoundManager.stopMusic();\r\n        PostMHelp.game_common({ do: \"playBgMusic\", param: false });\r\n    }\r\n\r\n    /**\r\n     * 播放声音\r\n     * @param url \r\n     */\r\n    public static play(url: string, isMutex: boolean, loops: number = 1) {\r\n        if (this.soundSwitch != 1) return;\r\n        if (isNative) {//native版本兼容\r\n            url = this.getNativeSound(url);\r\n        }\r\n        if (eSoundMgr.usePlay && !isNative) {//兼容ios13.1.3以上系统\r\n            eSoundMgr.play(url);\r\n            return;\r\n        }\r\n        var curMutex = this.curPlaySound[url];\r\n        if (!curMutex) { //当前不互斥才能继续播放相同音效\r\n            this.curPlaySound[url] = isMutex; //记录互斥标签\r\n            Laya.SoundManager.playSound(url, loops, Laya.Handler.create(null, () => {\r\n                delete SoundPlayer.curPlaySound[url];\r\n            }));\r\n        }\r\n    }\r\n\r\n    //非弹窗类按钮点击\r\n    public static clickSound(isMutex: boolean = false) {\r\n        this.play(\"assets/raw/click.mp3\", isMutex);\r\n    }\r\n    //关闭小弹窗(非全屏弹窗)\r\n    public static closeSound(isMutex: boolean = false) {\r\n        this.play(\"assets/raw/close.mp3\", isMutex);\r\n    }\r\n    public static enterbuySound(isMutex: boolean = false) {\r\n        this.play(\"assets/raw/enterbuyClick.mp3\", isMutex);\r\n    }\r\n    //进入游戏\r\n    public static enterGameSound(isMutex: boolean = false) {\r\n        this.play(\"assets/raw/enterGameClick.mp3\", isMutex);\r\n    }\r\n    //打开弹窗\r\n    public static enterPanelSound(isMutex: boolean = false) {\r\n        this.play(\"assets/raw/enterPanelClick.mp3\", isMutex);\r\n    }\r\n    //关闭全屏类弹窗\r\n    public static returnLobbySound(isMutex: boolean = false) {\r\n        SoundPlayer.closeSound(isMutex);//[下午1:54] Andersen Liao 大厅关闭音效 帮忙统一为小弹框关闭的音效\r\n        // this.play(\"assets/raw/returnLobbyClick.mp3\", isMutex);\r\n    }\r\n    /**\r\n     * 通过配置播放\r\n     * @param config \r\n     * @param isMutex \r\n     */\r\n    public static playConfig(config: SoundConfig, isMutex?: boolean, loops?: number) {\r\n        if (!loops) loops = 1;\r\n        let url: string = this.soundMap[config];\r\n        this.play(url, isMutex, loops);\r\n    }\r\n    /**\r\n     * 停止播放音效\r\n     * @param config \r\n     */\r\n    public static stopSound(config: SoundConfig) {\r\n        let url: string = this.soundMap[config];\r\n        if (isNative) url = this.getNativeSound(url);\r\n        Laya.SoundManager.stopSound(url);\r\n    }\r\n    private static getNativeSound(url: string): string {\r\n        let arr = url.split(\".mp3\");\r\n        return arr[0] + \".ogg\";\r\n    }\r\n}","import { DragonBoneAnim } from \"../../common/DragonBoneAnim\";\r\n\r\n/*\r\n* 通用导航菜单tab组件\r\n*/\r\nexport class MenuTabComp {\r\n    private tabOn: Laya.Image;//选中标签标题\r\n    private defIcon: Laya.Image;//默认标题\r\n    private anim: DragonBoneAnim;\r\n    private _selected: boolean;\r\n\r\n    public init(box: Laya.Image) {\r\n\r\n        this.tabOn = box.getChildByName(\"tab_on\") as Laya.Image;\r\n        this.tabOn.alpha = 0;\r\n        this.defIcon = box.getChildByName(\"defIcon\") as Laya.Image;\r\n        if (!this.defIcon) this.defIcon = box.getChildAt(0) as Laya.Image;\r\n\r\n        this.anim = new DragonBoneAnim();\r\n        this.anim.loadInit({ skUrl: \"./assets/animation/agent/btn.sk\", autoPlay: true });\r\n        this.tabOn.addChild(this.anim);\r\n        this.anim.pos(180, 55);\r\n    }\r\n    public set selected(value: boolean) {\r\n        this._selected = value;\r\n        this.defIcon.visible = !value;\r\n        if (value) {\r\n            this.tabOn.x = - 30;\r\n            Laya.Tween.clearTween(this.tabOn);\r\n            Laya.Tween.to(this.tabOn, { alpha: 1, x: 0 }, 400);\r\n            this.anim.resume();\r\n        } else {\r\n            Laya.Tween.clearTween(this.tabOn);\r\n            Laya.Tween.to(this.tabOn, { alpha: 0 }, 200);\r\n            this.anim.pause();\r\n        }\r\n    }\r\n    public get selecte
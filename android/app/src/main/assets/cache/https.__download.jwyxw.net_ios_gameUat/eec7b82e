per.onClosed(type);\r\n\t\tthis.destroy(true);\r\n\t}\r\n}\r\n","import { ConfObjRead } from \"../common/ConfObjRead\";\r\nimport { Debug } from \"../common/Debug\";\r\nimport { GameMgr } from \"../utils/GameMgr\";\r\nimport { HttpRequester } from \"./HttpRequester\";\r\n\r\n/**\r\n * 支付方式item数据结构(非必须的的字段未陈列出来)\r\n */\r\nexport interface PayItemVo {\r\n    code: string;//用于区分支付类型\r\n    description: string;//支付类型描述\r\n    id: number;\r\n    promotion: boolean;//标记是否需要显示惠\r\n    promotionTips: string;//优惠描述(需要显示到右侧titile)\r\n}\r\n/**\r\n * 支付方式详细列表(非必须的的字段未陈列出来)\r\n */\r\nexport interface PaySubListVo {\r\n    minAmount?: number;//最小数额\r\n    maxAmount?: number;//最大数额\r\n    merchantName?: string;//名称(如果是bank数据，则为收款人)\r\n    paymentId?: number;\r\n    paymentType?: string;\r\n    type?: string;//支付类型\r\n    //这几个参数为true时需要弹窗输入对应信息\r\n    realNameReq?: boolean;//是否需要输入真实姓名\r\n    cardNoReq?: boolean;//是否需要输入卡号\r\n    mobileNoReq?: boolean;//是否需要输入手机号\r\n\r\n    fixedAmount?: number[];//用于固定充值\r\n    recommendedAmounts?: number[];//用于三方转账支付\r\n    userPrompt?: string;//用户提示\r\n    //vip专用-------------------------\r\n    methodName?: string;\r\n    methodInfo?: string;\r\n    remarks?: string;//三方转账也有这个字段\r\n    vipTopUpType?: string;//vip充值类型\r\n    //网银支付专用--------------\r\n    platform?: string;\r\n    webBankList?: any[];//网银列表\r\n    //------从银行列表中合并进来的数据\r\n    combineVo?: BankSubListVo;\r\n    //---------以下信息在转张前会动态赋值\r\n    amount?: number;//需要充值的金额\r\n}\r\n/**\r\n * 银行转账列表(也包括了其他支付方式列表)\r\n */\r\nexport interface BankSubListVo {\r\n    adminBankId: number;\r\n    bankCardNo: string;//收款账号或二维码地址(非银行类型)\r\n    bankCode?: string;//其他支付方式特有\r\n    bankName: string;//开户银行\r\n    maxAmount: number;\r\n    minAmount: number;\r\n    receiptName: string;//收款人\r\n    remarks: string;\r\n    bankAddress?: string;//支行(开户网点)\r\n    realNameReq?: boolean;//是否需要输入真实姓名(为true时需要弹窗输入)\r\n    recommendedAmounts?: number[];//用于三方转账支付\r\n    isCombine?: boolean;//是否为合并数据\r\n    //---------以下信息在转张前会动态赋值\r\n    amount?: number;//需要充值的金额\r\n}\r\n/**\r\n * 充值明细(非必要字段未列出)\r\n */\r\nexport interface PayHistoryVo {\r\n    state: string;//订单状态\r\n    stateChineseDisplay: string;\r\n    subTypeChineseDisplay: string;//支付方式\r\n    transactionId: string;//订单号\r\n    createTime: string;//创建时间\r\n    amount: number;//支付金额\r\n    effectiveAmount: number;//总计金额\r\n}\r\n/**\r\n * ListV2方式需要的参数\r\n */\r\nexport interface PayInfoVo {\r\n    cardNo?: string;//卡号\r\n    mobileNo?: string;//手机号\r\n    realName?: string;//存款人姓名\r\n    bankCode?: string;//银行编码(针对网银)\r\n    cardType?: string;//银行卡类型(针对网银)\r\n}\r\n/**\r\n * 支付结果数据模型\r\n */\r\nexport interface PayResultVo {\r\n    amount: number;//支付金额\r\n    data?: string;//支付链接或二维码url\r\n    paymentJumpTypeEnum?: string;//支付处理方式\r\n    transactionId: string;//订单编号\r\n    webview: boolean;//是否跳转浏览器\r\n}\r\n/**\r\n * 二维码展示需要的数据模型\r\n */\r\nexport interface QRShowVo {\r\n    amount?: number;//充值金额\r\n    orderNo?: string;//订单号\r\n    qrUrl: string;//二维码链接\r\n    isBank?: boolean;//是否为银行类型\r\n    creatQR?: boolean;//是否需要自己生成二维码\r\n}\r\n/**\r\n * 网银列表数据\r\n */\r\nexport interface WebBankVo {\r\n    bankName: string;//银行名称\r\n    bankValue: string;\r\n    bankType?: string[];//DC-储蓄卡，CC-信用卡\r\n    cardType?: string;//由bankType分解而来\r\n}\r\n/**\r\n * 支付处理方式\r\n */\r\nexport enum PayMentJumpType {\r\n    URL = 1,\r\n    IMG,\r\n    FROM,\r\n    XML,\r\n    HTML\r\n}\r\n/**\r\n * vip充值类型\r\n */\r\nexport enum VipPayType {\r\n    OTHER,\r\n    ALIPAY,\r\n    WECHAT,\r\n    QQ,\r\n}\r\n/**\r\n * 支付类型\r\n */\r\nexport enum PayType {\r\n    OTHER,//银联支付\r\n    BANK,//银行转账\r\n    ZHB,//支付宝\r\n    JD,//京东\r\n    VIP,\r\n    QQ,\r\n    FIXED_WX,//微信固定金额\r\n    WX,\r\n    WX_PUBLIC,//微信公众号\r\n    FIXED_QQ,//qq固定金额\r\n    ONLINEBANK,//网银\r\n    THIRD_PARTY,//网银(因为后端数据没统一，所以有2个标记)\r\n    FIXED_ZHB,//支付宝固定金额\r\n}\r\n//充值三大类型\r\nexport enum ThreeType {\r\n    vip = 1,\r\n    fixedMoney,//固定额度\r\n    otherTransfer,//三方充值\r\n    bankTransfer,//银行转账\r\n    wechatPublic,//公众号\r\n}\r\n\r\n/*\r\n* 充值相关数据\r\n*/\r\nexport class PayModel {\r\n    public static payType: PayType;//当前支付类型(大类型)\r\n    public static btnMark: string = \"cache_payBtn\";//用于缓存按钮\r\n    public static useCache: boolean = true;//是否缓存数据\r\n    public static payList: PayItemVo[];\r\n    public static paySubMap: { [key: string]: PaySubListVo[] } = {};//用于缓存充值数据详细\r\n    public static bankSubList: BankSubListVo[];//银行转账支付\r\n    private static backAndOrList: BankSubListVo[];//为过滤前的数据\r\n    private static otherPayList: PaySubListVo[];//其他支付方式列表\r\n    //code对应的充值列表图标\r\n    public static tabIconMap = {\r\n        [PayType.OTHER]: \"ui/pay/payicon10.png\",\r\n        [PayType.BANK]: \"ui/pay/payicon09.png\",\r\n        [PayType.ZHB]: \"ui/pay/payicon11.png\",\r\n        [PayType.JD]: \"ui/pay/payicon02.png\",\r\n        [PayType.VIP]: \"ui/pay/payicon05.png\",\r\n        [PayType.QQ]: \"ui/pay/payicon03.png\",\r\n        [PayType.FIXED_WX]: \"ui/pay/payicon07.png\",\r\n        [PayType.WX]: \"ui/pay/payicon06.png\",\r\n        [PayType.WX_PUBLIC]: \"ui/pay/payicon01.png\",\r\n        [PayType.FIXED_QQ]: \"ui/pay/payicon04.png\",\r\n        [PayType.ONLINEBANK]: \"ui/pay/payicon08.png\",\r\n        [PayType.FIXED_ZHB]: \"ui/pay/payicon12.png\"\r\n    }\r\n    //vip充值方式中文\r\n    public static vipPayMap = {\r\n        [VipPayType.ALIPAY]: \"支付宝\",\r\n        [VipPayType.WECHAT]: \"微信\",\r\n        [VipPayType.QQ]: \"QQ\",\r\n    }\r\n    //网银卡类型\r\n    public static cardTypeMap = {\r\n        \"DC\": \"(储蓄卡)\",\r\n        \"CC\": \"(信用卡)\"\r\n    }\r\n    /**\r\n     * 检查支付类型\r\n     * @param code \r\n     */\r\n    public static checkType(code: string): ThreeType {\r\n        let type = PayType[code];\r\n        if (type == undefined) return null;\r\n        switch (type) {\r\n            case PayType.VIP: return ThreeType.vip;\r\n            case PayType.FIXED_WX:\r\n            case PayType.FIXED_QQ:\r\n            case PayType.FIXED_ZHB: return ThreeType.fixedMoney;\r\n            case PayType.BANK: return ThreeType.bankTransfer;\r\n            case PayType.WX_PUBLIC: return ThreeType.wechatPublic;\r\n        }\r\n        return ThreeType.otherTransfer;\r\n    }\r\n    /**\r\n     * 获取充值方式列表\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static getPayList(caller?: any, callback?: Function) {\r\n        if (this.useCache && this.payList) {\r\n            if (caller && callback) callback.call(caller);\r\n            return;\r\n        }\r\n\r\n        GameMgr.showCircleLoading(true);\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.payList, this, (suc: boolean, jobj: any) => {\r\n            GameMgr.showCircleLoading(false);\r\n            if (suc) {\r\n                this.payList = jobj;\r\n                if (caller && callback) callback.call(caller);\r\n            }\r\n        }, \"&version=5\");\r\n    }\r\n    /**\r\n     * 充值方式二级列表\r\n     * @param code \r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static getPayment(code: string, caller?: any, callback?: Function) {\r\n        if (this.useCache && this.paySubMap[code]) {\r\n            if (caller && callback) callback.call(caller, this.paySubMap[code]);\r\n            return;\r\n        }\r\n\r\n        let arr: PaySubListVo[] = this.otherPayList.filter(value => value.type == code);\r\n        if (code == PayType[PayType.ONLINEBANK]) {//网银有2个标记,所以需要特殊处理\r\n            let type = PayType[PayType.THIRD_PARTY];\r\n            let other = this.otherPayList.filter(value => value.type == type);\r\n            arr = arr.concat(other);\r\n        }\r\n        if (this.backAndOrList) {//将银行列表v2中的数据合并\r\n            let list: BankSubListVo[] = this.backAndOrList.filter(value => value.bankCode == code);\r\n            Debug.log(\"合并数据：\", list);\r\n            list.forEach(vo => {\r\n                vo.isCombine = true;\r\n                arr.push({//银行列表数据中不包括vip和固定金额充值\r\n                    combineVo: vo\r\n                });\r\n            });\r\n        }\r\n        this.paySubMap[code] = arr;\r\n        if (caller && callback) callback.call(caller, arr);\r\n    }\r\n    /**\r\n     * 获取银行转账列表(v2)\r\n     * 注意：此列表混杂了其他充值方式\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static getBanktransfers(caller?: any, callback?: Function) {\r\n        if (this.useCache && this.backAndOrList) {\r\n            if (caller && callback) callback.call(caller);\r\n            return;\r\n        }\r\n        GameMgr.showCircleLoading(true);\r\n        //.../banklist/v2\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.banktransfers, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                this.backAndOrList = jobj;\r\n                Debug.log(\"bankList:\", jobj);\r\n                this.bankSubList = this.backAndOrList.filter(value => value.bankCode == undefined);\r\n            }\r\n            this.getOtherPayList(caller, callback);\r\n        });\r\n    }\r\n    /**\r\n     * 获取其他充值方式列表(listV2)\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static getOtherPayList(caller?: any, callback?: Function) {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.payment, this, (suc: boolean, jobj: any) => {\r\n            GameMgr.showCircleLoading(false);\r\n            if (suc) {\r\n                Debug.log(\"otherList:\", jobj);\r\n                if (jobj.http || typeof (jobj) == \"string\") this.otherPayList = [];\r\n                else this.otherPayList = jobj;\r\n                if (caller && callback) callback.call(caller);\r\n            } else {\r\n                this.otherPayList = [];\r\n            }\r\n        });//\"&paymentNameCode=\" + code 传参数就会返回对应数据，否则返回全部数据\r\n    }\r\n    /**\r\n     * 充值方式-针对银行列表数据类型\r\n     * @param data \r\n     * @param money \r\n     * @param depositorName 存款人 \r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static payByBankList(vo: BankSubListVo, money: number, depositorName?: string, caller?: any, callback?: Function) {\r\n        GameMgr.showCircleLoading(true);\r\n\r\n        let data: any = {\r\n            adminBankId: vo.adminBankId,//银行卡id\r\n            topupAmount: money\r\n        };\r\n        if (depositorName) {\r\n            data.depositorName = depositorName;\r\n        }\r\n        //银行转账需要带的参数\r\n        if (!vo.isCombine) data.transferTopupType = \"BANK_ONLINE\";\r\n        //transfer/topups/banktransfers/v4\r\n        HttpRequester.putHttpData(ConfObjRead.httpCmd.payByBankList, data, this, (suc: boolean, jobj: any) => {\r\n            GameMgr.showCircleLoading(false);\r\n            if (caller && callback) callback.call(caller, suc, jobj);\r\n        });\r\n    }\r\n    /**\r\n     * 充值方式-ListV2类型\r\n     * @param data \r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static payByListV2(vo: PaySubListVo, money: number, info?: PayInfoVo, caller?: any, callback?: Function) {\r\n        let data: any = {\r\n            paymentId: vo.paymentId,\r\n            paymentType: vo.paymentType,\r\n            topupAmount: money\r\n        };\r\n        if (vo.platform) {//纠正网银支付的这个数据(可能管端给的有误)\r\n            data.paymentType = \"THIRD\";\r\n        }\r\n        if (info) {\r\n            if (info.cardNo) data.cardNo = info.cardNo;\r\n            if (info.mobileNo) data.mobileNo = info.mobileNo;\r\n            if (info.realName) data.realName = info.realName;\r\n            if (vo.platform) {//说明是网银支付\r\n                if (info.bankCode) data.bankCode = info.bankCode;\r\n                if (info.cardType) data.cardType = info.cardType;\r\n            }\r\n        }\r\n        Debug.log(\"ListV2支付:\", data);\r\n        GameMgr.showCircleLoading(true);\r\n        //cashmgt/me/transfer/topups\r\n        HttpRequester.postHttpData(ConfObjRead.httpCmd.payByListV2, data, this, (suc: boolean, jobj: any) => {\r\n            GameMgr.showCircleLoading(false);\r\n            if (caller && callback) callback.call(caller, suc, jobj);\r\n        });\r\n    }\r\n    /**\r\n     * 获取网银列表(网银支付需要)\r\n     * @param paymentSetId \r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static getOnlineBankList(paymentSetId: string, caller?: any, callback?: Function) {\r\n        GameMgr.showCircleLoading(true);\r\n        let params = \"&paymentSetId=\" + paymentSetId;\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.onlineBankList, this, (suc: boolean, jobj: any) => {\r\n            GameMgr.showCircleLoading(false);\r\n            if (caller && callback) callback.call(caller, suc, jobj);\r\n        }, params);\r\n    }\r\n    /**\r\n     * 充值明细\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static getPayHistory(caller?: any, callback?: Function) {\r\n        GameMgr.showCircleLoading(true);\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.payHistory, this, (suc: boolean, jobj: any) => {\r\n            GameMgr.showCircleLoading(false);\r\n            if (suc) {\r\n                if (caller && callback) callback.call(caller, jobj);\r\n            }\r\n        }, \"&pageSize=100&start=0&state=&type=TOPUP\");\r\n    }\r\n    //清理缓存\r\n    public static clear() {\r\n        this.payList = null;\r\n        this.paySubMap = {};\r\n        this.bankSubList = null;\r\n        this.backAndOrList = null;\r\n        this.otherPayList = null;\r\n    }\r\n}\r\n","/**Created by the LayaAirIDE*/\r\n\r\nimport { PaySubListVo } from \"../../../data/PayModel\";\r\nimport { EventManager } from \"../../../events/EventManager\";\r\nimport { ui } from \"../../../ui/layaMaxUI\";\r\n\r\n/**\r\n * 固定金额item\r\n */\r\nexport class FixedMoneyItem extends ui.sceneUI.dlg.pay.FixedMoneyItemUI {\r\n\tpublic vo: PaySubListVo;\r\n\tpublic _selected: boolean;\r\n\tprivate txtColors = [\"#8eb5ee\", \"#663d12\"];//默认颜色，选中颜色\r\n\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t\tthis.selectBg.alpha = 0;\r\n\t}\r\n\tpublic readData(vo: PaySubListVo) {\r\n\t\tthis.vo = vo;\r\n\t\tif (vo.combineVo) {//说明是银行列表合并数据\r\n\t\t\tthis.titleTxt.text = vo.combineVo.receiptName;\r\n\t\t\tthis.scopeTxt.text = \"￥\" + vo.combineVo.minAmount + \"-\" + vo.combineVo.maxAmount;\r\n\t\t} else {\r\n\t\t\tthis.titleTxt.text = vo.merchantName;\r\n\t\t\tthis.scopeTxt.text = \"￥\" + vo.minAmount + \"-\" + vo.maxAmount;\r\n\t\t}\r\n\t\tthis.titleTxt.color = this.txtColors[0];\r\n\t\tthis.scopeTxt.color = this.txtColors[0];\r\n\t}\r\n\tpublic set selected(value: boolean) {\r\n\t\tthis._selected = value;\r\n\t\tLaya.Tween.to(this.selectBg, { alpha: value ? 1 : 0 }, 300);\r\n\t\tlet index = value ? 1 : 0;\r\n\t\tthis.titleTxt.color = this.txtColors[index];\r\n\t\tthis.scopeTxt.color = this.txtColors[index];\r\n\t}\r\n\tpublic get selected(): boolean {\r\n\t\treturn this._selected;\r\n\t}\r\n\tpublic destroy(bl?: boolean) {\r\n\t\tEventManager.removeAllEvents(this);\r\n\t\tsuper.destroy(true);\r\n\t}\r\n}\r\n","/**Created by the LayaAirIDE*/\r\n\r\nimport { ui } from \"../../../ui/layaMaxUI\";\r\nimport { WebBankVo, PayModel } from \"../../../data/PayModel\";\r\n\r\n/**\r\n * 网银列表Item\r\n */\r\nexport class WebBankItem extends ui.sceneUI.dlg.pay.WebBankItemUI {\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t}\r\n\tpublic readData(vo: WebBankVo, cur: WebBankVo) {\r\n\t\tlet str = vo.bankName;\r\n\t\tif (vo.cardType) str += PayModel.cardTypeMap[vo.cardType];\r\n\t\tthis.nameTxt.text = str;\r\n\t\tif (vo == cur) this.setState(true);\r\n\t\telse this.setState(false);\r\n\t}\r\n\tpublic setState(vl: boolean) {\r\n\t\tthis.alpha = vl ? 0.6 : 1;\r\n\t}\r\n}\r\n","import { WebBankVo } from \"../../../data/PayModel\";\r\nimport { EventManager } from '../../../events/EventManager';\r\n/**Created by the LayaAirIDE*/\r\nimport { ui } from \"../../../ui/layaMaxUI\";\r\nimport { SoundPlayer } from '../../../utils/SoundPlayer';\r\nimport { Toast } from '../../toast/Toast';\r\nimport { ComponentTool } from './../../../utils/GameUtils';\r\nimport { WebBankItem } from './WebBankItem';\r\n\r\n/**\r\n * 网银列表\r\n */\r\nexport class WebBankDlg extends ui.sceneUI.dlg.pay.WebBankDlgUI {\r\n\r\n\tconstructor(private data: WebBankVo[], private caller?: any, private callback?: Function) {\r\n\t\tsuper();\r\n\t\tthis.initView();\r\n\t}\r\n\tpublic static show(data: WebBankVo[], caller?: any, callback?: Function) {\r\n\t\tlet dlg = new WebBankDlg(data, caller, callback);\r\n\t\tdlg.popup(false, true);\r\n\t}\r\n\tprivate initView() {\r\n\t\tComponentTool.setListSlide(this.itemList);\r\n\t\tthis.itemList.itemRender = WebBankItem;\r\n\t\tthis.itemList.spaceY = 0;\r\n\r\n\t\tthis.itemList.renderHandler = Laya.Handler.create(this, this.renderItems, null, false);\r\n\t\tthis.itemList.selectEnable = true;\r\n\r\n\t\tthis.itemList.array = this.resolveData();\r\n\t\t//\r\n\t\tEventManager.addTouchScaleListener(this.closeBtn, this, () => {\r\n\t\t\tSoundPlayer.closeSound();\r\n\t\t\tthis.close(null);\r\n\t\t});\r\n\t\tEventManager.addTouchScaleListener(this.confirmBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tif (!this.itemList.selectedItem) {\r\n\t\t\t\tToast.showToast(\"请选择银行\");\r\n\t\t\t\treturn;\r\n\t\t\t}\r\n\t\t\tif (this.caller && this.callback) this.callback.call(this.caller, this.itemList.selectedItem);\r\n\t\t\tthis.close(null);\r\n\t\t});\r\n\t}\r\n\t//分解数据\r\n\tprivate resolveData(): WebBankVo[] {\r\n\t\tlet arr = [];\r\n\t\tthis.data.forEach(value => {\r\n\t\t\tif (value.bankType) {\r\n\t\t\t\tthis.addItem(arr, value);\r\n\t\t\t} else {\r\n\t\t\t\tarr.push(value);\r\n\t\t\t}\r\n\t\t});\r\n\t\treturn arr;\r\n\t}\r\n\tprivate addItem(arr: WebBankVo[], vo: WebBankVo) {\r\n\t\tvo.bankType.forEach(value => {\r\n\t\t\tarr.push({\r\n\t\t\t\tbankName: vo.bankName,\r\n\t\t\t\tbankValue: vo.bankValue,\r\n\t\t\t\tcardType: value\r\n\t\t\t})\r\n\t\t});\r\n\t}\r\n\tprivate renderItems(cell: WebBankItem) {\r\n\t\tlet vo = this.itemList.selectedItem;\r\n\t\tcell.readData(cell.dataSource, vo);\r\n\t}\r\n\tonClosed(type?: string) {\r\n\t\tsuper.onClosed(type);\r\n\t\tthis.destroy(true);\r\n\t}\r\n}\r\n","/**Created by the LayaAirIDE*/\r\n\r\nimport { PayInfoVo, PayModel, PaySubListVo, WebBankVo } from \"../../../data/PayModel\";\r\nimport { EventManager } from \"../../../events/EventManager\";\r\nimport { ui } from \"../../../ui/layaMaxUI\";\r\nimport { GameUtils } from \"../../../utils/GameUtils\";\r\nimport { SoundPlayer } from \"../../../utils/SoundPlayer\";\r\nimport { PayManager } from \"./PayManager\";\r\nimport { WebBankDlg } from \"./WebBankDlg\";\r\n\r\n/**\r\n * 用于填写存款信息\r\n */\r\nexport class AccountInfoView extends ui.sceneUI.dlg.pay.AccountInfoViewUI {\r\n\tprivate uiList: Laya.Sprite[] = [];\r\n\tprivate webBankList: any[];\r\n\tprivate curBankVo: WebBankVo;\r\n\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t\tthis.initView();\r\n\t}\r\n\tprivate initView() {\r\n\t\tthis.centerX = this.centerY = 0;\r\n\t\tthis.bankNameTxt.editable = false;\r\n\t\tthis.bankNameTxt.mouseEnabled = false;\r\n\t\tthis.uiList.push(this.nameGroup, this.phoneGroup, this.bankNumGroup, this.bankGroup);\r\n\t}\r\n\t/**\r\n\t * \r\n\t * @param vo \r\n\t * @param isBank 是否为银行类型\r\n\t */\r\n\tpublic show(vo: any, isBank: boolean) {\r\n\t\tif (isBank) {\r\n\t\t\tthis.layoutUI(true);\r\n\t\t} else {\r\n\t\t\tlet data = vo as PaySubListVo;\r\n\t\t\tthis.layoutUI(data.realNameReq, data.mobileNoReq, data.cardNoReq, Boolean(data.webBankList));\r\n\t\t\tthis.webBankList = data.webBankList;\r\n\t\t}\r\n\t\t//上一步\r\n\t\tEventManager.addTouchScaleListener(this.prevBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tPayManager.inst.showLastView();\r\n\t\t});\r\n\t\t//确定按钮\r\n\t\tEventManager.addTouchScaleListener(this.confirmBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tif (!this.checkInput()) return;\r\n\r\n\t\t\tif (isBank) {\r\n\t\t\t\tPayManager.inst.showBankPayInfoView(vo, this.nameTxt.text);\r\n\t\t\t} else {\r\n\t\t\t\t
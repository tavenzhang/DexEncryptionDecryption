(this.moneyTxt, { caller: this, callback: this.inputEnd });\r\n\t}\r\n\t//输入完毕\r\n\tprivate inputEnd(value: string) {\r\n\t\tif (!value || (this.prevBtn && this.prevBtn.value.toString() != value)) this.clearLastBtn();\r\n\t}\r\n\t//重置选择金额按钮\r\n\tprivate clearLastBtn() {\r\n\t\tif (this.prevBtn) {\r\n\t\t\tthis.prevBtn.setSelect(false);\r\n\t\t\tthis.prevBtn = null;\r\n\t\t}\r\n\t}\r\n\t//开始提交>>>\r\n\tprivate doSubmit(vo: PaySubListVo, money: number) {\r\n\t\tlet isBank: boolean = Boolean(vo.combineVo);\r\n\t\tPayManager.inst.startPay(isBank ? vo.combineVo : vo, money, isBank);\r\n\t}\r\n\t//点击分类item\r\n\tprivate clickItem(e: Laya.Event) {\r\n\t\tif (e.target instanceof FixedMoneyItem) {\r\n\t\t\tSoundPlayer.enterPanelSound();\r\n\t\t\tlet item = e.target;\r\n\t\t\tthis.selectItem(item);\r\n\t\t}\r\n\t}\r\n\tprivate selectItem(item: FixedMoneyItem) {\r\n\t\tif (item.selected) return;\r\n\t\titem.selected = true;\r\n\t\tif (this.prev) this.prev.selected = false;\r\n\t\tthis.prev = item;\r\n\t\tlet vo = item.vo;\r\n\t\tif (vo.combineVo) {\r\n\t\t\tthis.creatMoneyBtn(vo.combineVo.recommendedAmounts);\r\n\t\t\tthis.infoTxt.text = vo.combineVo.remarks || \"---\";\r\n\t\t} else {\r\n\t\t\tthis.creatMoneyBtn(vo.recommendedAmounts);\r\n\t\t\tthis.infoTxt.text = vo.remarks || \"---\";\r\n\t\t}\r\n\r\n\t}\r\n\t//创建常用充值金额按钮\r\n\tprivate creatMoneyBtn(arr: any[]) {\r\n\t\tthis.recoverBtn();\r\n\t\tif (!arr || arr.length == 0) {\r\n\t\t\tDebug.error(\"没有配置常用金额数据\");\r\n\t\t\treturn;\r\n\t\t}\r\n\t\tlet btn: PayMoneyBtn;\r\n\t\tlet column = 4;//列数\r\n\t\tlet gapx = 11;\r\n\t\tlet gapy = 8;\r\n\t\tarr.forEach((value, index) => {\r\n\t\t\tif (index < this.maxNum) {\r\n\t\t\t\tbtn = Laya.Pool.getItem(PayModel.btnMark);\r\n\t\t\t\tif (!btn) btn = new PayMoneyBtn();\r\n\t\t\t\tbtn.value = value;\r\n\t\t\t\tbtn.mouseEnabled = true;\r\n\t\t\t\tbtn.x = (index % column) * (btn.width + gapx);\r\n\t\t\t\tbtn.y = Math.floor(index / column) * (btn.height + gapy);\r\n\t\t\t\tthis.btnBox.addChil
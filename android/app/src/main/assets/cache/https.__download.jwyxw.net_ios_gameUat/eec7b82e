istener(this.closeBtn, this, () => {\r\n            SoundPlayer.closeSound();\r\n            DownLoadView.hide();\r\n        });\r\n    }\r\n    /**\r\n     * 进入游戏时显示虚拟进度\r\n     */\r\n    public showVirtualProgress() {\r\n        this.progressValue = 0.01;\r\n        this.virtualValue = 0;\r\n        this.timerLoop(300, this, this.addVirtualValue);\r\n    }\r\n    private addVirtualValue() {\r\n        this.virtualValue += 0.08;\r\n        if (this.virtualValue >= 1) {\r\n            this.virtualValue = 1;\r\n            this.clearTimer(this, this.addVirtualValue);\r\n        }\r\n        this.progressValue = this.virtualValue;\r\n    }\r\n    /**\r\n\t * 设置下载进度值\r\n\t */\r\n    public set progressValue(value: number) {\r\n        value = Math.floor(value * 100);\r\n        if (value >= 100) value = 100;\r\n        this.end = this.start + value * 3.6;\r\n        this.numTxt.text = value + \"%\";\r\n        if (this.end <= 270) {\r\n            this.cirmask.graphics.clear();\r\n            if (this.end == 270) this.end = 269.99;//如果刚好等于270就看不到效果了\r\n            this.cirmask.graphics.drawPie(0, 0, this.radius, this.start, this.end, \"#ff0000\");\r\n        } else {\r\n            this.numTxt.text = \"100%\";\r\n        }\r\n    }\r\n    public set txtValue(txt: string) {\r\n        this._txt = txt;\r\n        this.infoTxt.text = txt;\r\n\r\n        this.startTimer();\r\n    }\r\n    private startTimer() {\r\n        this.txtArr = this._txt.split(\"\");\r\n        this.ptxt = \"\";\r\n\r\n        this.clearTimer(this, this.playTxt);\r\n        this.timer.clear(this, this.startLoop);\r\n        this.timer.once(1500, this, this.startLoop);\r\n    }\r\n    private startLoop() {\r\n        this.timerLoop(90, this, this.playTxt);\r\n    }\r\n    private playTxt() {\r\n        if (this.txtArr && this.txtArr.length > 0) {\r\n            this.ptxt += this.txtArr.shift();\r\n            this.infoTxt.text = this.ptxt;\r\n        } else {\r\n            this.startTimer();\r\n        }\r\n    }\r\n    onDisable() {\r\n        this.progressValue = 0.01;//warn:设置为0不会clear掉,有待解决\r\n        this.closeBtn.visible = false;\r\n        this.clearTimer(this, this.playTxt);\r\n        this.timer.clear(this, this.startLoop);\r\n        this.clearTimer(this, this.addVirtualValue);\r\n    }\r\n    //停止加载时的文字动画\r\n    public stopTxtAnim() {\r\n        this.clearTimer(this, this.playTxt);\r\n        this.timer.clear(this, this.startLoop);\r\n        this.infoTxt.text = \"当前网络不好，请耐心等候或者关闭\";\r\n    }\r\n    /**\r\n     * 显示加载界面\r\n     * @param download 是否为游戏下载过程，否则为游戏进入过程\r\n     */\r\n    public static show(download: boolean = true) {\r\n        if (!this.inst) {\r\n            this.inst = new DownLoadView();\r\n            this.inst.zOrder = Laya.Dialog.manager.zOrder + 1;\r\n        }\r\n        let str: string = download ? \"正在下载更新游戏...\" : \"正在进入游戏...\";\r\n        this.inst.txtValue = str;\r\n        if (!Laya.stage.contains(this.inst)) Laya.stage.addChild(this.inst);\r\n\r\n        Laya.timer.clear(this, this.showClose);\r\n        if (download) Laya.timer.once(60 * 1000, this, this.showClose, [download]);\r\n        else {\r\n            Laya.timer.once(30 * 1000, this, this.showClose, [download]);\r\n            this.inst.showVirtualProgress();\r\n        }\r\n    }\r\n    //显示关闭按钮\r\n    private static showClose(download: boolean) {\r\n        this.inst.closeBtn.visible = true;\r\n        if (!download) this.inst.stopTxtAnim();\r\n    }\r\n    /**\r\n     * 进度更新\r\n     * @param value \r\n     */\r\n    public static update(value: number) {\r\n        if (!this.inst) return;\r\n        this.inst.progressValue = value;\r\n    }\r\n    /**\r\n     * 关闭加载界面\r\n     */\r\n    public static hide() {\r\n        Laya.timer.clear(this, this.showClose);\r\n        if (this.inst) {\r\n            if (!Laya.stage.contains(this.inst)) return;\r\n\r\n            this.inst.removeSelf();\r\n        }\r\n    }\r\n}","import { EventManager } from \"../../events/
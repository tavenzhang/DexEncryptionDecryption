kCard) {//如果有绑定卡\r\n\t\t\tif (Common.alipayInfo || Common.bankInfo) {\r\n\t\t\t\tthis.readDetails();\r\n\t\t\t} else {\r\n\t\t\t\tEventManager.register(EventType.GET_BACKCARD_DETAIL, this, this.readDetails);\r\n\t\t\t}\r\n\t\t} else {\r\n\t\t\tthis.readDetails();\r\n\t\t}\r\n\t}\r\n\tprivate readDetails() {\r\n\t\tLaya.Tween.to(this.tab1, { alpha: 1 }, 300);\r\n\t\tLaya.Tween.to(this.tab2, { alpha: 1 }, 300, null, null, 100);\r\n\t\tthis.tab2.mouseEnabled ? this.showDetailsView(false, this.tab2) : this.showDetailsView(true, this.tab1);\r\n\r\n\t\t//\r\n\t\tEventManager.addTouchScaleListener(this.tab1, this, () => {\r\n\t\t\tSoundPlayer.enterPanelSound();\r\n\t\t\tthis.showDetailsView(true, this.tab1);\r\n\t\t}, null, 0.7, true);\r\n\t\tEventManager.addTouchScaleListener(this.tab2, this, () => {\r\n\t\t\tSoundPlayer.enterPanelSound();\r\n\t\t\tthis.showDetailsView(false, this.tab2);\r\n\t\t}, null, 0.7, true);\r\n\t}\r\n\tprivate showDetailsView(isbank: boolean, tab: WithDrawTab) {\r\n\t\ttab.selected = true;\r\n\t\ttab.mouseEnabled = false;\r\n\t\tif (this.lastTab) {\r\n\t\t\tthis.lastTab.selected = false;\r\n\t\t\tthis.lastTab.mouseEnabled = true;\r\n\t\t}\r\n\t\tthis.lastTab = tab;\r\n\r\n\t\tthis.vbox.destroyChildren();\r\n\t\tif (isbank) {//提取到银行卡\r\n\t\t\tlet bv = new DrawBankView();\r\n\t\t\tthis.vbox.addChild(bv);\r\n\t\t} else {//提取到支付宝\r\n\t\t\tlet av = new DrawAlipayView();\r\n\t\t\tthis.vbox.addChild(av);\r\n\t\t}\r\n\t}\r\n\tonClosed(type?: string) {\r\n\t\tLobbyModel.getCardInfo();//刷新状态(后台可能会更新)\r\n\t\tWithDrawModel.getBetInfo();//刷新状态\r\n\t\tEventManager.removeAllEvents(this);\r\n\t\tsuper.onClosed(type);\r\n\t\tthis.destroy(true);\r\n\t}\r\n}\r\n","import { Debug } from './../common/Debug';\r\nimport { GameIconView } from './../view/comp/GameIconView';\r\nimport { DownLoadView } from './../view/DownLoadView';\r\nimport { Toast } from './../view/toast/Toast';\r\nexport class GameUpdateMgr {\r\n    public static downloadPath: string;//用于游戏zip包下载的目录\r\n    public static gameResPath: string;//用于游戏解压的目录\r\n    public static imgPath: string;//用于存图片\r\n    public static localGameVerPath: string;//存放在本地的游戏版本号文件\r\n\r\n    public static game: GameIconView;//当前正在下载的游戏\r\n    public static file: string = \"file://\";//解压到本地的路径需要带上这个标记才能正常打开\r\n    public static gameVerUrl: string;//游戏版本号文件的网络地址\r\n    public static domain: string;//域名\r\n\r\n    private static webver: GameVerVo[];//服务器当前的游戏版本号数据\r\n    private static localver: any;//本地数据\r\n    private static curgameLabel: string;//当前需要下载的游戏标记\r\n    private static curgameVer: string;//当前需要下载的游戏版本号\r\n    private static gameZipUrl: string;//当前游戏的下载地址\r\n    //初始化\r\n    public static init() {\r\n        if (!Laya.Render.isConchApp) return;\r\n        this.downloadPath = appcache.getCachePath() + \"/gameZip\";\r\n        this.gameResPath = appcache.getCachePath() + \"/twgameRes\";\r\n        this.imgPath = appcache.getCachePath() + \"/imgRes\";\r\n        this.localGameVerPath = appcache.getCachePath() + \"/gamever.json\";\r\n    }\r\n    /**\r\n     * 检测当前需要加载的游戏是读取本地资源还是网络资源\r\n     * @param label \r\n     */\r\n    public static isReadLocal(label: string): boolean {\r\n        let filter: GameVerVo[] = this.webver.filter(value => value.name == label);\r\n        if (filter.length > 0) return true;\r\n        return false;\r\n    }\r\n    /**\r\n     * 读取对比游戏版本号文件\r\n     */\r\n    public static readGameVer() {\r\n        if (!Laya.Render.isConchApp) return;\r\n        let ver: string = Laya.loader.getRes(this.gameVerUrl);\r\n        this.webver = JSON.parse(ver);\r\n    }\r\n    /**\r\n     * 检测游戏是否需要下载\r\n     * @param label \r\n     */\r\n    public static checkDownload(label: string): boolean {\r\n        if (!fs_exists(this.localGameVerPath)) {//说明第一次下载游戏，本地还没有缓存\r\n            return true;\r\n        }\r\n\r\n        if (!this.localver) {//先读取本地数据\r\n            let verstr = readFileSync(this.localGameVerPath, \"utf-8\");\r\n            this.localver = JSON.parse(verstr);\r\n        }\r\n        if (!this.localver) this.localver = {};\r\n\r\n        let filterWeb: GameVerVo[] = this.webver.filter(value => value.name == label);\r\n        //说明该游戏没有配置，直接读取网络地址\r\n        if (filterWeb.length == 0) return false;\r\n        let curver: string = filterWeb[0].current_version;\r\n        let value: string = this.localver[label];\r\n        //说明本地还没有缓存该游戏，需要进行下载\r\n        if (!value || curver != value) {\r\n            this.curgameLabel = label;\r\n            this.curgameVer = curver;\r\n            this.gameZipUrl = this.domain + label + \"/\" + label + \"?v=\" + curver;\r\n            return true;\r\n        }\r\n        return false;\r\n    }\r\n    //删除游戏zip下载目录\r\n    public static delzipDir() {\r\n        if (!Laya.Render.isConchApp) return;\r\n        if (fs_exists(this.downloadPath)) {\r\n            let dl = fs_rm(this.downloadPath);\r\n        }\r\n    }\r\n    /**\r\n     * 开始下载游戏zip包\r\n     */\r\n    public static downloadGame() {\r\n        if (!Laya.Render.isConchApp) return;\r\n        Debug.print(\"download:\" + this.gameZipUrl);\r\n        this.delzipDir();\r\n        downloadBigFile(this.gameZipUrl, this.downloadPath, this.downloadProgress, this.downloadEnd, 10, 100000000);\r\n    }\r\n    private static downloadProgress(total: number, now: number, speed: number): boolean {\r\n        let value: number = now / total;\r\n        // let speedstr = (speed / 1024).toFixed(2) + \"M/s\";\r\n        // let size = (total / (1024 * 1024)).toFixed(2) + \"M\";\r\n        DownLoadView.update(value);\r\n        return false;\r\n    }\r\n    private static downloadEnd(curlret: number, httpret: number) {\r\n        if (curlret != 0 || httpret < 200 || httpret >= 300) {\r\n            // PageMgr.log(\"download-err:\" + httpret);\r\n            return;\r\n        }\r\n        GameUpdateMgr.unzipGame();\r\n    }\r\n    //解压游戏\r\n    private static unzipGame() {\r\n        let zip = new ZipFile();\r\n        let start = Laya.Browser.now();\r\n        let total: number = 0;//文件总数\r\n        let errFiles: string[] = [];//解压失败的文件统计\r\n        let errDirs: string[] = [];//目录创建失败的统计\r\n        let groot: string = null;//游戏根目录\r\n\r\n        if (zip.setSrc(this.downloadPath)) {\r\n            zip.forEach((id, name, isdir, sz) => {\r\n                if (isdir) {\r\n                    let dir = this.gameResPath + \"/\" + name;\r\n                    if (!fs_exists(dir)) {\r\n                        let mkdir = fs_mkdir(dir);\r\n                        if (!mkdir) errDirs.push(name);\r\n                    }\r\n                    if (!groot) {\r\n                        groot = name.split(\"/\")[0];\r\n                    }\r\n                } else {\r\n                    let buf = zip.readFile(id);\r\n                    let file = this.gameResPath + \"/\" + name;\r\n                    let saved = fs_writeFileSync(file, buf);\r\n                    if (!saved && sz != 0) {//保存失败了(应该是目录没创建)\r\n                        let arr = name.split(\"/\");\r\n                        arr.pop();\r\n                        let temp = arr.join(\"/\");\r\n                        let fileDir = this.gameResPath + \"/\" + temp;\r\n                        let ismk: boolean;\r\n                        if (!fs_exists(fileDir)) ismk = fs_mkdir(fileDir);\r\n                        saved = fs_writeFileSync(file, buf);\r\n                        if (!saved) errFiles.push(name);\r\n                    }\r\n                    total++;\r\n                }\r\n            });\r\n            zip.close();\r\n        }\r\n        let time = Laya.Browser.now() - start;\r\n        if (errFiles.length > 0 || errDirs.length > 0) {\r\n            Toast.showToast(\"游戏解压异常：\" + errFiles.length + \"-\" + errDirs.length);\r\n        } else {//解压成功\r\n            this.localver[this.curgameLabel] = this.curgameVer;\r\n            let saved = fs_writeFileSync(this.localGameVerPath, JSON.stringify(this.localver));\r\n            if (!saved) Toast.showToast(\"版本号保存异常!\");\r\n            if (this.game) {\r\n                this.game.onLaunchGame();\r\n                this.game = null;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\nexport interface GameVerVo {\r\n    current_version: string;//当前游戏版本号\r\n    last_success_version?: string;//\r\n    name: string;//游戏服务器相对地址(对应GameIconVo.url,需要截取)\r\n}","\r\nimport { ConfObjRead } from \"../../../common/ConfObjRead\";\r\nimport { Debug } from \"../../../common/Debug\";\r\nimport { InputExtend } from \"../../../common/InputExtend\";\r\nimport { VerCodeType } from \"../../../data/GameVos\";\r\nimport { HttpRequester } from \"../../../data/HttpRequester\";\r\nimport { LoginMethod, LoginModel } from \"../../../data/LoginModel\";\r\nimport { EventManager } from \"../../../events/EventManager\";\r\nimport { ui } from \"../../../ui/layaMaxUI\";\r\nimport { GameMgr } from \"../../../utils/GameMgr\";\r\nimport { GameUtils } from \"../../../utils/GameUtils\";\r\nimport { SoundPlayer } from \"../../../utils/SoundPlayer\";\r\nimport { KeyboardView } from \"../../comp/KeyboardView\";\r\nimport { Toast } from \"../../toast/Toast\";\r\n\r\n/**\r\n * 找回密码\r\n */\r\nexport class FindPasswordDlg extends ui.sceneUI.dlg.login.FindPasswordDlgUI {\r\n\tprivate pwdExtend1: InputExtend;\r\n\tprivate pwdExtend2: InputExtend;\r\n\tprivate codeTime: number;\r\n\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t\tthis.initView();\r\n\t}\r\n\tpublic static show() {\r\n\t\tlet dlg = new FindPasswordDlg();\r\n\t\tdlg.popup(false, true);\r\n\t}\r\n\tprivate initView() {\r\n\t\t// this.pwdExtend1 = InputExtend.getInput(this.pwdTxt1);\r\n\t\t// this.pwdExtend2 = InputExtend.getInput(this.pwdTxt2);\r\n\t\t//关闭\r\n\t\tEventManager.addTouchScaleListener(this.closeBtn, this, () => {\r\n\t\t\tSoundPlayer.closeSound();\r\n\t\t\tthis.close(null);\r\n\t\t});\r\n\t\t//获取验证码\r\n\t\tEventManager.addTouchScaleListener(this.codeBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tthis.getPhoneVerCode();\r\n\t\t});\r\n\t\t//确定\r\n\t\tEventManager.addTouchScaleListener(this.confirmBtn, this, () => {\r\n\t\t\tSoundPlayer.clickSound();\r\n\t\t\tlet pwd1 = this.pwdExtend1 ? this.pwdExtend1.text : this.pwdTxt1.text;\r\n\t\t\tlet pwd2 = this.pwdExtend2 ? this.pwdExtend2.text : this.pwdTxt2.text;\r\n\t\t\tif (!this.checkTxt()) return;\r\n\t\t\tif (pwd1 != pwd2) {\r\n\t\t\t\tToast.showToast(\"两次密码输入不一致\");\r\n\t\t\t\treturn;\r\n\t\t\t}\r\n\t\t\tlet epwd = GameUtils.encryptPwd(pwd1);\r\n\t\t\tlet data = {\r\n\t\t\t\tisWep: !GameUtils.isNativeApp,\r\n\t\t\t\tmode: \"PASSWORD\",\r\n\t\t\t\tnewPassword: epwd,\r\n\t\t\t\tphoneNumber: this.phoneTxt.text,\r\n\t\t\t\tverificationCode: this.codeTxt.text\r\n\t\t\t}\r\n\t\t\tGameMgr.showCircleLoading(true);\r\n\t\t\tLoginModel.findPassword(ConfObjRead.httpCmd.findPassword, data, this, this.findResult);\r\n\t\t});\r\n\t\tKeyboardView.bindKeyboard(this.phoneTxt, { maxLen: 11 });\r\n\t\tKeyboardView.bindKeyboard(this.codeTxt, { maxLen: 6 });\r\n\t}\r\n\tprivate getPhoneVerCode() {\r\n\t\tif (this.phoneTxt.text == \"\") {\r\n\t\t\tToast.showToast(\"手机号不能为空\");\r\n\t\t\treturn;\r\n\t\t}\r\n\t\tthis.codeTxt.text = \"\";\r\n\t\tthis.codeBtn.visible = false;\r\n\t\tthis.codeTime = 60;\r\n\t\tthis.timeTxt.text = this.codeTime.toString();\r\n\t\tLaya.timer.loop(1000, this, this.updateCodeTime);\r\n\t\tHttpRequester.getPhoneVercode(this.phoneTxt.text, false, VerCodeType.MSG_RESET_PWD, null, null);\r\n\t}\r\n\tprivate updateCodeTime() {\r\n\t\tthis.codeTime--;\r\n\t\tthis.timeTxt.text = this.codeTime.toString();\r\n\t\tif (this.codeTime <= 0) {\r\n\t\t\tthis.clearCodeTime();\r\n\t\t\tthis.codeBtn.visible = true;\r\n\t\t}\r\n\t}\r\n\tprivate clearCodeTime() {\r\n\t\tLaya.timer.clear(this, this.updateCodeTime);\r\n\t}\r\n\tprivate findResult(suc: boolean, jobj: any) {\r\n\t\tGameMgr.showCircleLoading(false);\r\n\t\tif (suc) {\r\n\t\t\tToast.showToast(\"密码修改成功\");\r\n\t\t\t//密码修改成功后自动登录\r\n\t\t\tLoginModel.accountLogin({\r\n\t\t\t\tuser: this
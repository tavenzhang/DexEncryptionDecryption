vl = this.tabPanel.vScrollBar.value;\r\n\t\t\tlet tov = vl + this.tabPanel.height / 2;\r\n\t\t\tLaya.Tween.to(this.tabPanel.vScrollBar, { value: tov }, 300, Laya.Ease.cubicOut, Laya.Handler.create(this, () => {\r\n\t\t\t\tthis.moveEnd();\r\n\t\t\t}));\r\n\t\t});\r\n\r\n\t\tthis.tabPanel.vScrollBar.on(Laya.Event.END, this, this.moveEnd);\r\n\t\tthis.tabPanel.on(Laya.Event.CLICK, this, this.tabItemHandler);\r\n\t}\r\n\t//点击充值方式列表\r\n\tprivate tabItemHandler(e: Laya.Event) {\r\n\t\tif (e.target instanceof PayTabItem) {\r\n\t\t\tSoundPlayer.enterPanelSound();\r\n\t\t\tlet item = e.target;\r\n\t\t\tthis.itemClick(item);\r\n\t\t}\r\n\t}\r\n\r\n\tprivate itemClick(item: PayTabItem) {\r\n\t\tif (this.prevTab == item) return;\r\n\t\titem.doActive();\r\n\t\tif (this.prevTab && this.prevTab != item) {\r\n\t\t\tthis.prevTab.deactive();\r\n\t\t}\r\n\t\tthis.prevTab = item;\r\n\t\t//\r\n\t\tlet vo = item.vo;\r\n\t\tPayModel.payType = PayType[vo.code];\r\n\t\tif (vo.code == PayType[PayType.BANK]) {//银行转账\r\n\t\t\tthis.showPayInfo(PayModel.bankSubList);\r\n\t\t} else {\r\n\t\t\tPayModel.getPayment(vo.code, this, this.showPayInfo);\r\n\t\t}\r\n\r\n\t\t//显示备注信息\r\n\t\tif (vo.promotionTips) {\r\n\t\t\tlet str = vo.promotionTips;\r\n\t\t\tif (str.length > 30) str = str.substr(0, 30) + \"...\";\r\n\t\t\tthis.infoGroup.visible = true;\r\n\t\t\tthis.infoTxt.text = str;\r\n\t\t\tthis.infoGroup.width = this.infoTxt.x + this.infoTxt.textWidth + 98;\r\n\t\t} else {\r\n\t\t\tthis.infoGroup.visible = false;\r\n\t\t}\r\n\t}\r\n\t//显示充值详细信息\r\n\tprivate showPayInfo(jobj: any[]) {\r\n\t\tif (!jobj) jobj = [];//容错处理\r\n\r\n\t\tlet type = PayModel.checkType(this.prevTab.vo.code);\r\n\t\tif (this.prevViewType != type) {\r\n\t\t\tPayManager.inst.clearLastView();\r\n\t\t}\r\n\t\tPayManager.inst.clearOther();//清理辅助界面\r\n\t\t//创建视图\r\n\t\tlet view = this.viewMap[type];\r\n\t\tif (view && jobj.length > 0) {\r\n\t\t\tDebug.log(\"typeData:\", jobj);\r\n\t\t\tview.visible = true;\r\n\t\t\tview.readData(jobj, this.prevTab.vo.promotionTips);\r\n\t\t\tif (!view.parent) this.viewBox.addChildAt(view, 0);\r\n\t\t\tPayManager.inst.lastView = view;\r\n\t\t} else {\r\n\t\t\tPayManager.inst.hideLastView();\r\n\t\t}\r\n\t\tthis.prevViewType = type;\r\n\t\tthis.errIcon.visible = Boolean(jobj.length == 0);\r\n\t}\r\n\t//滚动结束\r\n\tprivate moveEnd() {\r\n\t\tif (this.destroyed) return;\r\n\t\tlet max = this.tabPanel.contentHeight - this.tabPanel.height;\r\n\t\tlet bl = Boolean(this.tabPanel.vScrollBar.value >= max);\r\n\t\tthis.arrowBtn.visible = !bl;\r\n\t}\r\n\tonClosed(type?: string) {\r\n\t\tLaya.Tween.clearTween(this.tabPanel.vScrollBar);\r\n\t\tPayManager.inst.reset();\r\n\t\tPayModel.clear();\r\n\t\tLobbyModel.refreshMoney();\r\n\t\tthis.tabPanel.destroyChildren();\r\n\t\tEventManager.removeAllEvents(this);\r\n\t\tsuper.onClosed(type);\r\n\t\tthis.destroy(true);\r\n\t\tLobbyModel.getActivityVisible();\r\n\t}\r\n}\r\n","import { ConfObjRead } from \"../common/ConfObjRead\";\r\nimport { Tools } from \"../common/Tools\";\r\nimport { GameMgr } from \"../utils/GameMgr\";\r\nimport { GameUtils } from \"../utils/GameUtils\";\r\nimport { Toast } from \"../view/toast/Toast\";\r\nimport { HttpRequester } from \"./HttpRequester\";\r\n\r\nexport interface DrawDetailsVo {\r\n    state: string;\r\n    stateChineseDisplay: string;//状态\r\n    subTypeChineseDisplay: string;//支付方式\r\n    transactionId: string;//订单号\r\n    createTime: string;//创建时间\r\n    amount: number;//提现金额\r\n    effectiveAmount: number;//有效金额\r\n    remarks?: string;//描述\r\n}\r\n\r\nexport enum DrawState {\r\n    REFUSE,//拒绝\r\n    FAILED,//失败\r\n    COMPLETED,//成功\r\n}\r\n/*\r\n* 提现数据\r\n*/\r\nexport class WithDrawModel {\r\n    public static betInfo: any;//投注相关信息\r\n    public static totalMoney: number;\r\n    /**\r\n     * 获取提现历史记录\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static getHistory(caller?: any, callback?: Function) {\r\n        GameMgr.showCircleLoading(true);\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.payHistory, this, (suc: boolean, jobj: any) => {\r\n            GameMgr.showCircleLoading(false);\r\n            if (suc) {\r\n                if (caller && callback) callback.call(caller, jobj);\r\n            }\r\n        }, \"&pageSize=20&start=0&state=&type=WITHDRAWAL\");\r\n    }\r\n    /**\r\n     * 获取可提现余额\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static getBalance(caller: any, callback: Function) {\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.withdrawBalance, this, (suc: boolean, jobj: any) => {\r\n            if (suc) {\r\n                this.totalMoney = jobj.balance;\r\n                if (caller && callback) callback.call(caller, this.totalMoney);\r\n            }\r\n        });\r\n    }\r\n    /**\r\n     * 获取投注信息\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static getBetInfo(caller?: any, callback?: Function) {\r\n        if (caller && callback) GameMgr.showCircleLoading();\r\n        HttpRequester.getHttpData(ConfObjRead.httpCmd.betInfo, this, (suc: boolean, jobj: any) => {\r\n            if (caller && callback) GameMgr.showCircleLoading(false);\r\n            if (suc) {\r\n                this.betInfo = jobj;\r\n                if (caller && callback) callback.call(caller);\r\n            } else {\r\n                Toast.showToast(\"信息获取失败,请稍后再试\");\r\n            }\r\n        });\r\n    }\r\n    /**\r\n     * 请求提现\r\n     * @param caller \r\n     * @param callback \r\n     */\r\n    public static reqWithdraw(money: number, pwd: string, bankId: number, caller?: any, callback?: Function) {\r\n        let data = {\r\n            amount: money,\r\n            charge: this.getExempt(money),\r\n            chargeV1: true,\r\n            userBankId: bankId,\r\n            version: \"V2\",\r\n            withDrawCode: GameUtils.encryptPwd(pwd)\r\n        };\r\n        GameMgr.showCircleLoading();\r\n        HttpRequester.postHttpData(ConfObjRead.httpCmd.withdraw, data, this, (suc: boolean, jobj: any) => {\r\n            GameMgr.showCircleLoading(false);\r\n            if (caller && callback) callback.call(caller, suc);\r\n        });\r\n    }\r\n    /**\r\n     * 1.提款验证\r\n     * @param value \r\n     */\r\n    public static validateWithDraw(value: string): boolean {\r\n        if (!value) {\r\n            Toast.showToast(\"请输入取款金额\");\r\n            return false;\r\n        }\r\n        let dailyInfo = this.betInfo.dailyWithdrawWithAdminSettingsResult;\r\n        let setting = dailyInfo.withdrawalSettings;\r\n        if (setting.integerWithdrawalAmount) {//必须输入整数\r\n            let regExp = new RegExp(\"^[1-9]\\\\d*$\");\r\n            if (!regExp.test(value)) {\r\n                Toast.showToast(\"取款金额只能为整数\");\r\n                return false;\r\n            }\r\n        }\r\n        if (dailyInfo.surplusWithdrawCount <= 0) {\r\n            Toast.showToast(\"您今日出款次数已经用完，请明日再来!\");\r\n            return false;\r\n        }\r\n        if (dailyInfo.surplusMaxWithdraw <= 0) {\r\n            Toast.showToast('您今日取款额度已经用完，请明日再来!');\r\n            return false;\r\n        }\r\n\r\n        let money = parseFloat(value);\r\n        if (money < setting.minimumWithdrawAmount) {\r\n            Toast.showToast('单次取款最少' + setting.minimumWithdrawAmount + '元!');\r\n            return false;\r\n        }\r\n        if (setting.maximumWithdrawAmount !== 0 && money > setting.maximumWithdrawAmount) {\r\n            Toast.showToast('单次取款至多' + setting.maximumWithdrawAmount + '元!');\r\n            return false;\r\n        }\r\n        if (setting.withdrawSwitch && !dailyInfo.sufficeAggregateBetRequirements) {\r\n            Toast.showToast('打码量不足,不能提现!');\r\n            return false;\r\n        }\r\n        let exempt = this.getExempt(money);\r\n        let offset = money - exempt;\r\n        if (offset > this.totalMoney && exempt > 0) {\r\n            Toast.showToast('您的余额不足,请保留手续费 ' + exempt + ' 元 谢谢');\r\n            return false;\r\n        }\r\n        if (offset > this.totalMoney) {\r\n            Toast.showToast('您的余额不足,请重新输入!');\r\n            return false;\r\n        }\r\n        return true;\r\n    }\r\n    /**\r\n     * 2.获取提款提示消息\r\n     */\r\n    public static getTextWithdraw(): string {\r\n        let dailyInfo = this.betInfo.dailyWithdrawWithAdminSettingsResult;\r\n        let setting = dailyInfo.withdrawalSettings;\r\n        let num = dailyInfo.aggregateBetRequirements - dailyInfo.aggregateBets;\r\n        let newratioOfChargeExempt: number;\r\n        if (dailyInfo.sufficeAggregateBetRequirements) newratioOfChargeExempt = setting.ratioOfChargeExempt;\r\n        else newratioOfChargeExempt = setting.chargeRatioBeyondLimit;\r\n\r\n        if (dailyInfo.surplusFeeWithdrawCount > 0 || newratioOfChargeExempt === 0) {\r\n            return \"本次提现免手续费，是否确定提现？\";\r\n        }\r\n        if (!dailyInfo.sufficeAggregateBetRequirements && setting.ratioOfChargeExempt === 0 && setting.numOfChargeExempt === 0) {//打码量不满足时\r\n\r\n            return '您的有效投注量不足，本次提现手续费为' + newratioOfChargeExempt + '%，是否确定提现？'\r\n        }\r\n        return '本次提现手续费为' + newratioOfChargeExempt + '%，是否确定提现?';\r\n    }\r\n    /**\r\n     * 计算手续费\r\n     */\r\n    public static getExempt(money: number): number {\r\n        let dailyInfo = this.betInfo.dailyWithdrawWithAdminSettingsResult;\r\n        let setting = dailyInfo.withdrawalSettings;\r\n        if (dailyInfo.surplusFeeWithdrawCount > 0 || money === 0 || dailyInfo.surplusMaxWithdraw === 0) {\r\n            return 0;\r\n        }\r\n\r\n        let newratioOfChargeExempt: number;\r\n        if (dailyInfo.sufficeAggregateBetRequirements) newratioOfChargeExempt = setting.ratioOfChargeExempt;\r\n        else newratioOfChargeExempt = setting.chargeRatioBeyondLimit;\r\n\r\n        let regExp = new RegExp(\"^([1-9]\\\\d{0,9}|0)([.]?|(\\\\.\\\\d{1,2})?)$\");\r\n        if (regExp.test(money + \"\")) {\r\n            let tempExempt = money * newratioOfChargeExempt * 0.01;\r\n            let exempts = tempExempt >= setting.maxWithdrawCharge ? setting.maxWithdrawCharge : tempExempt;\r\n            let numStr = Tools.FormatMoney(exempts, 2);\r\n            return parseFloat(numStr);\r\n        }\r\n        return 0\r\n    }\r\n    /**\r\n     * 获取最大提现额度\r\n     */\r\n    public static get maxMoney(): number {\r\n        if (!this.betInfo) return 0;\r\n        let dailyInfo = this.betInfo.dailyWithdrawWithAdminSettingsResult;\r\n        let setting = dailyInfo.withdrawalSettings;\r\n        return setting.maximumWithdrawAmount;\r\n    }\r\n    public static clear() {\r\n        this.betInfo = null;\r\n        this.totalMoney = 0;\r\n    }\r\n}","/**Created by the LayaAirIDE*/\r\n\r\nimport { ui } from \"../../../ui/layaMaxUI\";\r\nimport { DrawDetailsVo } from \"../../../data/WithDrawModel\";\r\nimport { EventManager, EventType } from \"../../../events/EventManager\";\r\nimport { SoundPlayer } from \"../../../utils/SoundPlayer\";\r\nimport { PostMHelp } from \"../../../common/PostMHelp\";\r\n\r\nexport class WithdrawDetailsItem extends ui.sceneUI.dlg.withdraw.WithdrawDetailsItemUI {\r\n\t//处理中，已完成，失败\r\n\tprivate stateColor = [\"#fff600\", \"#0dfd2f\", \"#ff1b1b\"];\r\n\tprivate addev: boolean;\r\n\r\n\tconstructor() {\r\n\t\tsuper();\r\n\r\n\t}\r\n\tpublic readData(vo: DrawDetailsVo) {\r\n\t\tthis.stateTxt.text = vo.stateChineseDisplay;\r\n\t\tlet color;\r\n\t\tswitch (vo.state) {\r\n\t\t\tcase \"COMPLETED\": color = this.stateColor[1]; break;\r\n\t\t\tcase \"FAILED\": color = this.stateColor[2]; break;\r\n\t\t\tdefault: color = this.stateColor[0];\r\n\t\t}\r\n\t\tthis.stateTxt.color = color;\r\n\t\tthis.payMethodTxt.text = vo.subTypeChineseDisplay;\r\n\t\tthis.orderTxt.text = vo.transactionId;\r\n\t\tthis.timeTxt.text = vo.createTime;\r\n\t\tthis.getMoneyTxt.text = vo.amount + \"元\";\r\n\t\tthis.totalTxt.text = vo.effectiveAmount + \"元\";\r\n\t\tthis.feeTxt.text = (vo.effectiveAmount - vo.amount) + \"元\";\r\n\t\tle
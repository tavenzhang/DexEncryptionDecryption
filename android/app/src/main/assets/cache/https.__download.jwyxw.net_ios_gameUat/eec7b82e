ibox.addChild(lab);\r\n\t\t}\r\n\t\tif (this.curri > nowday)\r\n\t\t\tthis.curri = nowday;\r\n\t\tLaya.timer.frameOnce(2, this, this.setScrollBarri, [this.curri]);\r\n\t}\r\n\r\n\t//显示最早的那天\r\n\tprivate showminiTime(): void {\r\n\t\tthis.showtimeri(this.curyue, this.miniYue);\r\n\t\tthis.curnian = this.miniNian;\r\n\t\tthis.curyue = this.miniYue;\r\n\t\tthis.curri = this.miniRi;\r\n\t\tif (this.maxNianNum == 1)\r\n\t\t\tthis.nianpanel.vScrollBar.value = 64;\r\n\t\telse\r\n\t\t\tthis.nianpanel.vScrollBar.value = 0;\r\n\r\n\t\tLaya.timer.frameOnce(2, this, this.setScrollBaryue, [this.curyue]);\r\n\t\tLaya.timer.frameOnce(2, this, this.setScrollBarri, [this.curri]);\r\n\t\tthis.showtextColor();\r\n\t}\r\n\r\n\t//显示今天\r\n\tprivate showMaxTime(): void {\r\n\t\tlet nian: number = this.nowDate.getUTCFullYear();\r\n\t\tlet yue: number = this.nowDate.getUTCMonth() + 1;\r\n\t\tlet ri: number = this.nowDate.getUTCDate();\r\n\t\tthis.showtimeri(this.curyue, yue);\r\n\t\tthis.curnian = nian;\r\n\t\tthis.curyue = yue;\r\n\t\tthis.curri = ri;\r\n\t\tLaya.timer.frameOnce(2, this, this.setScrollBar, [yue, ri]);\r\n\t\tthis.showtextColor();\r\n\t}\r\n\tprivate showtextColor(): void {\r\n\t\tlet text: Laya.Label;\r\n\t\tfor (let i = 0; i < this.nianbox.numChildren; i++) {\r\n\t\t\ttext = this.nianbox.getChildAt(i) as Laya.Label;\r\n\t\t\tif (text && text.text.length > 0) {\r\n\t\t\t\tif (text.text.split(\" \")[0] == String(this.curnian))\r\n\t\t\t\t\ttext.color = \"#ffffff\";\r\n\t\t\t\telse\r\n\t\t\t\t\ttext.color = \"#8eb5ee\";\r\n\t\t\t}\r\n\t\t}\r\n\t\tfor (let i = 0; i < this.yuebox.numChildren; i++) {\r\n\t\t\ttext = this.yuebox.getChildAt(i) as Laya.Label;\r\n\t\t\tif (text && text.text.length > 0) {\r\n\t\t\t\tif (text.text.split(\" \")[0] == String(this.curyue))\r\n\t\t\t\t\ttext.color = \"#ffffff\";\r\n\t\t\t\telse\r\n\t\t\t\t\ttext.color = \"#8eb5ee\";\r\n\t\t\t}\r\n\t\t}\r\n\t\tfor (let i = 0; i < this.ribox.numChildren; i++) {\r\n\t\t\ttext = this.ribox.getChildAt(i) as L
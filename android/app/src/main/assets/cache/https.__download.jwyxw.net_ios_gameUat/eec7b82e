 {\r\n        GameMgr.showCircleLoading();\r\n        let cmd = ConfObjRead.getConfUrl().cmd.agentachievement;\r\n\r\n        let urlParams = \"&username=\" + Common.userInfo.username;\r\n        urlParams += \"&start=\" + data.start;\r\n        urlParams += \"&pageSize=\" + data.size;\r\n        urlParams += \"&dateType=\" + data.type;\r\n\r\n        HttpRequester.getHttpData(cmd, this, (suc: boolean, jobj: any) => {\r\n            GameMgr.showCircleLoading(false);\r\n            if (suc) {\r\n                if (caller && callback) {\r\n                    callback.call(caller, jobj);\r\n                }\r\n            }\r\n        }, urlParams);\r\n    }\r\n    //检测并创建邀请码(用于活动分享)\r\n    public static checkAndCreatAffcode() {\r\n        let vo = this.invationVo;\r\n        if (!vo) {\r\n            this.reqCode();\r\n        } else {\r\n            this.readCode();\r\n        }\r\n    }\r\n    private static readCode() {\r\n        if (this.invationVo.length == 0) {\r\n            AgentModel.creatAgentInvitCode(this, this.reqCode);\r\n        }\r\n    }\r\n    private static reqCode() {\r\n        AgentModel.searchAgentInvatCode(this, this.readCode, false);\r\n    }\r\n    /**\r\n     * 退出登录时需要清理缓存\r\n     */\r\n    public static clear() {\r\n        this.agentInfo = null;\r\n        this.invationVo = null;\r\n        this.shareBase64 = null;\r\n    }\r\n}\r\n//---------------------代理中心相关数据模型-----------------------------\r\n/**\r\n * 直属数据\r\n */\r\nexport interface DirectlyVo {\r\n    \"parentName\": string;//上级推荐人id\r\n    \"role\": string;//用户类型[平台, 总代, 代理, 玩家]\r\n    \"subBet\": number;//直属业绩\r\n    \"subMembers\": number,//直属人数\r\n    \"teamBet\": number,//团队业绩\r\n    \"teamMembers\": number,//团队人数\r\n    \"userId\": number,//用户id\r\n    \"username\": string;//推荐人id\r\n}\r\n/**\r\n * 我的业绩\r\n */\r\nexport interface AchievementVo {\r\n    bet?: number;\r\n    brokerage: number;\r\n    createBrokerage?: number;\r\n    directCount: number;\r\n    subBet: number;\r\n    teamBet: number;\r\n    teamCount: number;\r\n    userId?: number;\r\n    username: string;\r\n}\r\n/**\r\n * 返佣比例\r\n */\r\nexport interface CRatioVo {\r\n    level: number;\r\n    levelName: string;\r\n    lower: string;\r\n    rate: string;\r\n    upper: string;\r\n}\r\n/**\r\n * 提佣记录\r\n */\r\nexport interface CRecordVo {\r\n    brokerage: number;//佣金\r\n    createTime: string;//创建时间\r\n    id: string;//订单号\r\n    state: string\r\n    updateTime: string;//更新时间\r\n}","import { AgentModel } from '../data/AgentModel';\r\nimport { LobbyModel } from '../data/LobbyModel';\r\nimport { PersonalModel } from '../data/PersonalModel';\r\nimport { ResConfig } from '../data/ResConfig';\r\nimport { WithDrawModel } from '../data/WithDrawModel';\r\nimport { CardInfoVo, GameData, GameIconVo, GatewayVo, IBackCardVo } from './../data/GameVos';\r\nimport { MyUid } from './MyUid';\r\nexport class Common {\r\n\r\n    public static GM_SCREEN_H: number = 750;\r\n    public static GM_SCREEN_W: number = 1334;\r\n\r\n    public static loadConf: any;\r\n    public static confObj: any;\r\n\r\n    public static weChatData: any; //微信数据\r\n    public static userInfo: any;\r\n    public static userBalance: number = 0;//玩家实际余额\r\n    public static balanceInfo: any;//余额信息\r\n    public static userInfo_current: any;\r\n    public static loginInfo: any;\r\n    public static avatorInfo: any;\r\n    //银行卡绑定情况简单信息\r\n    public static cardInfo: CardInfoVo = { enabledAlipayWithdraw: false, hasAlipayCard: false, hasBankCard: false };\r\n    public static bankInfo: IBackCardVo;//银行卡绑定信息\r\n    public static alipayInfo: IBackCardVo;//支付宝绑定信息\r\n    public static isUpate: boolean;//是否有热更新\r\n\r\n    //----网关验证相关参数\r\n    public static gatewayInfo: GatewayVo;\r\n\r\n    public static emptyLoginInfo(): any {\r\n        return {\r\n            \"strongPwd\": true\r\n        };\r\n    }\r\n    public static isSit: boolean = true;//是否为sit环境\r\n    public static isWeb: boolean = false;//是否为web环境(发布web版使用)
I{constructor(){super(),this.initView()}initView(){this.checkVisitorMark(),this.flushMoney(),this.showNickName(),EventManager.addTouchScaleListener(this.headIcon,this,()=>{SoundPlayer.enterPanelSound(),GameMgr.showCircleLoading(!0,!0),PersonalModel.vipData?this.openPerson():PersonalModel.getVipData(this,this.openPerson)},null,1),EventManager.addTouchScaleListener(this.flushBtn,this,()=>{SoundPlayer.clickSound(),this.flushMoney()}),EventManager.register(EventType.SHOW_NICK_NAME,this,this.showNickName),EventManager.register(EventType.FLUSH_MONEY,this,this.showMoney),EventManager.register(EventType.GETAVATOR_INFO,this,this.showHeadIcon),EventManager.register(EventType.FLUSH_HEADICON,this,this.flushHeadIcon),EventManager.register(EventType.WITHDRAW_SUCC,this,this.flushMoney)}openPerson(){GameMgr.showCircleLoading(!1),PageManager.showDlg(DlgCmd.personCenter)}stopAnim(){this.destroyed||this.rotAnim.gotoAndStop(0)}checkVisitorMark(){this.visitorMark.visible=LoginModel.isVisitor}flushMoney(){this.rotAnim.play(),Laya.timer.clear(this,this.stopAnim),LobbyModel.refreshMoney(this,this.stopAnim),Laya.timer.once(6e3,this,this.stopAnim)}showHeadIcon(){let e=Common.avatorInfo.avatorId;this.flushHeadIcon(e)}flushHeadIcon(e){this.headIcon.skin=ResConfig.getHeadSkinByID(e)}showNickName(){let e=Common.userInfo_current;e&&(this.nameTxt.text=e.nickname||"")}showMoney(){if(this.bitFont||(this.bitFont=new BitmapFont(ResConfig.bitFont_norm),this.goldBox.addChild(this.bitFont),this.bitFont.scale(.8,.8),this.bitFont.text="0",this.bitFont.y=this.goldBox.height-.8*this.bitFont.height>>1),Common.userBalance>=0){let e=Common.userBalance;this.bitFont.text=Tools.FormatMoney(e,2),this.bitFont.x=this.goldBox.width-.8*this.bitFont.width>>1,this.bitFont.x-=6}}destroy(){EventManager.removeAllEvents(this),super.destroy(!0)}}class GameConfig{constructor(){}static init(){var e=Laya.ClassUtils.regClass;e("view/comp/ClassifyMenu.ts",ClassifyMenu),e("view/comp/ChessMenu.ts",ChessMenu),e("view/notice/message/spinner/Roullette.ts",Roullette),e("view/dlg/personaCenter/VipGiftsPanel.ts",VipGiftsPanel),e("view/dlg/personaCenter/VipLevelIntroduce.ts",VipLevelIntroduce),e("view/dlg/withdraw/WithDrawTab.ts",WithDrawTab),e("view/anim/LightAnim.ts",LightAnim),e("view/comp/GameIconListPanel.ts",GameIconListPanel),e("view/comp/CycleComp.ts",CycleComp),e("view/comp/NoticeComp.ts",NoticeComp),e("view/comp/UserInfoView.ts",UserInfoView)}}GameConfig.width=1334,GameConfig.height=750,GameConfig.scaleMode="fixedheight",GameConfig.screenMode="horizontal",GameConfig.alignV="top",GameConfig.alignH="left",GameConfig.startScene="sceneUI/LoginScene.scene",GameConfig.sceneRoot="",GameConfig.debug=!1,GameConfig.stat=!1,GameConfig.physicsDebug=!1,GameConfig.exportSceneToJson=!0,GameConfig.init();class Main{constructor(){isNative=Laya.Render.isConchApp,window.Laya3D?Laya3D.init(GameConfig.width,GameConfig.height):Laya.init(GameConfig.width,GameConfig.height,Laya.WebGL),Laya.stage.scaleMode=GameConfig.scaleMode,Laya.stage.screenMode=GameConfig.screenMode,Laya.stage.alignV=GameConfig.alignV,Laya.stage.alignH=GameConfig.alignH,Laya.stage.bgColor="#000000",Laya.URL.exportSceneToJson=GameConfig.exportSceneToJson,Printer.init(isNative),GameMgr.init(),GameUpdateMgr.init(),UIConfig.closeDialogOnSide=!1,Laya.Dialog.manager.closeEffectHandler=new Laya.Handler(null,PageManager.closeDlg),Laya.Dialog.manager.popupEffectHandler=new Laya.Handler(null,PageManager.openDlg),GameConfig.stat&&Laya.Stat.show(),Laya.alertGlobalError=!0,Laya.loader.load("assets/args.json",Laya.Handler.create(this,this.onVersionLoaded))}onVersionLoaded(){isNative&&(AppData=Laya.Loader.getRes("assets/args.json")),Laya.AtlasInfoManager.enable("fileconfig.json",Laya.Handler.create(this,this.onConfigLoaded))}onConfigLoaded(){let e=["res/atlas/ui/login.atlas","ui/login/loginAnim.png","ui/login/loginAnim.sk","./assets/conf/assets_lobby.json","./assets/conf/gameIcons.json","./assets/conf/version.json"];if(isNative){let t=AppData.apihome;t=t.spli
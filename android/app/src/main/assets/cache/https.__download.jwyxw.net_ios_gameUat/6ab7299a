List())}}addMailList(){let e;this.resetList();let t=this.itemList.width;this.mlistData.forEach((i,a)=>{(e=new MailItemView).readData(i),e.x=t-e.width>>1,e.y=a*(e.height+8),this.itemList.addChild(e),this.itemArr.push(e),0==a&&this.selectItem(e),e.award&&this.awardCount++}),this.updateGetAndDelBtn(),this.itemList.on(Laya.Event.CLICK,this,this.itemHandler)}updateGetAndDelBtn(){this.getAllBtn.visible=Boolean(this.awardCount>0),this.delAllBtn.visible=Boolean(this.awardCount<=0)}resetList(){this.awardCount=0,this.itemList.numChildren>0&&this.itemList.removeChildren(),this.itemArr.length=0,this.curItem=null,this.curDetailsVo=null,this.detailsGroup.visible=!1,this.getAllBtn.visible=!1,this.delAllBtn.visible=!1}itemHandler(e){if(e.target instanceof MailItemView){SoundPlayer.enterPanelSound();let t=e.target;this.selectItem(t)}}selectItem(e){e.selected||(this.reqDetail(e.mailId),e.selected=!0,this.curItem&&(this.curItem.selected=!1),this.curItem=e)}reqDetail(e){let t=ConfObjRead.getConfUrl().cmd.getMailDetail+e;GameMgr.showCircleLoading(!0),HttpRequester.getHttpData(t,this,(e,t)=>{GameMgr.showCircleLoading(!1),e&&this.showDetails(t)})}showDetails(e){this.curItem&&this.curItem.markRead(),this.curDetailsVo=e,this.ctitleTxt.text=e.title,this.contentTxt.text=e.content||"";let t,i=MailState[e.state];if(e.amount&&e.amount>0&&(i!=MailState.UNCLAIMED&&i!=MailState.READUNCLAIMED||(t=!0)),this.awardGroup.visible=t,this.getCurBtn.visible=t,this.delCurBtn.visible=!t,t){let t=.42;this.bitFont||(this.bitFont=new BitmapFont(ResConfig.bitFont_mail),this.bitFont.scale(t,t),this.bitFont.text="0",this.goldBox.addChild(this.bitFont),this.bitFont.y=this.goldBox.height-this.bitFont.height*t>>1),this.bitFont.text="x"+e.amount,this.bitFont.x=this.goldBox.width-this.bitFont.width*t>>1,this.ctxtList.height=210}else this.ctxtList.height=376;this.detailsGroup.visible=!0}onClosed(e){this.itemList.off(Laya.Event.CLICK,this,this.itemHandler),EventManager.removeAllEvents(this),this.bitFont&&this.bitFont.dest
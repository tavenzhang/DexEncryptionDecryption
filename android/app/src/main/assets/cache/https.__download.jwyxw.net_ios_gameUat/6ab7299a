eb(i.data);else if(!(i.webview||i.paymentJumpTypeEnum&&PayMentJumpType[i.paymentJumpTypeEnum]!=PayMentJumpType.IMG)){this.hideLastView();let t=new SaveQrInfoView,a={amount:i.amount,orderNo:i.transactionId,qrUrl:i.data,creatQR:!i.paymentJumpTypeEnum};t.show(e,a),this.viewbox.addChild(t)}})}reset(){this.clearLastView(),this.clearOther(),this.viewbox&&(this.viewbox=null)}}class PayMoneyBtn extends ui.sceneUI.dlg.pay.PayMoneyBtnUI{constructor(){super(),this._value=0,this.selectBg.alpha=0}set value(e){this._value=e,this.txt.text=e+"元",this.selectBg.alpha=0,this.selected=!1}get value(){return this._value}setSelect(e){Laya.Tween.to(this.selectBg,{alpha:e?1:0},300),this.selected=e}}class FixedMoneyView extends ui.sceneUI.dlg.pay.FixedMoneyViewUI{constructor(){super(),this.itemArr=[],this.gap=0,this.btnArr=[],this.maxNum=12,this.centerX=0,this.centerY=0,this.initView()}initView(){ComponentTool.setPanelSlide(this.itemPanel,!1),this.itemPanel.on(Laya.Event.CLICK,this,this.clickItem),EventManager.addTouchScaleListener(this.submitBtn,this,()=>{SoundPlayer.clickSound(),this.prevBtn?PayManager.inst.startPay(this.prev.vo,this.prevBtn.value):Toast.showToast("请选择充值金额")})}clickItem(e){if(e.target instanceof FixedMoneyItem){SoundPlayer.enterPanelSound();let t=e.target;this.selectItem(t)}}selectItem(e){e.selected||(e.selected=!0,this.prev&&(this.prev.selected=!1),this.prev=e,this.creatMoneyBtn(e.vo.fixedAmount),this.infoTxt.text=e.vo.remarks||"---")}creatMoneyBtn(e){if(this.recoverBtn(),!e||0==e.length)return void Debug.error("没有配置常用金额数据");let t;e.forEach((e,i)=>{i<this.maxNum&&((t=Laya.Pool.getItem(PayModel.btnMark))||(t=new PayMoneyBtn),t.value=e,t.mouseEnabled=!0,t.x=i%4*(t.width+11),t.y=Math.floor(i/4)*(t.height+8),this.btnBox.addChild(t),this.btnArr.push(t),EventManager.addTouchScaleListener(t,this,this.btnClick,t,.6,!0))})}recoverBtn(){this.prevBtn&&(this.prevBtn=null),this.btnArr.forEach(e=>{e.removeSelf(),Laya.Pool.recover(PayModel.btnMark,e)}),this.btnArr.length=0}btnClick(e,t)
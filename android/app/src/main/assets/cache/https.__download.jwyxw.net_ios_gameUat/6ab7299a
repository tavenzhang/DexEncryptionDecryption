Sound();let e=parseFloat(this.inputTxt.text);isNaN(e)||e<=0?Toast.showToast("请输入大于0的数字"):e>this.yuebaoInfo.balanceAmt?Toast.showToast("输入金额大于个人账户余额，无法存入"):this.deposit(e)}),EventManager.addTouchScaleListener(this.clearBtn,this,()=>{SoundPlayer.clickSound(),this.inputTxt.text=""}),EventManager.register(EventType.FLUSH_YUEBAOINFO,this,this.reqInfo),EventManager.register(EventType.BALANCE_PADSETING,this,this.flushPwdState),KeyboardView.bindKeyboard(this.inputTxt,{})}flushPwdState(){this.yuebaoInfo&&(this.yuebaoInfo.isPasswordAvailable=!0)}reqInfo(){GameMgr.showCircleLoading(!0),HttpRequester.getHttpData(ConfObjRead.httpCmd.yuebaoInfo,this,this.infoCallback)}deposit(e){GameMgr.showCircleLoading(!0),HttpRequester.postHttpData(ConfObjRead.httpCmd.yuebaoDeposit,{amount:e},this,(t,i)=>{GameMgr.showCircleLoading(!1),t&&(TipsDlg.show("成功存入:"+e+"元"),this.inputTxt.text="",this.reqInfo())})}infoCallback(e,t){if(GameMgr.showCircleLoading(!1),e){this.yuebaoInfo=t,this.showBitFont(this.bitBox1,FormatTool.dollarFormat(t.amount,"")),this.showBitFont(this.bitBox2,FormatTool.dollarFormat(t.preIncome,"")),this.showBitFont(this.bitBox3,FormatTool.dollarFormat(t.balanceAmt,"")),this.showBitFont(this.bitBox4,FormatTool.dollarFormat(t.sumIncome,""),.8),this.showBitFont(this.bitBox5,FormatTool.dollarFormat(t.curIncome,""),.8),this.showBitFont(this.bitBox6,FormatTool.dollarFormat(t.myriadProfit,""),.8);let e=t.myriadProfit/1e4*365;e*=100,this.showBitFont(this.bitBox7,FormatTool.dollarFormat(e,""),.8),t.isClose?(this.setBtn.disabled=!0,this.weihuMask.visible=!0):(this.setBtn.disabled=!1,this.weihuMask.visible=!1),Common.userBalance=t.balanceAmt,EventManager.dispath(EventType.FLUSH_MONEY)}else this.yuebaoInfo={balanceAmt:0,amount:0}}showBitFont(e,t,i=1){null==e.$_GID&&(e.$_GID=Laya.Utils.getGID());let a=this.fontMap[e.$_GID];a||(a=new BitmapFont(ResConfig.bitFont_norm),1!=i&&a.scale(i,i),a.text="0",e.addChild(a),a.y=e.height-a.height*i>>1,this.fontArr.push(a),this[e.$_GID]=a),a.text=t,a.x=e.wid
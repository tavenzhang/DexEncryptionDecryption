Index%this.total)}init(e,t){this.itemList=e,this.spaceTime=t,this.total=e.length,this.moveIndex=0,e.forEach(e=>{let t=new Laya.Image;t.width=this.viewWidth,t.height=this.viewHeight,t.skin=e.carouselUrl,this.imgList.push(t)}),this.total>0&&this.itemBox.addChild(this.imgList[0]),this.total>1?this.addListener():1==this.total&&this.on(Laya.Event.CLICK,this,this.singleHandler)}singleHandler(){let e=this.itemList[0];this.openLink(e)}openLink(e){if(e&&e.carouselHref)if(e.jumpInner)if(e.jumpGame)EventManager.dispath(EventType.JUMP_GAME,e.carouselHref);else if(SoundPlayer.enterPanelSound(),-1!=e.carouselHref.indexOf("activityDetails")){let t=e.carouselHref.split("-"),i=t[1],a=t[2];Debug.log("跳转指定活动页:",e.carouselHref),a&&i?PageManager.showDlg(DlgCmd.activityCenter,parseInt(a)+1,parseInt(i)):i?PageManager.showDlg(DlgCmd.activityCenter,DlgCmd.activityCenter,parseInt(i)):Debug.error("配置异常：",e.carouselHref)}else InnerJumpUtil.doJump(DlgCmd[e.carouselHref]);else SoundPlayer.clickSound(),GameUtils.openWeb(e.carouselHref)}flushData(e){e&&0!=e.length&&(this.off(Laya.Event.MOUSE_DOWN,this,this.touchBeginHandler),this.off(Laya.Event.CLICK,this,this.singleHandler),this.stopTimer(),this.imgList.forEach(e=>{e.destroy(!0)}),this.imgList.length=0,this.itemBox.x=0,this.itemBox.removeChildren(),this.init(e,this.spaceTime))}destroy(e=!0){this.off(Laya.Event.MOUSE_DOWN,this,this.touchBeginHandler),this.off(Laya.Event.CLICK,this,this.singleHandler),this.stopTimer(),super.destroy(!0)}addListener(){this.on(Laya.Event.MOUSE_DOWN,this,this.touchBeginHandler),this.addTimer()}addTimer(){this.total>1&&Laya.timer.loop(this.spaceTime,this,this.doTimer)}stopTimer(){Laya.timer.clear(this,this.doTimer)}touchBeginHandler(e){this.downPos=this.itemBox.x,this.downStageX=e.stageX,this.downTime=Laya.Browser.now(),this.stopTimer(),this.on(Laya.Event.MOUSE_MOVE,this,this.touchMoveHandler),this.on(Laya.Event.MOUSE_OUT,this,this.touchEndHandler),this.on(Laya.Event.MOUSE_UP,this,this.touchEndHandler)}touchMoveHandler(